  margin: calc(0.5em);
}
[am-margin][am-margin=&quot;large&quot;] {
  margin: calc(1.5em);
}
[am-margin][am-margin~=&quot;top&quot;] {
  margin: 1em 0 0 0;
}
[am-margin][am-margin~=&quot;top&quot;][am-margin~=&quot;large&quot;] {

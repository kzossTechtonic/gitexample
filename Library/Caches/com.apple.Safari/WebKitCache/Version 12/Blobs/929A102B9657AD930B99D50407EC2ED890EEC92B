  [am-row][am-row~=&quot;portrait-100&quot;][am-row^=&quot;gutter margin&quot;] &gt; div[am-col] {
    margin-left: 0;
    padding-left: 0;
  }
  [am-row][am-row~=&quot;portrait-100&quot;] &gt; div[am-col] {
    -webkit-flex-basis: auto;
        -ms-flex-preferred-size: auto;
            flex-basis: auto;
    width: 100%;
  }
}

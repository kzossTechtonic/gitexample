}
@media (orientation: portrait) {
  [am-layout~=&quot;portrait-100&quot;] div[am-row] div[am-col] {
    margin-left: 0;
    padding-left: 0;
    -webkit-flex-basis: auto;
        -ms-flex-preferred-size: auto;
            flex-basis: auto;

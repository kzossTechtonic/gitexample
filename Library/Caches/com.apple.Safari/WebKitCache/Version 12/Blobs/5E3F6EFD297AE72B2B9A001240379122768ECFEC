(function (window) {
    AbsenceAllowanceBar = function (options) {

        console.info("BAR", options);

        this.textControl = null;

        this.settings = $.extend({
            domSelector: null,
            allowance: {},
            initHidden: false,
            displayText: false,// false, "ALLOWED", "SELECTED"
            displayTooltip: false
        }, options);

        this.container = $('<div class="absenceAllowanceBar"/>');
        this.allowedBar = $('<div class="allowed animatedTransition partPart" data-tooltip="true"  data-tooltip-glued="true"/>')
            .css("width", 0)
            .appendTo(this.container);

        this.futurePredictionBar = $('<div class="allowedFuture animatedTransition partPart" data-tooltip="true" data-tooltip-glued="true"/>')
            .css("width", 0)
            .appendTo(this.container);

        this.pendingBar = $('<div class="pending animatedTransition partPart" data-tooltip="true" data-tooltip-glued="true"/>')
            .css("width", 0)
            .appendTo(this.container);

        this.acceptedBar = $('<div class="accepted animatedTransition partPart" data-tooltip="true" data-tooltip-glued="true"/>')
            .css("width", 0)
            .appendTo(this.container);

        this.selectedBar = $('<div class="selected animatedTransition partPart" />')
            .css("width", 0)
            .appendTo(this.container);

        this.textControl = $('<div class="text"/>')
            .appendTo(this.container);

        this.selectedAmount = 0;

        if (this.settings.initHidden == false)
            redraw.call(this);

        if (this.settings.domSelector != null) {
            this.appendTo(this.settings.domSelector);
        }
    };

    AbsenceAllowanceBar.prototype.appendTo = function (selector) {
        var _t = this;
        $(selector).append(_t.container);
    };

    AbsenceAllowanceBar.prototype.detach = function () {
        var _t = this;
        $(_t.container).detach();
    };

    AbsenceAllowanceBar.prototype.selectAmount = function (amount) {
        this.selectedAmount = amount;

        if (amount == 0) {
            this.selectedBar
                .css("width", "0")
                .removeClass("selectedNotAllowed");
        } else if (this.settings.allowance.unlimitedAllowed) {
            selectAmountOnUnlimited.call(this, amount);
        } else if (this.settings.allowance.allowed < 0) {
            this.selectedBar
                .css("width", "100%")
                .addClass("selectedNotAllowed");
        } else {
            var allowanceSum = countSumOfPendingUsedAllowedPrediction.call(this);
            var percentageAmount = allowanceSum == 0 ? 100 : (amount / allowanceSum) * 100;

            if (percentageAmount > 100) {
                percentageAmount = 100;
            }

            if (amount > this.settings.allowance.allowed)
                this.selectedBar.addClass("selectedNotAllowed");
            else
                this.selectedBar.removeClass("selectedNotAllowed");


            this.selectedBar.css("width", percentageAmount + "%");
        }

        redrawText.call(this);
    }

    AbsenceAllowanceBar.prototype.updateEntitlement = function (entitlement) {
        this.settings.allowance = entitlement;
        redraw.call(this);
    }

    function redraw() {
        if (this.settings.allowance.unlimitedAllowed) {
            redrawUnlimited.call(this);
        } else {
            if ((this.settings.allowance.allowed) < 0) {
                redrawAllowedUnderZero.call(this);
            } else {
                redrawLimitedAllowance.call(this);
            }
        }

        redrawTooltip.call(this);
        redrawText.call(this);
    };

    function redrawAllowedUnderZero() {
        this.container.addClass("underZeroAllowance");
        this.allowedBar.css("left", 0).css("width", "0");
        this.futurePredictionBar.css("left", 0).css("width", "0");
        this.pendingBar.css("left", 0).css("width", "0");
        this.acceptedBar.css("left", 0).css("width", "0");
    }

    function countSumOfPendingUsedAllowedPrediction() {
        return this.settings.allowance.futureAccrue + this.settings.allowance.earned;
    }


    function redrawLimitedAllowance() {
        var sumOfEntitlementMax = countSumOfPendingUsedAllowedPrediction.call(this);
        var usedAndReducedAndCarried = this.settings.allowance.used + this.settings.allowance.reduced + this.settings.allowance.carriedForSure;
        var futurePrediction = sumOfEntitlementMax - (usedAndReducedAndCarried + this.settings.allowance.pending + this.settings.allowance.allowed);

        var percentageAllowed = sumOfEntitlementMax <= 0 ? 0 : (this.settings.allowance.allowed / sumOfEntitlementMax) * 100;
        var percentagePending = sumOfEntitlementMax <= 0 ? 0 : (this.settings.allowance.pending / sumOfEntitlementMax) * 100;
        var percentageFuture = sumOfEntitlementMax <= 0 ? 0 : (futurePrediction / sumOfEntitlementMax) * 100;
        var percentageUsed = sumOfEntitlementMax <= 0 ? 0 : (usedAndReducedAndCarried / sumOfEntitlementMax) * 100;


        this.container.removeClass("underZeroAllowance");

        if (percentageAllowed > 0 || percentagePending > 0 || percentageFuture > 0) {
            this.container.addClass("no-leftborder");
        } else {
            this.container.removeClass("no-leftborder");
        }


        if (percentageUsed == 0 && sumOfEntitlementMax > 0) {
            this.container.addClass("no-rightborder");
        } else {
            this.container.removeClass("no-rightborder");
        }

        this.allowedBar
            .css("width", percentageAllowed + "%")
            .css("left", "0")
            .data("tooltip", "true");

        this.futurePredictionBar
            .css("width", percentageFuture + "%")
            .css("left", percentageAllowed + "%");

        this.pendingBar
            .css("width", percentagePending + "%")
            .css("left", (percentageAllowed + percentageFuture) + "%");

        this.acceptedBar
            .css("width", percentageUsed + "%")
            .css("left", (percentageAllowed + percentageFuture + percentagePending) + "%");


        if (percentageUsed > 99) {
            this.acceptedBar.addClass("percent100");
        } else {
            this.acceptedBar.removeClass("percent100");
        }
    }

    function redrawUnlimited() {
        this.container.addClass("no-rightborder no-leftborder");
        this.allowedBar.css("width", "100%");
        this.pendingBar.css("width", "0");
        this.acceptedBar.css("width", "0");
        this.futurePredictionBar.css("width", "0");
    }


    function redrawTooltip() {

        if (this.settings.displayTooltip == false) {
            //nothing
        } else {
            this.allowedBar.data("tooltip-content", Messages.get("AbsenceAllowanceBar.allowedBar"));
            this.futurePredictionBar.data("tooltip-content", Messages.get("AbsenceAllowanceBar.futurePredictionBar"));
            this.pendingBar.data("tooltip-content", Messages.get("AbsenceAllowanceBar.pendingBar"));
            this.acceptedBar.data("tooltip-content", Messages.get("AbsenceAllowanceBar.acceptedBar"));
        }

    }

    function redrawText() {
        var amount = "";

        if (this.settings.displayText == false) {
            //nothing
        } else if (this.settings.displayText == "ALLOWED") {
            amount = textOfAllowedAmount.call(this);
        } else if (this.settings.displayText == "SELECTED") {
            var amountSelected = AbsenceRequestLength.anyAmountOfEntitlementDisplayable(this.selectedAmount, false, this.settings.allowance);
            var amountAllowed = textOfAllowedAmount.call(this);

            amount = amountSelected + " / " + amountAllowed;
        }

        this.textControl.text(amount);
    }

    function selectAmountOnUnlimited(amount) {

    }

    function textOfAllowedAmount() {
        if (this.settings.allowance.entitlementType == "UNLIMITED") {
            return Messages.get("entitlement.unlimited");
        } else if (this.settings.allowance.entitlementType == "UNDEFINED") {
            return Messages.get("entitlement.undefined");
        } else {
            return AbsenceRequestLength.allowedEntitlementToDisplayable(this.settings.allowance);
        }
    }

    window.AbsenceAllowanceBar = AbsenceAllowanceBar;
})(window);
//module:EntitlementTable_init
var __EntitlementTable__template__tab;
var __EntitlementTable__template__row;
var __EntitlementTable__template__bar;
var __EntitlementTable__template__manyyears;

(function (window) {
    function fillYears(tab, annualsPools) {
        tab.years = new Array();

        for (var i = 0; i < annualsPools.length; i++) {
            tab.years.push(annualsPools[i].calendarYear);
        }

        tab.years.sort();
    }

    EntitlementTable = function () {
        this.years = [];
        this.entitlementManager = new EntitlementManager();
        this.daysReducingManager = new DaysReducingManager();

        this.reducingPool = null;
        this.averageWorkingSeconds = null;

        this.forPersonId = null;
        this.templateTable = __EntitlementTable__template__tab;
        this.templateRow = __EntitlementTable__template__row;
        this.templateBar = __EntitlementTable__template__bar;
        this.templateManyYears = __EntitlementTable__template__manyyears;

        this.annualsAbsencePool = {};
        this.contentContainer = $("<div />");

        this.onYearChangeHandler = function (y) {
        };
    };

    EntitlementTable.prototype.loadData = function () {
        var _this = this;

        return $.when(
            _this.getDaysReducingPool()
                .done(function (response) {
                    _this.reducingPool = new DaysReducingLeavePool(response);
                    _this.averageWorkingSeconds = _this.reducingPool.getAverageWorkingSeconds();
                }),

            _this.getDataFromRemote().done(function (response) {
                fillYears(_this, response.annualsAbsencePools);
                _this.annualsAbsencePool = response.annualsAbsencePools;
            })
        );
    };

    EntitlementTable.prototype.getDataFromRemote = function () {
        if (this.forPersonId == null)
            return this.entitlementManager.getCurrentEmployeeEntitlement(moment());
        else
            return this.entitlementManager.getEmployeeEntitlement(this.forPersonId, moment());
    }

    EntitlementTable.prototype.getDaysReducingPool = function () {
        if (this.forPersonId == null)
            return this.daysReducingManager.getForCurrentUser();
        else
            return this.daysReducingManager.getForUser(this.forPersonId);
    }


    EntitlementTable.prototype.displayYear = function (y) {
        var year = y;

        if (y == null || this.years.indexOf(y) === -1)
            year = this.years[0];

        var yearTableContent = this.getYearTableContent(year);
        this.contentContainer.empty().append(yearTableContent);
        this.onYearChangeHandler(year);
    }


    EntitlementTable.prototype.displayAllYears = function () {
        var content = $("<div />");

        for (var i = 0; i < this.years.length; i++) {
            var year = this.years[i];
            var _this = this;

            var tr = _this.getYearTableContent(year, true);
            content.append(tr);
        }

        this.contentContainer.empty().append(content);

        this.onYearChangeHandler(null);
    }


    EntitlementTable.prototype.getYearTableContent = function (year, showYear) {
        var poolInfo = getPoolInfoByYear.call(this, year);
        var container = prepareContainer.call(this, showYear, year, DateTimeTool.fromStrutsDateToDateDisplayable(moment()));
        var containerBody = container.find("tbody");

        if (this.years.length == 0 || year == null || this.years.indexOf(year) === -1)
            return "";

        for (var i = 0; i < poolInfo.allowanceOfAbsenceTypes.length; i++) {
            var allowancePool = poolInfo.allowanceOfAbsenceTypes[i];

            var allowancePoolDisplayabe = $.extend(allowancePool, {
                hasFutureAccrueToDisplay: AbsenceRequestLength.hasFutureAccrueEntitlementToDisplay(allowancePool),
                futureAccrueDisplayable: AbsenceRequestLength.futureAccrueEntitlementToDisplayable(allowancePool),
                hasFutureReduceDisplayable: AbsenceRequestLength.hasFutureReduceEntitlementToDisplay(allowancePool),
                futureReduceDisplayable: AbsenceRequestLength.futureReduceEntitlementToDisplayable(allowancePool),
                allowedDisplayable: AbsenceRequestLength.allowedEntitlementToDisplayable(allowancePool),
                pendingDisplayable: AbsenceRequestLength.pendingEntitlementToDisplayable(allowancePool),
                usedDisplayable: AbsenceRequestLength.usedEntitlementToDisplayable(allowancePool),
            });
            prepareBalanceToDisplay(allowancePoolDisplayabe);

            var rowContent = this.templateRow(allowancePoolDisplayabe).trim();
            var rowObject = $(rowContent);
            var row_id = "EntitlementTable__ROW__id__" + Math.floor((Math.random() * 1000000000) + 1) + "_" + Math.floor((Math.random() * 100000) + 1);
            rowObject.attr("id", row_id);

            var barContainer = $("<div />");
            var barControl = new AbsenceAllowanceBar({
                domSelector: barContainer,
                allowance: allowancePool,
                initHidden: false,
                displayText: 'ALLOWED'
            });
            $(barContainer).data('barControl', barControl);

            rowObject.find(".colAllowance").append(barContainer);

            rowObject.find("button.iconMore")
                .data("rowId", row_id)
                .click(function () {
                    $("#" + $(this).data("rowId") + " .moreDetails").slideToggle();
                });

            containerBody.append(rowObject);
        }

        return container;
    }

    function prepareBalanceToDisplay(allowance) {

        for (var y = 0; y < allowance.balanceSummary.length; y++) {
            var balanceItem = allowance.balanceSummary[y];
            balanceItem.reasonDisplayable = Messages.get("BalanceSummarize." + balanceItem.reason);
            if (balanceItem.reason == "ACCRUAL" && balanceItem.unlimited) {
                balanceItem.amountDisplayable = AbsenceRequestLength.allowedEntitlementToDisplayable(allowance);
            } else {
                balanceItem.amountDisplayable = AbsenceRequestLength.anyAmountOfEntitlementDisplayable(balanceItem.amount, balanceItem.unlimited, allowance);
            }

        }
    }

    function prepareContainer(showYear, year, asOfDay) {
        return $(this.templateTable({
            showYear: showYear == true,
            year: year,
            asOfDay: asOfDay
        }).trim());
    }

    function getPoolInfoByYear(year) {
        var poolInfo = null;

        for (var i = 0; i < this.annualsAbsencePool.length; i++) {
            if (this.annualsAbsencePool[i].calendarYear == year) {
                poolInfo = this.annualsAbsencePool[i];
            }
        }

        //sort
        for (var i = 0; i < poolInfo.allowanceOfAbsenceTypes.length; i++) {
            var allowancePool = poolInfo.allowanceOfAbsenceTypes[i];

            if (allowancePool.referenceAbsenceTypeId != null) {
                var refIndex = -1;

                for (var y = 0; y < poolInfo.allowanceOfAbsenceTypes.length; y++) {

                    if (allowancePool.referenceAbsenceTypeId == poolInfo.allowanceOfAbsenceTypes[y].absenceTypeId) {
                        refIndex = y;
                        break;
                    }
                }

                if (refIndex + 1 >= poolInfo.allowanceOfAbsenceTypes.length && refIndex > -1 && refIndex != i) {
                    Utils.arrayMove(poolInfo.allowanceOfAbsenceTypes, i, refIndex);
                } else if (refIndex > -1 && refIndex != i) {
                    Utils.arrayMove(poolInfo.allowanceOfAbsenceTypes, i, refIndex + 1);
                }
            }
        }

        return poolInfo;
    }

    EntitlementTable.prototype.appendTo = function (selector) {
        $(selector).append(this.contentContainer);
    }

    EntitlementTable.prototype.getYears = function () {
        return this.years;
    }

    EntitlementTable.prototype.onYearChange = function (f) {
        this.onYearChangeHandler = f;
    }

    window.EntitlementTableFactory = {
        forCurrentUser: function () {
            var t = new EntitlementTable();
            return t;
        },

        forPerson: function (id) {
            var t = new EntitlementTable();
            t.forPersonId = id;
            return t;
        }
    };
})
(window);

function EntitlementTable_init() {
    return $.when(
        Dependency3.loadScript("/js/manager/AbsenceTypeManager.js"),
        Dependency3.loadScript("/js/manager/DaysReducingManager.js"),
        Dependency3.loadScript("/js/kth/model/DaysReducingLeavePool.js"),
        Dependency3.loadScript("/template/module_absence/widgets/EntitlementTable/yearTemplate.do", "text/x-dot-template").done(function (desc) {
            __EntitlementTable__template__tab = doT.templateFromElement(desc.id);
        }),
        Dependency3.loadScript("/template/module_absence/widgets/EntitlementTable/manyYearsTemplate.do", "text/x-dot-template").done(function (desc) {
            __EntitlementTable__template__manyyears = doT.templateFromElement(desc.id);
        }),
        Dependency3.loadScript("/template/module_absence/widgets/EntitlementTable/barContent.do", "text/x-dot-template").done(function (desc) {
            __EntitlementTable__template__bar = doT.templateFromElement(desc.id);
        }),
        Dependency3.loadScript("/template/module_absence/widgets/EntitlementTable/rowTemplate.do", "text/x-dot-template").done(function (desc) {
            __EntitlementTable__template__row = doT.templateFromElement(desc.id);
        })
    );
}
//module:init__EntitlementPresentation
(function (window) {
    EntitlementPresentation = function () {
        this.entitlementManager = new EntitlementManager();

        this.container = $('<div />').css('position', 'relative');
        this.table = $('<table style="width: 100%;" class="presentationTable" />').appendTo(this.container);
        this.tableBody = $('<tbody />').appendTo(this.table);
        this.trTemplate = doT.template('<tr style="height: 50px;" id="absencePoolRow{{=it.calendarYear}}"><td class="barControlTd" style="width: 65%; padding-left: 0; padding-right: 0;">{{$it.allowanceBar}}</td><td style="width: 60px;">{{=it.calendarYear}}</td><td>&nbsp;</td></tr>');

        this.absenceTypeId = null;
        this.employeeId = null;
        this.currentUser = null;

        this.selectFromDate = null;
        this.selectToDate = null;
        this.selectFirstDayAmount = null;
        this.selectLastDayAmount = null;
        this.selectFromPart = null;
        this.selectToPart = null;

        this.asOfDay = null;


        this.currentPool = null;

        this.allowanceBars = {};

        //latest known entitlement
        this.latestAbsenceTypeId = null;
        this.latestEmployeeId = null;
        this.latestCurrentUser = null;

        //
        this.amountCounting = null;
    };

    EntitlementPresentation.prototype.appendTo = function (selector) {
        var _t = this;
        $(selector).append(_t.container);
    };

    EntitlementPresentation.prototype.detach = function () {
        var _t = this;
        $(_t.container).detach();
    };

    EntitlementPresentation.prototype.selectDays = function (fromDate, toDate, firstDayAmount, lastDayAmount) {
        this.selectFromDate = fromDate;
        this.selectToDate = toDate;
        this.selectFirstDayAmount = firstDayAmount;
        this.selectLastDayAmount = lastDayAmount;
        this.asOfDay = toDate;

        redrawEntitlement.call(this);
    }

    EntitlementPresentation.prototype.changeEmployee = function (id) {
        this.currentUser = false;
        this.employeeId = id;

        redrawEntitlement.call(this);
    }

    EntitlementPresentation.prototype.changeToCurrentEmployee = function () {
        this.currentUser = true;
        this.employeeId = null;

        redrawEntitlement.call(this);
    }

    EntitlementPresentation.prototype.changeAbsenceType = function (id) {
        this.absenceTypeId = id;
        redrawEntitlement.call(this);
    }

    EntitlementPresentation.prototype.changeAsOfDate = function (day) {
        this.asOfDay = moment(day);

        redrawEntitlement.call(this);
    }

    EntitlementPresentation.prototype.changeParams = function (params) {
        var p = $.extend({
            absenceTypeId: null,
            fromDate: null,
            toDate: null,
            fromPart: null,
            toPart: null,
            asOfDay: null
        }, params);

        this.absenceTypeId = p.absenceTypeId;
        this.selectFromDate = p.fromDate;
        this.selectToDate = p.toDate;
        this.selectFromPart = p.fromPart;
        this.selectToPart = p.toPart;

        this.asOfDay = p.asOfDay;

        redrawEntitlement.call(this);
    }

    //private

    function redrawEntitlement() {
        var _this = this;

        if (isInited.call(this)) {

            reloadPool.call(this).done(function (poolReloaded) {
                if (poolReloaded || latestEmployeeOrAbsenceTypeChanged.call(_this)) {

                    doRedrawPool.call(_this);

                    _this.latestAbsenceTypeId != _this.absenceTypeId;
                    _this.latestEmployeeId = _this.employeeId;
                    _this.latestCurrentUser = _this.currentUser;
                    _this.latestAsOfDay = _this.asOfDay;
                }
            });
        } else {
            this.tableBody.empty();

            this.latestAbsenceTypeId = null;
            this.latestEmployeeId = null;
            this.latestCurrentUser = null;
            this.latestAsOfDay = null;
        }
    }

    function doRedrawPool() {
        var _this = this;

        this.tableBody.empty();
        this.allowanceBars = {};

        for (var i = 0; i < this.currentPool.annualsAbsencePools.length; i++) {
            var annualEntitlement = this.currentPool.annualsAbsencePools[i];
            var tr = $(this.trTemplate(annualEntitlement).trim());
            var barContainer = tr.find(".barControlTd");

            var barControl = new AbsenceAllowanceBar({
                domSelector: barContainer,
                allowance: findAbsenceTypePool(annualEntitlement, _this.absenceTypeId),
                initHidden: false,
                displayText: 'SELECTED',
                displayTooltip: true
            });
            _this.allowanceBars[annualEntitlement.calendarYear] = barControl;

            this.tableBody.append(tr);
        }


        redrawSelection.call(this);
    }

    function redrawSelection() {
        var _this = this;

        console.log("redrawSelection");

        if (isInited.call(this)) {
            if (this.selectFromDate != null && this.selectToDate != null) {
                countAmount.call(this)
                    .done(function (amount) {

                        console.log("countAmount.call(this) = done");
                        for (var year in _this.allowanceBars) {
                            var barControl = _this.allowanceBars[year];
                            var inYearAmount = amount.inYearAmount[year];

                            if (inYearAmount != null) {
                                barControl.selectAmount(inYearAmount);
                            } else {
                                barControl.selectAmount(0);
                            }
                        }
                    })
                    .fail(function () {
                        console.log("countAmount.call(this) = fail");

                        for (var year in _this.allowanceBars) {
                            var barControl = _this.allowanceBars[year];
                            barControl.selectAmount(0);
                        }
                    });
            } else {
                for (var year in this.allowanceBars) {
                    var barControl = this.allowanceBars[year];
                    barControl.selectAmount(0);
                }
            }
        }
    }

    function isInited() {
        return (this.absenceTypeId != null && ((this.currentUser == true) || (this.currentUser == false && this.employeeId != null)) );
    }

    function reloadPool() {
        if (latestPoolShouldBeReloaded.call(this)) {
            var poolDownloading = $.Deferred();
            var _this = this;
            var asOfDay = !!_this.asOfDay && _this.asOfDay.isValid() ? _this.asOfDay : moment();

            if (this.currentUser) {
                this.entitlementManager.getCurrentEmployeeEntitlement(asOfDay).done(function (pool) {
                    _this.currentPool = pool;
                    poolDownloading.resolve(true);
                });
            } else {
                this.entitlementManager.getEmployeeEntitlement(this.employeeId, asOfDay).done(function (pool) {
                    _this.currentPool = pool;
                    poolDownloading.resolve(true);
                });
            }

            _this.latestAsOfDay = moment(_this.asOfDay);
            return poolDownloading.promise();
        } else {
            return $.Deferred().resolve().promise(false);
        }
    }

    function latestPoolShouldBeReloaded() {
        return (
            this.currentPool == null
            ||
            latestEmployeeChanged.call(this)
            ||
            this.latestAsOfDay != this.asOfDay
        );
    }

    function latestEmployeeOrAbsenceTypeChanged() {
        return (
            this.latestAbsenceTypeId != this.absenceTypeId
            ||
            latestEmployeeChanged.call(this)
        );
    }

    function latestEmployeeChanged() {
        return (
            this.latestEmployeeId != this.employeeId
            ||
            this.latestCurrentUser != this.currentUser
        );
    }

    function findAbsenceTypePool(annualEntitlement, absenceTypeId) {
        for (var i = 0; i < annualEntitlement.allowanceOfAbsenceTypes.length; i++) {
            var typeEntitlement = annualEntitlement.allowanceOfAbsenceTypes[i];

            if (typeEntitlement.absenceTypeId == absenceTypeId) {
                return typeEntitlement;
            }
        }

        return null;
    }

    function countAmount() {
        var _this = this;

        if (this.amountCounting != null) {
            this.amountCounting.abort();
        }

        if (this.currentUser) {
            this.amountCounting = this.entitlementManager.countRequiredEntitlementForCurrentEmployeeNew(
                _this.absenceTypeId,
                _this.selectFromDate,
                _this.selectToDate,
                _this.selectFromPart,
                _this.selectToPart
            );
        } else {
            this.amountCounting = this.entitlementManager.countRequiredEntitlementForAnyEmployee(
                _this.employeeId,
                _this.absenceTypeId,
                _this.selectFromDate,
                _this.selectToDate,
                _this.selectFromPart,
                _this.selectToPart
            );
        }

        return this.amountCounting;
    }

    window.EntitlementPresentation = EntitlementPresentation;
})(window);

function init__EntitlementPresentation() {
    return $.when(
    )
}
(function (window, undefined) {

    SettingsManager = function () {
    }

    SettingsManager.prototype.getSettings = function () {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/configuration/get"),
            contentType: "application/json"
        });
    }

    SettingsManager.prototype.saveSettings = function (settings) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/configuration/update"),
            contentType: "application/json",
            data: JSON.stringify(settings)
        });
    }

    window.SettingsManager = SettingsManager;
})(window);
/*
 nowTS - current timestamp according to real server time and user time zone - it will be incremented in frontend
 zeroTimeTS - nowTS when it was downloaded from remote server
 zeroTimeBrowserTS - time in user browser when nowTS was downloaded from remote server

 workStartDiff
 workEndDiff
 * */
(function (window, undefined) {

    ShiftProgressTimer = function (options) {
        this.startOptions = options;

        this.nowTS = 0;
        this.zeroTimeTS = 0;
        this.zeroTimeBrowserTS = 0;
        this.browserTimeFromZeroDiff = 0;
        this.currentShiftInfo = 0;
        this.alarms = {};
        this.workStartDiff = 0;
        this.workEndDiff = 0;
        this.plannedShiftLength = 0;
        this.fromPlannedShiftStartToNowLength = 0;
        this.howLongInWork = 0;
        this.destroing = false;

        this.container = $('<div class="ShiftProgressTimer"/>');
        this.shiftContainer = $('<div class="ShiftProgressTimer_shiftContainer"/>').appendTo(this.container);
        this.plannedStartIndicator = $('<div class="ShiftProgressTimer_plannedStartIndicator"/>').appendTo(this.container);
        this.plannedStopIndicator = $('<div class="ShiftProgressTimer_plannedStopIndicator"/>').appendTo(this.container);
        this.beforeStartContainer = $('<div class="ShiftProgressTimer_beforeStartContainer"/>').appendTo(this.container);
        this.afterFinishContainer = $('<div class="ShiftProgressTimer_afterFinishContainer"/>').appendTo(this.container);
        this.shiftProgressContainer = $('<div class="ShiftProgressTimer_shiftProgressContainer"/>').appendTo(this.container);
        this.inWorkTimeContainer = $('<div class="ShiftProgressTimer_inWorkTimeContainer"/>').appendTo(this.shiftProgressContainer);
        this.inWorkTimeContainerShort = $('<div class="ShiftProgressTimer_inWorkTimeContainerShort"/>').appendTo(this.container);

        this.shiftStartIndicator = $('<div class="ShiftProgressTimer_shiftStartIndicator"/>').hide().appendTo(this.container);
        this.shiftStopIndicator = $('<div class="ShiftProgressTimer_shiftStopIndicator"/>').hide().appendTo(this.container);
    };

    ShiftProgressTimer.prototype.init = function () {
        var _this = this;

        ToolTip.setTip(_this.plannedStartIndicator, Messages.get("ShiftProgressTimer.plannedStartIndicator.tooltip"), true);
        ToolTip.setTip(_this.plannedStopIndicator, Messages.get("ShiftProgressTimer.plannedStopIndicator.tooltip"), true);
        ToolTip.setTip(_this.shiftStartIndicator, Messages.get("ShiftProgressTimer.shiftStartIndicator.tooltip"), true);
        ToolTip.setTip(_this.shiftStopIndicator, Messages.get("ShiftProgressTimer.shiftStopIndicator.tooltip"), true);
        ToolTip.setTip(_this.shiftProgressContainer, Messages.get("ShiftProgressTimer.shiftProgressContainer.tooltip"), true);

        _this.updateData(_this.startOptions);

        return $.Deferred().resolve().promise();
    };

    ShiftProgressTimer.prototype.appendTo = function (selector) {
        var _t = this;
        $(selector).append(_t.container);
    };

    ShiftProgressTimer.prototype.redraw = function () {
        clearTimeout(this.redrawing);

        if (!this.destroing) {
            var _this = this;
            this.recalculate();

            if (this.currentShiftInfo.started)
                this.redrawStarted();
            else
                this.redrawNotStarted();

            this.redrawing = setTimeout(function () {
                _this.redraw();
            }, 1000);
        }
    };

    ShiftProgressTimer.prototype.recalculate = function () {
        this.browserTimeFromZeroDiff = ((new Date()).getTime() - this.zeroTimeBrowserTS)
        this.nowTS = this.zeroTimeTS + this.browserTimeFromZeroDiff;

        if (this.currentShiftInfo.started) {

            if (this.currentShiftInfo.todayWorkStartTS != null) {
                this.workStartDiff = this.currentShiftInfo.todayWorkStartTS - this.currentShiftInfo.currentShift.startedTimeTS;
                this.workEndDiff = this.currentShiftInfo.todayWorkEndTS - this.nowTS;
            } else {
                this.workStartDiff = 0;
                this.workEndDiff = 0;
            }

            this.howLongInWork = this.nowTS - this.currentShiftInfo.currentShift.startedTimeTS;
        } else {
            this.workStartDiff = this.currentShiftInfo.todayWorkStartTS - this.nowTS;
            this.howLongInWork = 0;
        }
    }

    ShiftProgressTimer.prototype.redrawStarted = function () {
        var plannedShiftContainerLeft = this.shiftContainer.position().left;
        var plannedShiftContainerWidth = this.shiftContainer.outerWidth();
        var beforeTimeWidth = 30;

        var shiftProgressLeft = plannedShiftContainerLeft;
        var shiftProgressWidth = 0;


        if (this.thisDayHasDefinedWorkingHours()) {
            if (this.isLateForWork()) {
                this.beforeStartContainer
                    .text(Messages.get("ShiftProgressTimer.started.late", DateTimeTool.msToDisplayableDuration(Math.abs(this.workStartDiff))))
                    .addClass("late")
                    .show();
            } else if (this.isAfterTimeForWork()) {
                this.beforeStartContainer
                    .text(Messages.get("ShiftProgressTimer.started.afterTime", DateTimeTool.msToDisplayableDuration(Math.abs(this.workStartDiff))))
                    .removeClass("late")
                    .show();
            } else {
                this.beforeStartContainer
                    .text(Messages.get("ShiftProgressTimer.started.startBeforeTheTime", DateTimeTool.msToDisplayableDuration(Math.abs(this.workStartDiff))))
                    .removeClass("late")
                    .show();
            }
        } else {
            this.beforeStartContainer.hide();
        }


        if (this.isAfterTimeForWork()) {
            shiftProgressLeft += Math.round(plannedShiftContainerWidth * (Math.abs(this.workStartDiff) / this.plannedShiftLength))
        } else {
            shiftProgressLeft -= beforeTimeWidth;
            shiftProgressWidth += beforeTimeWidth;
        }

        if (this.isAfterTimeForWork()) {
            shiftProgressWidth +=
                Math.min(
                    plannedShiftContainerWidth - (shiftProgressLeft - plannedShiftContainerLeft),
                    Math.floor(plannedShiftContainerWidth * (this.howLongInWork / this.plannedShiftLength))
                );
        } else if (this.nowIsBeforePlannedStart()) {
            shiftProgressWidth += -beforeTimeWidth / 2;
        } else {
            shiftProgressWidth +=
                Math.min(
                    plannedShiftContainerWidth,
                    Math.floor(plannedShiftContainerWidth * ((this.nowTS - this.currentShiftInfo.todayWorkStartTS) / this.plannedShiftLength))
                )
        }


        if (this.itsAfterPlannedWorkEnd()) {
            shiftProgressWidth += beforeTimeWidth;
        }

        if (this.thisDayHasDefinedWorkingHours()) {
            if (this.itsAfterPlannedWorkEnd()) {
                this.afterFinishContainer
                    .text(Messages.get("ShiftProgressTimer.started.toStopAfterPlanned", DateTimeTool.msToDisplayableDuration(Math.abs(this.workEndDiff))))
                    .addClass("overtime");
            } else {
                this.afterFinishContainer
                    .text(Messages.get("ShiftProgressTimer.started.toStopBeforePlanned", DateTimeTool.msToDisplayableDuration(Math.abs(this.workEndDiff))))
                    .removeClass("overtime");
            }
        } else {
            this.afterFinishContainer.hide();
        }


        this.shiftProgressContainer.css("left", shiftProgressLeft + "px");
        this.shiftProgressContainer.css("width", shiftProgressWidth + "px");

        this.shiftProgressContainer.show();
        this.inWorkTimeContainer.text(DateTimeTool.msToDisplayableStoperDuration(Math.abs(this.howLongInWork)));
        this.inWorkTimeContainer.css("left", shiftProgressLeft + "px");

        this.inWorkTimeContainer.css("width", shiftProgressWidth + "px");
        if (this.inWorkTimeContainer.get(0).offsetWidth >= this.inWorkTimeContainer.get(0).scrollWidth) {
            this.inWorkTimeContainer.css("visibility", "visible");
            this.inWorkTimeContainerShort.hide();
        } else {

            this.inWorkTimeContainer.css("visibility", "hidden");
            this.inWorkTimeContainerShort
                .css("left", Math.round(shiftProgressLeft + 5) + "px")
                .text(DateTimeTool.msToDisplayableStoperDuration(Math.abs(this.howLongInWork)))
                .show();
        }


        this.shiftStartIndicator.css("left", shiftProgressLeft + "px").show();
        this.shiftStopIndicator.css("left", (shiftProgressLeft + shiftProgressWidth) + "px").show();

        if (shiftProgressWidth < 15) {
            this.shiftStopIndicator.text(" ");
        } else {
            this.shiftStopIndicator.text(moment(this.currentShiftInfo.myTime).add(this.browserTimeFromZeroDiff, 'ms').format(DateTimeSettings.getTimeFormatFromMoment()));
        }


    }

    ShiftProgressTimer.prototype.redrawNotStarted = function () {
        if (this.workStartDiff > 0) {
            this.beforeStartContainer
                .text(Messages.get("ShiftProgressTimer.notstarted.startIn", DateTimeTool.msToDisplayableDuration(Math.abs(this.workStartDiff))))
                .removeClass("late");
        } else {
            this.beforeStartContainer
                .text(Messages.get("ShiftProgressTimer.notstarted.startLate", DateTimeTool.msToDisplayableDuration(Math.abs(this.workStartDiff))))
                .addClass("late");
        }

        this.shiftProgressContainer.hide();
        this.inWorkTimeContainer.css("visibility", "hidden");
        this.shiftStartIndicator.hide();
        this.shiftStopIndicator.hide();
        this.inWorkTimeContainerShort.hide();
    }

    ShiftProgressTimer.prototype.destroy = function () {
        this.destroing = true;
        clearTimeout(this.redrawing);
    };


    ShiftProgressTimer.prototype.updateData = function (options, forceRedraw) {
        clearTimeout(this.redrawing);

        var settings = $.extend({
            currentShiftInfo: {},
            zeroTimeTS: 0,
            zeroTimeBrowserTS: 0,
            alarms: {}
        }, options);


        this.nowTS = settings.zeroTimeBrowserTS;
        this.zeroTimeTS = settings.zeroTimeTS;
        this.zeroTimeBrowserTS = settings.zeroTimeBrowserTS;
        this.currentShiftInfo = settings.currentShiftInfo;
        this.alarms = settings.alarms;

        if (this.thisDayHasDefinedWorkingHours()) {
            this.plannedShiftLength = settings.currentShiftInfo.todayWorkEndTS - settings.currentShiftInfo.todayWorkStartTS;
            this.plannedStartIndicator.text(DateTimeTool.fromStrutsTimeToDisplayable(this.currentShiftInfo.todayWorkStart)).show();
            this.plannedStopIndicator.text(DateTimeTool.fromStrutsTimeToDisplayable(this.currentShiftInfo.todayWorkEnd)).show();
            this.beforeStartContainer.show();
            this.afterFinishContainer.show();
        } else {
            this.plannedShiftLength = 0;

            this.plannedStartIndicator.hide();
            this.plannedStopIndicator.hide();
            this.beforeStartContainer.hide();
            this.afterFinishContainer.hide();
        }

        this.workStartDiff = 0;

        if (this.currentShiftInfo.started)
            this.shiftStartIndicator.text(moment(this.currentShiftInfo.currentShift.startedTime).format(DateTimeSettings.getTimeFormatFromMoment()));

        this.redraw();
    }


    ShiftProgressTimer.prototype.isAfterTimeForWork = function () {
        return this.workStartDiff < 0;
    }

    ShiftProgressTimer.prototype.isLateForWork = function () {
        return this.workStartDiff <= -(this.alarms.lateForWork * 60 * 1000);
    }

    ShiftProgressTimer.prototype.itsAfterPlannedWorkEnd = function () {
        return this.nowTS > this.currentShiftInfo.todayWorkEndTS;
    }

    ShiftProgressTimer.prototype.thisDayHasDefinedWorkingHours = function () {
        return this.currentShiftInfo.todayWorkStartTS != null;
    }

    ShiftProgressTimer.prototype.nowIsBeforePlannedStart = function () {
        return (this.nowTS < this.currentShiftInfo.todayWorkStartTS);
    }

    window.ShiftProgressTimer = ShiftProgressTimer;
})(window);
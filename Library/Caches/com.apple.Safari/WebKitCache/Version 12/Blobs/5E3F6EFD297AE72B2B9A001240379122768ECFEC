(function (window, undefined) {

    TerminalManager = function () {
    }

    TerminalManager.prototype.openTerminalsList = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/list.do"));
    }

    TerminalManager.prototype.openNewWebappTerminalWindow = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/webapp-new.do"));
    }

    TerminalManager.prototype.openNewBeaconTerminalWindow = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/beacon-new.do"));
    }

    TerminalManager.prototype.openNewQRCodeTerminalWindow = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/qrcode-new.do"));
    }

    TerminalManager.prototype.openApiTerminalWindow = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/api-new-edit.do"));
    }

    TerminalManager.prototype.openMobileTerminalWindow = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/mobile-new-edit.do"));
    }

    TerminalManager.prototype.openSlackTerminalWindow = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/slack-new-edit.do"));
    }

    TerminalManager.prototype.openTogglTerminalWindow = function () {
        Location.openWindow(URL2.get("/clockin/conf/terminals/toggl-new-edit.do"));
    }

    TerminalManager.prototype.deleteTerminal = function (id) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/delete"),
            contentType: "application/json",
            data: JSON.stringify({
                id: id
            })
        });
    }

    TerminalManager.prototype.getTerminals = function () {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/list"),
            contentType: "application/json"
        });
    }

    TerminalManager.prototype.listEmployeesAssignedForTerminal = function (id) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/listEmployeesAssignedForTerminal"),
            contentType: "application/json",
            data: JSON.stringify({
                id: id
            })
        });
    }

    TerminalManager.prototype.getTerminalTypesForCurrentPerson = function () {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/listTerminalTypesForCurrentPerson"),
            contentType: "application/json"
        });
    }

    TerminalManager.prototype.openEditWindow = function (terminal) {
        if (terminal.type == "webApp") {
            Location.openWindow(URL2.get("/clockin/conf/terminals/webapp-edit.do"), {
                id: terminal.id
            });
        } else if (terminal.type == "beacon") {
            Location.openWindow(URL2.get("/clockin/conf/terminals/beacon-edit.do"), {
                id: terminal.id
            });
        } else if (terminal.type == "qrcode") {
            Location.openWindow(URL2.get("/clockin/conf/terminals/qrcode-edit.do"), {
                id: terminal.id
            });
        } else if (terminal.type == "api") {
            Location.openWindow(URL2.get("/clockin/conf/terminals/api-new-edit.do"));
        } else if (terminal.type == "slack") {
            Location.openWindow(URL2.get("/clockin/conf/terminals/slack-new-edit.do"));
        } else if (terminal.type == "toggl") {
            Location.openWindow(URL2.get("/clockin/conf/terminals/toggl-new-edit.do"));
        } else if (terminal.type == "mobile") {
            Location.openWindow(URL2.get("/clockin/conf/terminals/mobile-new-edit.do"));
        } else {
            throw "UNSUPPORDED TERMINAL TYPE " + terminal.type
        }
    }

    TerminalManager.prototype.get = function (terminal) {
        if (terminal.type == "webApp") {
            var webAppTerminalManager = new WebAppTerminalManager;
            return webAppTerminalManager.get(terminal.id);
        } else if (terminal.type == "beacon") {
            var beaconTerminalManager = new BeaconTerminalManager();
            return beaconTerminalManager.get(terminal.id);

        } else if (terminal.type == "qrcode") {
            var qrcodeTerminalManager = new QRCodeTerminalManager();
            return qrcodeTerminalManager.get(terminal.id);
        } else if (terminal.type == "api") {
            var apiTerminalManager = new ApiTerminalManager();
            return apiTerminalManager.getState();
        } else if (terminal.type == "slack") {
            var slackTerminalManager = new SlackTerminalManager();
            return slackTerminalManager.getState();
        } else if (terminal.type == "toggl") {
            var togglTerminalManager = new TogglTerminalManager();
            return togglTerminalManager.getState();
        } else if (terminal.type == "mobile") {
            var mobileTerminalManager = new MobileTerminalManager();
            return mobileTerminalManager.getState();
        } else {
            throw "UNSUPPORDED TERMINAL TYPE " + terminal.type
        }
    }

    TerminalManager.prototype.generateTerminalName = function (type) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/generateTerminalName"),
            contentType: "application/json",
            data: JSON.stringify({
                type: type
            })
        });
    }

    window.TerminalManager = TerminalManager;


    /*Web app terminal*/

    WebAppTerminalManager = function () {
    }

    WebAppTerminalManager.prototype.get = function (id) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/webapp/get"),
            contentType: "application/json",
            data: JSON.stringify({
                id: id
            })
        });
    }

    WebAppTerminalManager.prototype.webappAvailableForCurrentUser = function () {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/webapp/availability"),
            contentType: "application/json"
        });
    }

    WebAppTerminalManager.prototype.clockIn = function (id) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/webapp/clock-in"),
            contentType: "application/json"
        });
    }

    WebAppTerminalManager.prototype.clockOut = function (id) {
        var stoppingShift = $.Deferred();

        Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/webapp/clock-out"),
            contentType: "application/json",

            success: function (response) {
                stoppingShift.resolve(response);
            },

            "error_OPEN_SHIFT_NOT_FOUND": function () {
                stoppingShift.reject("OPEN_SHIFT_NOT_FOUND");
            },

            error: function () {
                stoppingShift.reject();
            }
        });

        return stoppingShift;
    }

    WebAppTerminalManager.prototype.startBreak = function (typeId) {
        var startingBreak = $.Deferred();

        Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/webapp/break-start"),
            contentType: "application/json",
            data: JSON.stringify({
                breakType: typeId
            }),

            success: function(response){
                startingBreak.resolve(response);
            },

            "error_OPEN_SHIFT_NOT_FOUND": function () {
                startingBreak.reject("OPEN_SHIFT_NOT_FOUND");
            },

            error: function () {
                startingBreak.reject();
            }
        });

        return startingBreak;
    }

    WebAppTerminalManager.prototype.stopBreak = function () {
        var stoppingBreak = $.Deferred();

        Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/webapp/break-stop"),
            contentType: "application/json",
            success: function (response) {
                stoppingBreak.resolve(response);
            },

            "error_AUTOINCLUDED_MANUAL_NOT_ALLOWED": function () {
                stoppingBreak.reject("AUTOINCLUDED_MANUAL_NOT_ALLOWED");
            },

            error: function () {
                stoppingBreak.reject();
            }
        });

        return stoppingBreak.promise();
    }

    WebAppTerminalManager.prototype.worklogFromBeginning = function (projectId) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/workloging/from-beginning"),
            contentType: "application/json",
            data: JSON.stringify({
                projectId: projectId
            })
        });
    }

    WebAppTerminalManager.prototype.worklogSwitch = function (projectId) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/workloging/switch-job"),
            contentType: "application/json",
            data: JSON.stringify({
                projectId: projectId
            })
        });
    }

    WebAppTerminalManager.prototype.worklogGetCurrent = function (projectId) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/workloging/get-current"),
            contentType: "application/json",
            data: JSON.stringify({
                projectId: projectId
            })
        });
    }

    window.WebAppTerminalManager = WebAppTerminalManager;


    /**/
    /*Beacon terminal*/

    BeaconTerminalManager = function () {
    }

    BeaconTerminalManager.prototype.get = function (id) {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/beacon/get"),
            contentType: "application/json",
            data: JSON.stringify({
                id: id
            })
        });
    }

    BeaconTerminalManager.prototype.getMyProfile = function () {
        return Ajax.post({
            url: URL2.get("/webapi/clockin/terminal/beacon/my-profile"),
            contentType: "application/json"
        });
    }

    window.BeaconTerminalManager = BeaconTerminalManager;

    /**/
    /*Beacon terminal*/
    QRCodeTerminalManager = function () {
    }

    QRCodeTerminalManager.prototype.get = function (id) {

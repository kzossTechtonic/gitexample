(function (window) {

    $.Velocity.RegisterEffect("chr.fadeFromRightIn", {
        defaultDuration: 350,
        calls: [
            [ { opacity: [ 1, "easeInCubic", 0 ], translateX: [ 0, "easeInOutCubic", 40 ], translateZ: 0 } ]
        ]
    });

    Animator = function () {
    };

    Animator.prototype.in = function (element, effectName, postAnimationCallback) {
        $(element).velocity(
            "chr."+effectName+"In",

            {
                display: "",
                complete: function(){
                    postAnimationCallback();
                }
            }
        );
    }

    Animator.prototype.inSequence = function (elementsSelector, effectName, postAnimationCallback) {
        $(elementsSelector).velocity(
            "chr."+effectName+"In",

            {
                stagger: 25,
                display: "",
                drag: true,
                complete: function(){
                    postAnimationCallback();
                }
            }
        );
    };




    Animator.prototype.heightTo = function (element, expectedHeight, postAnimationCallback) {
        $(element).velocity(
            "chr."+effectName+"In",

            {
                display: "",
                complete: function(){
                    postAnimationCallback();
                }
            }
        );
    }



    Animator.prototype.animateIn = function (element, effectName, postAnimationCallback) {
        var hideFinalizationTimeout = $(element).data("hideFinalizationTimeout");

        if(hideFinalizationTimeout!=null){
            clearTimeout(hideFinalizationTimeout);
        }

        $(element).one("webkitAnimationEnd", function (e) {
            $(e.target).removeClass(effectName + 'In');

            if (typeof(postAnimationCallback) == "function") {
                postAnimationCallback(e.target);
            }
        });

        $(element).one("animationend", function (e) {
            $(e.target).removeClass(effectName + 'In');

            if (typeof(postAnimationCallback) == "function") {
                postAnimationCallback(e.target);
            }
        });

        $(element).removeClass(effectName+'Out');
        $(element).addClass(effectName + 'In animated').css('display', '');
        $(element).removeClass('preInAnimation');
    };

    Animator.prototype.animateOut = function (element, effectName) {
        $(element)
            .removeClass('preInAnimation')
            .removeClass(effectName + 'In')
            .addClass(effectName + 'Out');

        var oldHideFinalizationTimeout = $(element).data("hideFinalizationTimeout");

        if(oldHideFinalizationTimeout!=null){
            clearTimeout(oldHideFinalizationTimeout);
        }

        var hideFinalizationTimeout =  setTimeout(function () {

            $(element)
                .css('display', 'none')
                .addClass('preInAnimation')
                .removeClass(effectName + 'Out');
        }, 400);

        $(element).data("hideFinalizationTimeout", hideFinalizationTimeout);
    };

    Animator.prototype.animateInSequence = function (elementsSelector, effectName, postAnimationCallback) {
        var elements = $(elementsSelector);
        var elementsCount = elements.length;


        elements.each(function (index, element) {
            var x = (Math.PI / (2 * elementsCount)) * index;
            var delay = Math.sin(x) * 300;

            $(element).one("webkitAnimationEnd", function (e) {
                $(e.target).removeClass(effectName + 'In');

                if (typeof(postAnimationCallback) == "function") {
                    postAnimationCallback(e.target);
                }
            });

            $(element).one("animationend", function (e) {
                $(e.target).removeClass(effectName + 'In');

                if (typeof(postAnimationCallback) == "function") {
                    postAnimationCallback(e.target);
                }
            });

            setTimeout(function () {
                $(element).addClass(effectName + 'In animated');
                $(element).removeClass('preInAnimation');
            }, delay);

        });
    };

    window.Animator = new Animator();
})(window);
(function (window) {
    Spinner = function (options) {
        this.settings = $.extend({
            showDelay: 500,
        }, options);

        this.container = $('<span class="Spinner-Container" />').hide();
        this.showingTimeout = null;

        this.loader = $('<div><div class="circle"></div><div class="circle"></div><div class="circle"></div><div class="circle"></div><div class="circle"></div></div>')
            .addClass("loader")
            .preventDragAndSelection();

        var loaderContainer = $('<div class="loaderContainer" />')
            .preventDragAndSelection()
            .append(this.loader);


        this.container.append(loaderContainer);

        this.showingCounter = 0;
        this.showing = false;

    };

    Spinner.prototype.pinTo = function (selector) {
        $(selector).append(this.container);
    };

    Spinner.prototype.show = function () {
        this.showingCounter = this.showingCounter + 1;
        this.container.show();
    };

    Spinner.prototype.showDelayed = function () {
        var _this = this;

        this.showingCounter = this.showingCounter + 1;

        if (this.showing == false) {
            this.showing = true;

            this.showingTimeout = setTimeout(function () {
                _this.container.show();
            }, _this.settings.showDelay);
        }
    };

    Spinner.prototype.hide = function () {
        this.showingCounter = this.showingCounter - 1;

        if (this.showingCounter == 0) {
            clearTimeout(this.showingTimeout);
            this.container.hide();
            this.showing = false;
        }
    };

    Spinner.prototype.dark = function () {
        this.loader.addClass("dark");
    };

    Spinner.prototype.small = function () {
        this.loader.addClass("small");
    };

    Spinner.prototype.withBackground = function () {
        this.container.addClass("withBackground");
    };


    window.Spinner = Spinner;
})(window);

(function (window, undefined) {
    loaderShowRequests = new Array();

    function init() {
        $("#LockingHourglass").remove();

        var loader = $('<div><div class="circle"></div><div class="circle"></div><div class="circle"></div><div class="circle"></div><div class="circle"></div></div>')
            .addClass("loader")
            .preventDragAndSelection();

        var loaderContaienr = $('<div class="loaderContainer" />')
            .preventDragAndSelection()
            .append(loader);

        var txtContaienr = $('<div class="text" />')
            .preventDragAndSelection();


        $("<div />")
            .attr("id", "LockingHourglass")
            .addClass("hidden")
            .append(txtContaienr)
            .append(loaderContaienr)
            .preventDragAndSelection()
            .appendTo("body");
    };

    function displayLoader(text) {
        if (text == undefined || text == null)
            text = "";

        $("#LockingHourglass .text").text(text);

        $("#LockingHourglass").removeClass("hidden");
    }

    function removeTimeout(timeoutId) {
        clearTimeout(timeoutId);

        for (var i = 0; i < loaderShowRequests.length; i++) {
            if (timeoutId == loaderShowRequests[i].id) {
                loaderShowRequests.splice(i, 1);
                return;
            }
        }
    }

    function clearTimeouts() {
        loaderShowRequests = new Array();
    }

    function showRequestsCount() {
        return loaderShowRequests.length;
    }

    function displayLastText() {
        for (var i = loaderShowRequests.length - 1; i >= 0; i--) {
            if (loaderShowRequests[i].text != undefined) {
                $("#LockingHourglass span").text(loaderShowRequests[i].text);
                return;
            }
        }
    }

    LockingHourglass = function () {

    };


    LockingHourglass.show = function (loaderText, waitForMilliseconds) {
        if (waitForMilliseconds == undefined || waitForMilliseconds == null) {
            waitForMilliseconds = 500;
        }

        var timeoutId = setTimeout(function () {
            displayLoader(loaderText);
        }, waitForMilliseconds);

        loaderShowRequests.push({
            id: timeoutId,
            text: loaderText
        });

        return timeoutId;
    };

    LockingHourglass.showIntermediate = function (loaderText) {
        return LockingHourglass.show(loaderText, 0);
    };

    LockingHourglass.stopWaiting = function (showId) {
        if (showId != undefined && showId != null) {
            clearTimeout(showId);

            textToShow = "";

            for (var i = 0; i < loaderShowRequests.length; i++) {
                if (showId == loaderShowRequests[i].id) {
                    textToShow = loaderShowRequests[i].text;
                }
            }

            displayLoader(textToShow);
        }
    };

    LockingHourglass.hide = function (showId) {
        if (showId != undefined && showId != null)
            removeTimeout(showId);
        else
            clearTimeouts();

        if (showRequestsCount() == 0) {
            $("#LockingHourglass").addClass("hidden");//fadeOut();
        } else
            displayLastText();
    };

    window.LockingHourglass = LockingHourglass;

    $(function () {
        init();
    });
})(window);
/** tmajewski jQuery plugin to manipulation around drag and drop elements */

(function ($) {
    $.fn.preventDragging = function () {
        return this
            .addClass("lockDragging")
            .on('dragstart', function () {
                return false;
            });
    };

    $.fn.preventSelection = function () {
        return this
            .addClass("lockSelecting");
    };

    $.fn.preventDragAndSelection = function () {
        return this
            .preventDragging()
            .preventSelection();
    };


}(jQuery));


(function (window, undefined) {
    var lastKnownHash = null;

    var lastKnownPage = null;
    var lastKnownInternalParameters = null;
    var currentParameters = {};

    function init() {
        onHashChanged();

        if ("onhashchange" in window)
            bindOnHashChangeEvent();
        else
            checkChangeOfHashInIntervals();
    }

    function bindOnHashChangeEvent() {
        window.onhashchange = onHashChanged;
    }

    function checkChangeOfHashInIntervals() {
        if (window.location.hash != lastKnownHash) {
            lastKnownHash = window.location.hash;
            onHashChanged();
        }

        setTimeout(function () {
            checkChangeOfHashInIntervals();
        }, 300); //TODO:
    }

    function onHashChanged() {
        //if hash is present location.hash returns "#....." (first char of returned string is #)
        //replace becasuse; google chrome encoding # to %23 to we have to do it..
        var hashLocation = decodeURI(window.location.hash).replace(/\%23/g, '#');
        var locationParts = hashLocation.split("#");

        var justChangedToPage = null;
        var justChangedToParams = null;

        if (locationParts.length == 2)
            justChangedToPage = locationParts[1];
        else if (locationParts.length > 2) {
            justChangedToPage = locationParts[1];
            justChangedToParams = locationParts[2];
        }


        if (justChangedToPage != lastKnownPage) {
            lastKnownPage = justChangedToPage;
            lastKnownInternalParameters = justChangedToParams;

            if (justChangedToPage == null) {
                window.location.reload();
            } else {
                onPageChanged(justChangedToPage, justChangedToParams);
            }
        } else if (justChangedToParams != lastKnownInternalParameters) {
            lastKnownPage = justChangedToPage;
            lastKnownInternalParameters = justChangedToParams;

            onParametersChanged(justChangedToParams);
        }
    }

    function onPageChanged(pageUrl, params) {
        if (params != "")
            params = JSON.parse(params);
        else
            params = {};

        currentParameters = params;
        loadPage(pageUrl, params);
    }

    function onParametersChanged(params) {
        if (params != "")
            params = JSON.parse(params);
        else
            params = {};

        currentParameters = params;
        $(window.eventBus).trigger({
            type: 'Location.paramsChanged',
            params: params
        });
    }

    function loadPage(pageUrl, paramsObject) {
        if (pageUrl != null) {
            if (pageUrl.indexOf("https://") == 0 || pageUrl.indexOf("http://") == 0 || pageUrl.indexOf("://") != -1) {
                return;
            }
        }

        var loadingId = null;

        $.ajax({
            url: pageUrl,
            method: 'POST',

            beforeSend: function () {
                $(window.eventBus).trigger('Location.loadPageStart');
            },

            complete: function () {

            },

            headers: {
                "X-CALAMARI-WEBAPP-REQUEST": "1"
            },

            error: function (xhr, textStatus, errorThrown) {
                if (401 == xhr.status) {
                    $(window.eventBus).trigger('Location.loadPageError', {err: "401", url: pageUrl});
                } else if (403 == xhr.status) {
                    $(window.eventBus).trigger('Location.loadPageError', {err: "403", url: pageUrl});
                } else {
                    $(window.eventBus).trigger('Location.loadPageError', {err: textStatus, url: pageUrl});
                }
            },

            success: function (data, textStatus, request) {
                $(window.eventBus).trigger('Location.unloading');
                $("#mainWindowContainer").empty();
                $(window.eventBus).trigger('Location.unloaded');


                if (request.getResponseHeader('initable') != null || _.endsWith(pageUrl, ".html")) {
                    $(window.eventBus).one('Location.inited', function () {
                        pageLoadFinishedHandler(loadingId, paramsObject);
                    });

                    var domData = $("<section>" + data + "</section>");//todo temporary solution

                    $(domData).find("c-label").each(function () {
                        var _t = $(this);
                        _t.text(Messages.get(_t.attr("name")));
                    });

                    $("#mainWindowContainer").html(domData);
                } else {
                    $("#mainWindowContainer").html(data);
                    $(document).ready(function () {
                        pageLoadFinishedHandler(loadingId)
                    });
                }
            }
        });
    }

    function pageLoadFinishedHandler(loadingId) {
        $(window.eventBus).trigger('Location.showing');
        $(window.eventBus).trigger('Location.afterShowing');
    }

    Location = function () {

    };

    Location.prototype.openWindow = function (url, argumentObject) {
        var argumentPart = (argumentObject instanceof Object) ? "#" + JSON.stringify(argumentObject) : "";

        window.location.hash = "#" + url + argumentPart;
    };

    Location.prototype.bindEventForLocation = function (eventName, eventListener) {
        $(window.eventBus).on(eventName, eventListener);

        $(window.eventBus).one('Location.unloading', function () {
            $(window.eventBus).off(eventName, eventListener);
        });
    };

    Location.prototype.getPage = function () {
        return lastKnownPage;
    };

    Location.prototype.getParameters = function () {
        return currentParameters != null ? currentParameters : {};
    };

    Location.prototype.getParameter = function (name) {
        if (currentParameters == null) {
            return null;
        } else {
            return currentParameters[name];
        }
    };

    Location.prototype.getParameterOrDefault = function (name, defaultValueOrFunc) {
        var value = undefined;

        if (currentParameters == null) {
            value == null;
        } else {
            value = currentParameters[name];
        }

        if (value == null && value == undefined) {
            if (_.isFunction(defaultValueOrFunc)) {
                value = defaultValueOrFunc();
            } else {
                value = defaultValueOrFunc;
            }
        }

        return value;
    };

    Location.prototype.setParameter = function (name, value) {
        var page = this.getPage();
        var curParams = this.getParameters();

        curParams[name] = value;

        window.location.hash = "#" + page + "#" + JSON.stringify(curParams);
    }

    Location.prototype.stateParameter = function (name, value) {
        var page = this.getPage();
        var curParams = this.getParameters();

        curParams[name] = value;

        currentParameters = curParams;

        window.history.replaceState(null, null, "#" + page + "#" + JSON.stringify(curParams));
    }

    Location.prototype.clearStateParameter = function (name) {
        this.stateParameter(name, undefined);
    }

    Location.prototype.reload = function () {
        window.location.reload();
    };

    Location.prototype.getModulePath = function () {
        return window.location.pathname;
    }

    window.Location = new Location();

    init();
})(window);
(function (window, undefined) {

    var today;

    var timezone;

    var firstDayOfWeek;

    var dateFormatFromMoment;

    var timeFormatFromMoment;

    var dateTimeFormatFromMoment;

    var dateTimeFormatFromDate;

    var dateFormatFromDate;


    var DateTimeSettings = function () {
    };

    DateTimeSettings.prototype.getToday = function () {
        return moment(this.today);
    };

    DateTimeSettings.prototype.setToday = function (date) {
        this.today = moment(date);
    };

    DateTimeSettings.prototype.getTimezone = function () {
        return this.timezone;
    };

    DateTimeSettings.prototype.setTimezone = function (value) {
        this.timezone = value;
    };

    DateTimeSettings.prototype.getDateFormatFromDate = function () {
        return this.dateFormatFromDate;
    };

    DateTimeSettings.prototype.setDateFormatFromDate = function (value) {
        this.dateFormatFromDate = value;
    };

    DateTimeSettings.prototype.getDateTimeFormatFromDate = function () {
        return this.dateTimeFormatFromDate;
    };

    DateTimeSettings.prototype.setDateTimeFormatFromDate = function (value) {
        this.dateTimeFormatFromDate = value;
    };


    DateTimeSettings.prototype.getDateFormatFromMoment = function () {
        return this.dateFormatFromMoment;
    };

    DateTimeSettings.prototype.setDateFormatFromMoment = function (value) {
        this.dateFormatFromMoment = value;
    };

    DateTimeSettings.prototype.getDateTimeFormatFromMoment = function () {
        return this.dateTimeFormatFromMoment;
    };

    DateTimeSettings.prototype.setDateTimeFormatFromMoment = function (value) {
        this.dateTimeFormatFromMoment = value;
    };


    DateTimeSettings.prototype.getFirstDayOfWeek = function () {
        return this.firstDayOfWeek;
    };

    DateTimeSettings.prototype.setFirstDayOfWeek = function (value) {
        this.firstDayOfWeek = value;
    };

    DateTimeSettings.prototype.setTimeFormatFromMoment = function (value) {
        this.timeFormatFromMoment = value;
    };

    DateTimeSettings.prototype.getTimeFormatFromMoment = function () {
        return this.timeFormatFromMoment;
    };

    DateTimeSettings.prototype.mondayIsFirstDayOfWeek = function () {
        return this.firstDayOfWeek == 1;
    };

    DateTimeSettings.prototype.sundayIsFirstDayOfWeek = function () {
        return this.firstDayOfWeek == 0;
    };

    DateTimeSettings.prototype.saturdayIsFirstDayOfWeek = function () {
        return this.firstDayOfWeek == 6;
    };

    window.DateTimeSettings = new DateTimeSettings();

})(window);
(function(window, undefined) {

	StrutsValidation = function() {
	};

	StrutsValidation.prototype.isValid = function(xhr){
		return (xhr.status!=409);
	};

	StrutsValidation.prototype.markErrorsOnForm = function(form, xhr, clearPreviousErrors){
		var validationData = jQuery.parseJSON( xhr.responseText );
		if(clearPreviousErrors==true){
			this.clearErrors(form);
		}
		
		if(validationData!=null){
			var formDom = $(form);
			
			for(fieldName in validationData.fieldErrors){
				var errorMessage = $('<span class="errorMessage"/>').text(validationData.fieldErrors[fieldName]);
				
				
				var elementDom = formDom.find("[name='"+fieldName+"']"); 
				
				elementDom.addClass("error")
				
				if(elementDom.attr("validation-message")!="hidden")
					elementDom.after(errorMessage);
			}
		}
	};
	
	StrutsValidation.prototype.clearErrors = function(form){
		var formDom = $(form);
		
		formDom.find(".error").removeClass("error");
		
		formDom.find(".errorMessage").remove();
	};
	
	window.StrutsValidation = new StrutsValidation();
})(window);
(function (window, undefined) {

        SimpleValidation = function () {
        };

        SimpleValidation.prototype.isValid = function (xhr) {
            return (xhr.status != 409);
        };

        SimpleValidation.prototype.markErrorsOnForm = function (form, xhr, clearPreviousErrors) {
            var validationData = jQuery.parseJSON(xhr.responseText);

            if (clearPreviousErrors == true) {
                this.clearErrors(form);
            }

            if (validationData != null) {
                var formDom = $(form);


                for (fieldName in validationData.fields) {
                    var errorMessage = $('<span class="errorMessage"/>').text(validationData.fields[fieldName]);
                    var elementDom = formDom.find("[name='" + fieldName + "']");
                    if (elementDom != null && elementDom.length > 0) {
                        elementDom.addClass("error");
                        if (elementDom.attr("validation-hidemessage") != "true") {
                            var tagNameOfElement = elementDom.prop("tagName");
                            var typeOfElement = elementDom.attr("type");
                            if (tagNameOfElement != null && tagNameOfElement.toUpperCase() == "INPUT" && typeOfElement != null && typeOfElement.toUpperCase() == "CHECKBOX") {
                                if (elementDom.hasClass("checkbox")) {
                                    $(elementDom).closest("div.checkboxContainer").after(errorMessage);
                                } else {
                                    elementDom.after(errorMessage);
                                }
                            } else {
                                elementDom.after(errorMessage);
                            }
                        }
                    }
                }

                var nonFieldContainer = formDom.find(".errorNonfield > ul");
                if (nonFieldContainer.length == 0) {
                    nonFieldContainer = $("<ul />");
                    formDom.find(".errorNonfield").append(nonFieldContainer);
                }

                for (var i = 0; i < validationData.nonfields.length; i++) {
                    var message = validationData.nonfields[i];

                    $('<li class="nonFieldErrorMessage"/>').text(message).appendTo(nonFieldContainer);
                }


                var firstErrorOfTopPosition = -1;
                var el = null;

                formDom.find(".error,.errorNonfield>li").each(function (index, element) {
                    var elementTop = $(element).position().top;

                    if (firstErrorOfTopPosition == -1 || elementTop < firstErrorOfTopPosition) {
                        firstErrorOfTopPosition = elementTop;
                        el = element;
                    }
                });

                if (firstErrorOfTopPosition > -1) {
                    $.scrollTo(firstErrorOfTopPosition, 500);
                    $(el).focus();
                }
            }
        }
        ;

        SimpleValidation.prototype.clearErrors = function (form) {
            var formDom = $(form);

            formDom.find(".errorNonfield").empty();
            formDom.find(".error").removeClass("error");
            formDom.find(".errorMessage").remove();
        };

        SimpleValidation.prototype.markErrorsOnFields = function (form, selectors, message) {

            this.clearErrors(form);

            for (i in selectors) {

                if (message != null) {

                    var errorMessage = $('<span class="errorMessage"/>').text(message);
                    var element = $(selectors[i]);
                    element.addClass("error")
                    //if(element.attr("validation-message")!="hidden")
                    element.after(errorMessage);
                }
            }

        };

        window.SimpleValidation = new SimpleValidation();
    })
(window);
(function (window, undefined) {

    function loadFields(parentObject, dataObject, context) {
        $.each(dataObject, function (index, value) {
            if (value == null) {
            } else if (typeof (value) == "object")
                loadFields(parentObject, value, (context == "") ? index : context + "." + index);
            else {
                var fieldName = index;

                if (context != "")
                    fieldName = context + '.' + fieldName;

                var formField = parentObject.find("*[name='" + fieldName + "']");

                if (formField.is('select')) {
                    formField.find('option').attr('selected', false);
                    formField.find('option[value="' + value + '"]').attr('selected', 'selected');
                    formField.val(value);
                   // console.log(value);
                } else if (formField.is('input[type="checkbox"]')) {
                    formField.attr('checked', (value == true || value == "true" || value > 0));
                } else
                    formField.val(value);
            }

        });
    };

    FormPopulator = function (options) {
        this.settings = $.extend({
            parentElement: $(document),
            context: "",
            data: {}
        }, options);
    };

    FormPopulator.prototype.populate = function () {
        loadFields(this.settings.parentElement, this.settings.data, this.settings.context);
    };

    window.FormPopulator = FormPopulator;

})(window);
(function (window, undefined) {

    var monthsShortNames = [
        Messages.get("calendar.month.short.january"),
        Messages.get("calendar.month.short.february"),
        Messages.get("calendar.month.short.march"),
        Messages.get("calendar.month.short.april"),
        Messages.get("calendar.month.short.may"),
        Messages.get("calendar.month.short.june"),
        Messages.get("calendar.month.short.july"),
        Messages.get("calendar.month.short.august"),
        Messages.get("calendar.month.short.september"),
        Messages.get("calendar.month.short.october"),
        Messages.get("calendar.month.short.november"),
        Messages.get("calendar.month.short.december")
    ];

    var monthsNames = [
        Messages.get("calendar.month.january"),
        Messages.get("calendar.month.february"),
        Messages.get("calendar.month.march"),
        Messages.get("calendar.month.april"),
        Messages.get("calendar.month.may"),
        Messages.get("calendar.month.june"),
        Messages.get("calendar.month.july"),
        Messages.get("calendar.month.august"),
        Messages.get("calendar.month.september"),
        Messages.get("calendar.month.october"),
        Messages.get("calendar.month.november"),
        Messages.get("calendar.month.december")
    ];

    DateTimeTool = function () {
    };

    DateTimeTool.prototype.toStrutsDateTime = function (date) {
        return this.momentToStrutsDateTime(moment(date));
    };

    DateTimeTool.prototype.toStrutsDate = function (date) {
        if (date == null || date == '') {
            return null;
        } else if (typeof date === 'string' || date instanceof String) {
            var m = moment(date, DateTimeSettings.getDateFormatFromMoment(), true);

            if (m.isValid()) {
                return this.momentToStrutsDate(m);
            } else {
                return null;
            }
        } else {
            var m = moment(date);

            if(m.isValid()){
                return this.momentToStrutsDate(m);
            }else{
                return null;
            }
        }
    };

    DateTimeTool.prototype.momentToStrutsDate = function (moment) {
        if(moment!=null && moment.isValid()){
            return moment.format("YYYY-MM-DD");
        }else{
            return null;
        }
    };

    DateTimeTool.prototype.momentToStrutsDateTime = function (moment) {
        return moment.format("YYYY-MM-DD[T]HH:mm:ss[Z]") + DateTimeSettings.getTimezone();
    };


    DateTimeTool.prototype.fromStrutsDateTimeToDateDisplayable = function (date) {
        if (date == null) {
            return
        }
        var m = moment(date);
        return m.format(DateTimeSettings.getDateFormatFromMoment());
    };

    DateTimeTool.prototype.fromStrutsDateTimeToDateTimeDisplayable = function (date) {
        if (date == null) {
            return
        }

        var dateWithoutTZ = date.substr(0, date.length - 5);
        return moment(dateWithoutTZ)
            .format(DateTimeSettings.getDateTimeFormatFromMoment());
    };

    DateTimeTool.prototype.fromStrutsLocalDateTimeToTimeDisplayable = function (date) {
        if (date == null) {
            return
        }

        var m = moment(date);
        return m.format(DateTimeSettings.getTimeFormatFromMoment());
    };

    DateTimeTool.prototype.fromStrutsDateToMoment = function (date) {
        var m = moment(date, "YYYY-MM-DD", true);
        return m;
    };

    DateTimeTool.prototype.fromStrutsDateToDateDisplayable = function (date) {
        var m = moment(date, "YYYY-MM-DD", true);
        return m.format(DateTimeSettings.getDateFormatFromMoment());
    };

    DateTimeTool.prototype.fromStrutsDateTimeToMoment = function (date) {
        var m = moment(date, "YYYY-MM-DD[T]HH:mm:ss", true);
        return m;
    };

    DateTimeTool.prototype.fromStrutsTimeToDisplayable = function (time) {
        if (time == null) {
            return
        }

        return moment("2000-01-01T" + time)
            .format(DateTimeSettings.getTimeFormatFromMoment());
    }

    DateTimeTool.prototype.msToDisplayableDuration = function (ms) {
        var seconds = Math.abs(ms / 1000);
        return DateTimeTool.secToDisplayableDuration(seconds);
    }


    DateTimeTool.prototype.secToDisplayableDuration = function (sec) {
        var h, m, s, result = '';
        var seconds = sec;

        h = Math.floor(seconds / 3600);
        seconds -= h * 3600;
        result = h > 0 ? h + 'h' : '';

        m = Math.floor(seconds / 60);
        seconds -= m * 60;
        if (m > 0) {
            result += result == '' ? m + 'm' : ' ' + m + 'm';
        }

        if (h==0 && m == 0 && seconds > 0 && seconds < 60) {
            s = Math.round(seconds);
            result += result == '' ? s + 's' : ' ' + s + 's';
        }

        if (result == '') {
            result = "0m"
        }

        return result;
    }

    DateTimeTool.prototype.msToDisplayableStoperDuration = function (ms) {
        var seconds = Math.abs(ms / 1000);
        return this.secToDisplayableStoperDuration(seconds);
    }

    DateTimeTool.prototype.secToDisplayableStoperDuration = function (sec) {
        var seconds = sec;
        var h, m, result = '';

        h = Math.floor(seconds / 3600);
        seconds -= h * 3600;

        m = Math.floor(seconds / 60);
        seconds -= m * 60;
        seconds = Math.round(seconds);

        result = h >= 10 ? h : "0" + h;
        result += ":";
        result += m >= 10 ? m : "0" + m;
        result += ":";
        result += seconds >= 10 ? seconds : "0" + seconds;

        return result;
    }

    DateTimeTool.prototype.getDayShortName = function (weekdayNumber) {
        if (weekdayNumber instanceof moment) {
            return Messages.get("calendar.day.short." + weekdayNumber.isoWeekday());
        } else {
            return Messages.get("calendar.day.short." + weekdayNumber);
        }
    }

    DateTimeTool.prototype.getMonthShortName = function (month) {
        if (month instanceof moment) {
            return monthsShortNames[month.month()];
        } else {
            return monthsShortNames[month - 1];
        }
    }

    DateTimeTool.prototype.getMonthName = function (month) {
        return monthsNames[month - 1];
    }

    DateTimeTool.prototype.getWeekDaysArray = function () {
        var days = [];
        var firstDay = parseInt(DateTimeSettings.getFirstDayOfWeek());

        if (firstDay == 0)
            firstDay = 7;

        var lastDay = 7;

        for (var day = firstDay; day <= lastDay; day++) {
            days.push(day);
        }

        for (var day = 1; day < firstDay; day++) {
            days.push(day);
        }

        return days;
    }

    DateTimeTool.prototype.getWeekDaysShortNamesArray = function () {
        var days = this.getWeekDaysArray();
        var names = [];

        for (var i = 0; i < days.length; i++) {
            var day = days[i];
            names.push(this.getDayShortName(day));
        }

        return names;
    }

    DateTimeTool.prototype.whichDayOfWeek = function (day) {
        var days = this.getWeekDaysArray();

        return days.indexOf(day) + 1;
    }

    DateTimeTool.prototype.validTimeFormat = function (timeSting) {
        var pattern = "^(0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$";
        if (!timeSting.match(pattern))
            return false;
        else return true;

    }

    DateTimeTool.prototype.momentDurationToDisplayable = function (duration) {
        var text = "";

        if (duration.hours() < 10) {
            text += "0" + duration.hours();
        } else {
            text += duration.hours();
        }

        text += ":";

        if (duration.minutes() < 10) {
            text += "0" + duration.minutes();
        } else {
            text += duration.minutes();
        }

        return text;
    }


    DateTimeTool.prototype.getLastWeekStart = function () {
        var now = moment();
        var start = moment().isoWeekday(DateTimeSettings.getFirstDayOfWeek()).subtract('week', 1);
        if (now.diff(start, 'days') < 7) {
            start.subtract('week', 1);
        }
        return start;
    }


    DateTimeTool.prototype.getLastWeekEnd = function () {
        var now = moment();
        var end = moment().isoWeekday(DateTimeSettings.getFirstDayOfWeek()).subtract('week', 1).add('days', 7);
        if (now.diff(end, 'days') < 0) {
            end.subtract('week', 1);
        }
        return end;
    }


    DateTimeTool.prototype.formatDate = function (date) {
        var m = moment(date);
        return m.format(DateTimeSettings.getDateFormatFromMoment());
    }


    window.DateTimeTool = new DateTimeTool();
})(window);


(function (window, undefined) {

    MomentPeriod = function (from, to) {
        this.from = moment(from);
        this.to = moment(to);
    };

    MomentPeriod.prototype.overlaps = function (otherPeriod) {
        return ( (this.getTo().isAfter(otherPeriod.getFrom()) ) && (this.getFrom().isBefore(otherPeriod.getTo())) );
    };

    MomentPeriod.prototype.isWholeIn = function (otherPeriod) {
        return (
            (this.getTo().isBefore(otherPeriod.getTo()) || this.getTo().isSame(otherPeriod.getTo()))
                &&
                (this.getFrom().isAfter(otherPeriod.getFrom()) || this.getFrom().isSame(otherPeriod.getFrom()))
            );
    };

    MomentPeriod.prototype.containsMoment = function (m) {

        return(this.getFrom().isSame(m)
            ||
            this.getTo().isSame(m)
            ||
            ((this.getTo().isAfter(m) ) && (this.getFrom().isBefore(m)))
            )
    };

    MomentPeriod.prototype.daysCount = function () {
        return Math.abs(this.from.diff(this.to, 'days')) + 1;
    }

    MomentPeriod.prototype.getFrom = function () {
        return moment(this.from);
    };

    MomentPeriod.prototype.getTo = function () {
        return moment(this.to);
    }

    window.MomentPeriod = MomentPeriod;
})(window);
(function (window, undefined) {

    MethodDelayer = function () {
        this.delayId = null
    };

    MethodDelayer.prototype.delayed = function (f, delay) {
        var _this = this;

        if(this.delayId!=null){
            clearTimeout(this.delayId);
            this.delayId = null;
        }

        this.delayId = setTimeout(function(){
            f();
            _this.delayId = null;
        }, delay);
    };

    MethodDelayer.prototype.immediate = function (f) {
        if(this.delayId!=null){
            clearTimeout(this.delayId);
            this.delayId = null;
        }

        f();
    };

    MethodDelayer.prototype.clear = function () {
        if(this.delayId!=null){
            clearTimeout(this.delayId);
            this.delayId = null;
        }
    }

    window.MethodDelayer = MethodDelayer;
})(window);


(function (window, undefined) {

    DurationObject = function (seconds) {
        this.seconds = seconds;
        this.minutes = this.seconds / 60;
        this.hours = this.minutes / 60;
    };

    DurationObject.prototype.getSeconds = function () {
        return this.seconds;
    };

    DurationObject.prototype.getMinutes = function () {
        return this.minutes;
    };

    DurationObject.prototype.getHours = function () {
        return this.hours;
    };

    function parseHours(hoursString) {
        var seconds = 0;
        var hours = parseFloat(hoursString);

        if (hours == NaN || hours == null) {
            seconds = 0;
        } else {
            seconds = hours * 60 * 60;
        }

        return new DurationObject(seconds);
    };

    function parseSeconds(secondsString) {
        var seconds = parseFloat(secondsString);

        if (seconds == NaN || seconds == null) {
            seconds = 0;
        }

        return new DurationObject(seconds);
    }

    window.Duration = {
        parseHours: parseHours,
        parseSeconds: parseSeconds
    };
})(window);

//TODO do weryfikacji....
(function (window, undefined) {
    //constants
    var LOADING = 1;
    var LOADED = 2;
    var ERROR = 3;

    var loadingId = 1;
    var loadings = {};

    var loadedModules = {};

    function loadScripts(scriptsUrls, options) {
        var thisLoadingId = loadingId++;

        $(document).one(thisLoadingId + ".moduleLoaded.Dependency",
            function (event, loadingResult) {
                if (loadingResult) {
                    if (typeof options.onLoaded == 'function')
                        options.onLoaded();
                } else {
                    if (typeof options.onError == 'function')
                        options.onError();
                }
            });

        loadings[thisLoadingId] = {};
        for (var i = 0; i < scriptsUrls.length; i++) {
            loadings[thisLoadingId][scriptsUrls[i]] = LOADING;
        }

        for (var i = 0; i < scriptsUrls.length; i++) {
            var url = scriptsUrls[i];
            downloadModule(thisLoadingId, url);
        }

        return thisLoadingId;
    }


    //TODO rozważyć: jeśli jest dataTYpe "script" to jquery robi jakiś eval() i już skrypt jest załadowany, a success(data) i tak jest null, jeśli damy text to samemu trzeba go wklieć
    //TODO ... zabezpieczyć przed wykonaniem tego samego skryptu w różnych oknach
    function downloadModule(loadingId, moduleUrl) {
        $.ajax({
            url: moduleUrl,
            dataType: "script",
            cache: false,
            crossDomain: false,

            error: function () {
                updateLoadingState(loadingId, moduleUrl, ERROR);
            },

            success: function (data) {
                if (navigator.appName == 'Microsoft Internet Explorer') {
                    $('<script type="text/javascript" />')
                        .attr("src", moduleUrl)
                        .attr("id", "modulescript_" + moduleName)
                        .appendTo("#mainWindow");
                } else {
                    $('<script type="text/javascript" />')
                        .attr("id", "modulescript_" + moduleName)
                        .text(data)
                        .appendTo("#mainWindow");
                }

                updateLoadingState(loadingId, moduleUrl, LOADED);
            }
        });
    }

    function updateLoadingState(loadingId, moduleUrl, changeStateTo) {
        loadings[loadingId][moduleUrl] = changeStateTo;

        var isError = false;
        var isLoading = false;

        for (module in loadings[loadingId]) {
            if (loadings[loadingId][module] == LOADING)
                isLoading = true;
            else if (loadings[loadingId][module] == ERROR)
                isError = true;
        }
        ;

        if (isLoading == false) {
            if (isError)
                informAboutLoadingError(loadingId);
            else
                informAboutLoadingSuccess(loadingId);

            delete loadings[loadingId];
        }
        ;
    }

    function informAboutLoadingError(loadingId) {
        $(document).triggerHandler(loadingId + ".moduleLoaded.Dependency", [ false ]);
    }

    function informAboutLoadingSuccess(loadingId) {
        $(document).triggerHandler(loadingId + ".moduleLoaded.Dependency", [ true ]);
    }

    window.Dependency = {
        loadScripts: loadScripts
    };
})(window);
(function (window, undefined) {
    //constants
    var LOADING = 1;
    var LOADED = 2;
    var ERROR = 3;

    function downloadContent(url, versionHash) {
        var loadUrl = url;

        if(versionHash!=null){
            loadUrl = loadUrl + "?v="+versionHash;
        }

        return $.ajax({
            url: loadUrl,
            dataType: "text",
            cache: true,
            crossDomain: false
        });
    }

    Dependency3 = function (versionHash) {
        this.loadedModules = {};
        this.loadIncrementation = 0;
        this.loadedStylesheets = {};

        this.versionHash = null;

        if(versionHash!=null && versionHash!=""){
            this.versionHash = versionHash;
        }
    }

    Dependency3.prototype.loadScript = function (urlParam, type) {
        var _this = this;
        var url = URL2.get(urlParam);

        if (type == undefined)
            type = "text/javascript";

        if (url in this.loadedModules) {
            var loadingDescriptor = this.loadedModules[url];
            return loadingDescriptor.promise;
        } else {
            var loadingScript = jQuery.Deferred();
            var generatedId = "loadedSCRIPT_" + Math.floor((Math.random() * 10000000) + 1) + "_" + (++this.loadIncrementation);
            var loadDescriptor = {
                id: generatedId,
                type: type,
                status: LOADING,
                promise: loadingScript
            };

            _this.loadedModules[url] = loadDescriptor;


            downloadContent(url, this.versionHash)
                .done(function (loadedContent) {
                    if (navigator.appName == 'Microsoft Internet Explorer') {
                        $('<script />')
                            .attr("type", type)
                            .attr("src", url)
                            .attr("id", generatedId)
                            .appendTo("body");
                    } else {
                        $('<script />')
                            .attr("type", type)
                            .text(loadedContent)
                            .attr("id", generatedId)
                            .appendTo("body");
                    }

                    if (loadedContent.indexOf("//module:") === 0) {
                        var loadModuleMethod = loadedContent.substring("//module:".length, loadedContent.indexOf("\n"));
                        var subLoadModulePromise = eval(loadModuleMethod + "()");

                        subLoadModulePromise
                            .done(function () {
                                loadDescriptor.status = LOADED;
                                loadingScript.resolve(loadDescriptor);
                            })
                            .fail(function () {
                                loadDescriptor.status = ERROR;
                                loadingScript.reject(loadDescriptor);
                            });
                    } else {
                        loadDescriptor.status = LOADED;
                        loadingScript.resolve(loadDescriptor);
                    }
                })
                .fail(function () {
                    loadDescriptor.status = ERROR;
                    loadingScript.reject(loadDescriptor);
                });

            return loadingScript.promise();
        }
    }


    Dependency3.prototype.loadCss = function (urlParam) {
        var _this = this;
        var url = URL2.get(urlParam);

        if (url in this.loadedStylesheets) {
            var loadingDescriptor = this.loadedStylesheets[url];
            return loadingDescriptor.promise;
        } else {
            var loadingStylesheet = jQuery.Deferred();
            var generatedId = "loadedSTYLESHEET_" + Math.floor((Math.random() * 10000000) + 1) + "_" + (++this.loadIncrementation);
            var loadDescriptor = {
                id: generatedId,
                status: LOADING,
                promise: loadingStylesheet
            };

            _this.loadedStylesheets[url] = loadDescriptor;

            downloadContent(url, this.versionHash)
                .done(function (loadedContent) {

                    $('<style />')
                        .attr("type", "text/css")
                        .attr("id", generatedId)
                        .text(loadedContent)
                        .appendTo("head");

                    loadDescriptor.status = LOADED;
                    loadingStylesheet.resolve(loadDescriptor);
                })
                .fail(function () {
                    loadDescriptor.status = ERROR;
                    loadingStylesheet.reject(loadDescriptor);
                });

            return loadingStylesheet.promise();
        }
    }


    var versionHash = $("meta[name='version-hash']").attr("content");
    window.Dependency3 = new Dependency3(versionHash);
})(window);
(function (window, undefined) {

    Ajax = function () {
    }

    Ajax.prototype.post = function (settings) {
        settings.method = 'POST';
        return this.request(settings);
    }

    Ajax.prototype.get = function (settings) {
        settings.method = 'GET';
        return this.request(settings);
    }

    Ajax.prototype.request = function (settings) {
        var userOptions = prepareOptions(settings);

        var decoratedOptions = $.extend({}, userOptions);

        decoratedOptions.error = function (xhr, textStatus, errorThrown) {
            if (textStatus == "abort") {
                return;
            } else if (xhr.status == 0 || textStatus == "timeout") {
                if(userOptions.triggerSystemErrors){
                    $(window.eventBus).trigger('IO.error.serverNotResponds', {url: settings.url});
                }

                userOptions.alwaysAfterError(xhr, textStatus, errorThrown);
            } else if (xhr.status == 401) {
                if(userOptions.triggerSystemErrors){
                    $(window.eventBus).trigger('Security.unauthorized', {url: settings.url});
                }

                userOptions.alwaysAfterError(xhr, textStatus, errorThrown);
            } else if (xhr.status == 403) {
                if(userOptions.triggerSystemErrors){
                    $(window.eventBus).trigger('Security.accessDenied', {url: settings.url});
                }

                userOptions.alwaysAfterError(xhr, textStatus, errorThrown);
            } else if (!SimpleValidation.isValid(xhr)) {
                if (userOptions['validation'] != null) {
                    userOptions.validation(xhr);
                } else {
                    userOptions.error(xhr, textStatus, errorThrown, errorObject, errorMessage);
                }
            } else {
                var errorObject = parseErrorObject(xhr);
                var errorMessage = parseErrorMessage(errorObject);
                var errorPayload = parseErrorPayload(errorObject);

                if (errorObject == null) {
                    userOptions.error(xhr, textStatus, errorThrown, errorObject, errorMessage, errorPayload);
                } else {
                    var errorCode = errorObject.errorCode;

                    if (errorCode != null) {
                        if (userOptions["error_" + errorCode] != null) {
                            userOptions["error_" + errorCode](errorCode, errorMessage, errorPayload);
                        } else {
                            userOptions.error(xhr, textStatus, errorThrown, errorObject, errorMessage, errorPayload);
                        }
                    } else {
                        userOptions.error(xhr, textStatus, errorThrown, errorObject, errorMessage, errorPayload);
                    }
                }

                userOptions.alwaysAfterError(xhr, textStatus, errorThrown, errorObject, errorMessage, errorPayload);
            }
        }

        return $.ajax(decoratedOptions);
    }

    function prepareOptions(settings) {
        var preparedSettings = $.extend({
                url: "",
                data: {},
                accept: 'application/json',
                dataType: 'json',
                cache: false,
                method: 'POST',
                triggerSystemErrors: true,

                beforeSend: function () {
                },

                complete: function () {
                },

                success: function (response, testStatus, xhr) {
                },

                alwaysAfterError: function (xhr, textStatus, errorThrown, errorObject, errorMessage) {
                },

                error: function (xhr, textStatus, errorThrown, errorObject, errorMessage) {
                    $(window.eventBus).trigger('FormSender.error.unhandledError', errorMessage);
                }
            },

            settings
        );

        if(preparedSettings.headers == null){
            preparedSettings.headers = {};
        }

        var csrfToken = $("meta[name='_csrf']").attr("content");
        var csrfHeader = $("meta[name='_csrf_header']").attr("content");
        preparedSettings.headers[csrfHeader] = csrfToken;

        return preparedSettings;
    }

    function parseErrorObject(xhr) {
        try {
            return jQuery.parseJSON(xhr.responseText);
        } catch (error) {
            return null;
        }
    }

    function parseErrorMessage(errorObject) {
        try {
            if (errorObject == null) {
                return null;
            } else {
                if (errorObject["message"] != null) {
                    return errorObject["message"];
                } else {
                    return null;
                }
            }
        } catch (err) {
            return null;
        }
    }

    function parseErrorPayload(errorObject) {
        try {
            if (errorObject == null) {
                return null;
            } else {
                if (errorObject["payload"] != null) {
                    return errorObject["payload"];
                } else {
                    return null;
                }
            }
        } catch (err) {
            return null;
        }
    }

    window.Ajax = new Ajax();
})(window);
(function (window, undefined) {
    var requestsArray = {};

    FormSender = function () {
    };

    FormSender.prototype.post = function (orginalSettings) {
        var latestWaitingId = LockingHourglass.show(Messages.get("common.wait"));

        var settings = $.extend({
            dataType: "json",
            accept: "application/json",
        }, orginalSettings);

        settings.complete = function () {
            if (orginalSettings['complete'] != null)
                orginalSettings.complete();

            doAfterPerformedAjaxRequest(latestWaitingId, settings);
        }

        settings.validation = function (xhr) {
            if (settings.formSelector != null) {
                SimpleValidation.markErrorsOnForm(settings.formSelector, xhr);
            }

            if(_.isFunction(orginalSettings.validation)){
                orginalSettings.validation(xhr);
            }
        }

        doBeforePerformAjaxRequest(settings);

        var post = Ajax.post(settings);
        requestsArray[settings.url] = post;

        return post;
    };

    function doBeforePerformAjaxRequest(options) {
        if (options.sendButtonSelector != null) {
            $(options.sendButtonSelector).attr("disabled", "disabled");
        }

        if (options.formSelector != null) {
            SimpleValidation.clearErrors(options.formSelector);
        }

        if (requestsArray[options.url] != null) {
            requestsArray[options.url].abort();
        }
    }

    function doAfterPerformedAjaxRequest(latestWaitingId, options) {
        if (options.sendButtonSelector != null) {
            $(options.sendButtonSelector).removeAttr("disabled");
        }

        LockingHourglass.hide(latestWaitingId);

        delete requestsArray[options.url];
    }

    window.FormSender = new FormSender();
})(window);
(function (window, undefined) {

    HelperTool = function () {
    };

    HelperTool.prototype.sortByPropertyDesc = function (property) {
        'use strict';
        return function (a, b) {
            var sortStatus = 0;
            if (a[property] > b[property]) {
                sortStatus = -1;
            } else if (a[property] < b[property]) {
                sortStatus = 1;
            }

            return sortStatus;
        };
    };

    HelperTool.prototype.sortByPropertyAsc = function (property) {
        'use strict';
        return function (a, b) {
            var sortStatus = 0;
            if (a[property] < b[property]) {
                sortStatus = -1;
            } else if (a[property] > b[property]) {
                sortStatus = 1;
            }

            return sortStatus;
        };
    };

    window.HelperTool = new HelperTool();
})(window);

(function (window, undefined) {
    LocalDataStorage = function () {
    };

    LocalDataStorage.prototype.get = function (name) {
        try {
            if (typeof(Storage) !== "undefined")
                return localStorage.getItem(name);
            else
                return null;
        } catch (e) {
            return null;
        }
    };

    LocalDataStorage.prototype.getAndClear = function (name) {
        try {
            if (typeof(Storage) !== "undefined") {
                var value = localStorage.getItem(name);
                localStorage.removeItem(name);
                return value;
            } else
                return null;
        } catch (e) {
            return null;
        }
    };

    LocalDataStorage.prototype.clear = function (name) {
        try {
            if (typeof(Storage) !== "undefined")
                return localStorage.removeItem(name);
        } catch (e) {

        }
    };

    LocalDataStorage.prototype.save = function (name, value) {
        try {
            if (typeof(Storage) !== "undefined")
                return localStorage.setItem(name, value);
        } catch (e) {

        }
    };

    window.LocalDataStorage = new LocalDataStorage();
})(window);
(function (window, undefined) {
    function verify(v){
        Ajax.post({
            url: URL2.get("/webapi/version/verify/" + v),
            error: function(){
                //swallow error
            },
            triggerSystemErrors: false
        }).done(function(result){
            if(result == false){
                location.reload();
            }
        });
    }

    VersionVerifier = function (v) {
        this.version = v;
        this.delay = 5 * 60 * 1000;
    }

    VersionVerifier.prototype.start = function () {
        var _this = this;

        setInterval(function(){
            verify(_this.version);
        }, this.delay);
    }




    window.VersionVerifier = VersionVerifier;
})(window);
(function (window, undefined) {

    FileUploader = function (options) {
        this.settings = $.extend({
            buttonId: "",
            selfDestruction: true,
            category: "",
            handleErrors: false,
            errorTitle: "files.upload.error.title",

            onFileAdded: function (fileIdentifier, fileName) {
            },
            onFileProgress: function (fileIdentifier, progress) {
            },
            onFileSuccess: function (fileIdentifier, uploadedTempId, uploadedAccessId) {
            },
            onFileError: function (fileIdentifier, errorResponse) {
            }

        }, options);

        this.csrfToken = null;
        this.csrfHeader = null;
        this.additionalUploadHeaders = {};
        this.resumable = null;

        this.uploadingFilesCount = 0;
        this.uploading = $.Deferred().resolve();
    };

    FileUploader.prototype.init = function () {
        var _this = this;
        init_fileUploader_csrf(_this);
        _this.additionalUploadHeaders["Accept"] = "application/json";

        this.resumable = new Resumable({
            target: URL2.get('/files/uploadMultipartChunk.do') + "?category=" + _this.settings.category,
            testChunks: false,
            fileParameterName: "file",
            generateUniqueIdentifier: function () {
                return Math.round(Math.random() * 1000000000) + "_" + Math.round(Math.random() * 10000000000) + "_" + moment().format('YYYY_MM_DD');
            },
            permamentErrors: [404, 415, 500, 501, 401, 409],
            maxChunkRetries: 4,
            headers: _this.additionalUploadHeaders
        });

        if (_this.settings.buttonId != null && _this.settings.buttonId != "") {
            this.resumable.assignBrowse(document.getElementById(_this.settings.buttonId));
        }

        this.resumable.on('fileAdded', function (file) {
            _this.uploadingFilesCount = _this.uploadingFilesCount + 1;

            if (_this.uploading != "pending") {
                _this.uploading = $.Deferred();
            }

            _this.settings.onFileAdded(file.uniqueIdentifier, file.fileName);
            _this.resumable.upload();
        });

        this.resumable.on('fileProgress', function (file) {
            var progress = Math.round(file.progress(true) * 100);
            _this.settings.onFileProgress(file.uniqueIdentifier, progress);
        });

        this.resumable.on('fileSuccess', function (file, response) {
            _this.resumable.removeFile(file);

            var uploadedResponse = JSON.parse(response);
            var uploadedTempFileId = uploadedResponse.fileId;
            var uploadedTempFileAccessId = uploadedResponse.accessId;

            _this.settings.onFileSuccess(file.uniqueIdentifier, uploadedTempFileId, uploadedTempFileAccessId);

            _this.uploadingFilesCount = _this.uploadingFilesCount - 1;
            if (_this.uploadingFilesCount == 0) {
                _this.uploading.resolve();
            }
        });

        this.resumable.on('error', function (msg, file) {
            var errorResponse = JSON.parse(msg);

            if(_this.settings.handleErrors){
                var messageContent = extractErrorMessageContent(errorResponse);
                displayFileAttachmentErrorDialog.call(_this, messageContent);
            }

            _this.settings.onFileError(file.uniqueIdentifier, errorResponse);

            _this.uploadingFilesCount = _this.uploadingFilesCount - 1;
            if (_this.uploadingFilesCount == 0) {
                _this.uploading.resolve();
            }
        });


        if (this.settings.selfDestruction) {
            $(window.eventBus).one('Location.unloading', function () {
                _this.resumable.cancel();
            });
        }

    }

    FileUploader.prototype.assignButtonId = function (buttonId) {
        this.resumable.assignBrowse(document.getElementById(buttonId));
    }

    FileUploader.prototype.uploadingProcess = function () {
        return this.uploading;
    }

    function init_fileUploader_csrf(_this) {
        _this.csrfToken = $("meta[name='_csrf']").attr("content");
        _this.csrfHeader = $("meta[name='_csrf_header']").attr("content");
        _this.additionalUploadHeaders[_this.csrfHeader] = _this.csrfToken;
    }



    function displayFileAttachmentErrorDialog(errorMessage) {
        var _this = this;

        var dialog = new MessageDialog({
            title: Messages.get(_this.settings.errorTitle),
            message: errorMessage,
            type: "error",
            selfDestruction: true,
            buttons: [
                {label: Messages.get("common.ok"), value: true},
            ]
        });

        dialog.open();

    }


    function extractErrorMessageContent(errorObject) {
        if (errorObject != null) {
            if (errorObject.nonfields != null && errorObject.nonfields.length > 0) {
                for (var i = 0; i < errorObject.nonfields.length; i++) {
                    console.info(4);
                    return errorObject.nonfields[i];
                }

                return null;
            } else if (errorObject.fields != null) {
                for (fieldName in errorObject.fields) {
                    return errorObject.fields[fieldName];
                }

                return null;
            } else if (errorObject.message != null) {
                return errorObject.message;
            } else {
                return null;
            }
        } else {
            return null;
        }
    }

})(window);




(function (window) {
    var map = {
        'Backspace': 8,
        'Tab': 9,
        'Enter': 13,
        'Shift': 16,
        'Ctrl': 17,
        'Alt': 18,
        'Esc': 27,
        'Space': 32,
        'Page_Up': 33,
        'Page_Down': 34,
        'Left': 37,
        'Right': 39,
        'Up': 38,
        'Down': 40,
        'Delete': 46
    };

    KeyCodesMap = function () {

    };

    KeyCodesMap.prototype.getKeys = function (keyName) {
        return map[keyName];
    };

    window.KeyCodesMap = new KeyCodesMap();
})(window);
(function (window, undefined) {
    var lastFocusElement;

    function init() {

    }

    function fullfillMesageDialogTexts(dialog) {
        $(dialog.dialogContent).find('.MessageDialogTitle').text(
            dialog.settings.title);

        if(dialog.settings.messageHtml){
            $(dialog.dialogContent).find('.MessageDialogContent').html(dialog.settings.message);
        }else{
            $(dialog.dialogContent).find('.MessageDialogContent').text(dialog.settings.message);
        }

    }

    function keydownEscHandler(e) {
        if (e.which == KeyCodesMap.getKeys('Esc')) {
            e.data.close();
        }
    }

    function keydownTabHandler(e) {
        if (e.which == KeyCodesMap.getKeys('Tab')) {
            var focusableElements = $(e.data.dialogContent).find(
                $(".focusable"));
            var focusedElement = $(focusableElements).filter(
                $(document.activeElement));

            if ($(focusedElement).size() == 0) {
                $(focusableElements).first().focus();
            }
            if ($(focusedElement)[0] === $(focusableElements).last()[0])
                $(focusableElements).first().focus();
            else
                $(focusedElement).next().focus();
            e.stopImmediatePropagation();
            e.preventDefault();
        }
    }

    MessageDialog = function (options) {
        this.settings = $.extend({
            title: "",
            message: "",
            messageHtml: true,
            type: "info",
            selfDestruction: false,
            autoClosing: true,

            buttons: [
                {
                    label: "OK",
                    value: true
                }
            ],

            callback: function (value) {
            },

            additionalContentSelector: null

        }, options);
        this.lastFocusedElement = null;
        this.overlayInfo = {};
        var _thisDialog = this;

        this.buttonsContener = $('<div class="MessageDialogButtonContainer"/>');

        for (var i = 0; i < this.settings.buttons.length; i++) {
            var value = _thisDialog.settings.buttons[i].value;

            _thisDialog.settings.buttons[i].dom = $(
                '<button class="focusable">'
                    + this.settings.buttons[i].label + '</button>')
                .appendTo(_thisDialog.buttonsContener).data('value', value).click(

                function () {
                    _thisDialog.settings.callback($(this).data(
                        'value'));
                    if (_thisDialog.settings.autoClosing) {
                        _thisDialog.close();
                    }
                });
        }

        var contenerForAdditionalContent = $('<div class="MessageDialogContenerForContent" />');


        var dialogIcon = $('<div class="MessageDialogIcon iconWhite" />');

        switch (this.settings.type) {
            case 'info':
                dialogIcon.addClass('iconInfo2');
                break;

            case 'error':
                dialogIcon.addClass('iconError');
                break;

            case 'question':
                dialogIcon.addClass('iconQuestion');
                break;

            case 'warning':
                dialogIcon.addClass('iconWarning');
                break;

            case 'connection_error':
                dialogIcon.addClass('iconNoWifi');
                break;

            case 'access_denied':
                dialogIcon.addClass('iconAccessDenied');
                break;

            case 'comment':
                dialogIcon.addClass('iconComment');
                break;
        }


        var dialogContentInner = $('<div class="MessageDialogInner"/>')
            .append('<div class="MessageDialogTitle" />')
            .append(dialogIcon)
            .append('<div class="MessageDialogContent" />')
            .append(contenerForAdditionalContent)
            .append(_thisDialog.buttonsContener);

        this.dialogContent = $(
            '<div class="MessageDialog animated" style="display: none;"/>')
            .addClass(this.settings.type).append(dialogContentInner)
            .appendTo('body');


        fullfillMesageDialogTexts(this);

        if (_thisDialog.settings.additionalContentSelector) {
            $(_thisDialog.settings.additionalContentSelector)
                .appendTo(contenerForAdditionalContent)
                .show();
        }
    }

    MessageDialog.prototype.destroy = function () {
        this.buttonsContener.find("button").unbind('click');
        $(this.dialogContent).remove();
    }

    MessageDialog.prototype.open = function (options) {
        this.settings = $.extend(this.settings, options);
        fullfillMesageDialogTexts(this);
        var _thisDialog = this;
        this.lastFocusedElement = document.activeElement;
        // $._data($(document)[0],"events"); // EVENTY PODPIETE AKTUALNIE DO
        // DOCUMENTU BEDZIE TRZEBA JE WRZUCAC NA STOS A POTEM POBIERAĆ

        $(document).on('keydown', _thisDialog, keydownEscHandler);
        $(document).on('keydown', _thisDialog, keydownTabHandler);
        if (_thisDialog.settings.selfDestruction) {
            _thisDialog.buttonsContener.find("button").bind('click');
        }
        this.overlayInfo = Overlay.show();
        $(this.dialogContent).css('z-index', this.overlayInfo.zindex + 1);

        $(this.dialogContent)
            .addClass("preInAnimation")
            .css("display", '');

        Animator.animateIn(this.dialogContent, 'fadeFromTop');

        if (this.settings.buttons[0] != null)
            this.settings.buttons[0].dom.focus();
    }

    MessageDialog.prototype.close = function () {
        var _this = this;

        this.overlayInfo = Overlay.hide();
        this.dialogContent.find($(":focus")).blur();
        Animator.animateOut(this.dialogContent, 'fadeFromTop');

        $(document).off('keydown', keydownEscHandler);
        $(document).off('keydown', keydownTabHandler);

        if (this.lastFocusedElement != null) {
            this.lastFocusedElement.focus();
        }

        if (this.settings.selfDestruction) {
            setTimeout(function () {
                _this.destroy();
            }, 300);
        }

    }

    MessageDialog.prototype.getButtons = function () {
        return this.buttonsContener.find("button");
    }

    window.MessageDialog = MessageDialog;
})(window);

(function (window, undefined) {

    ModalDialog = function (contentSelector, options) {
        var _this = this;

        this.settings = $.extend({
            title: "",
            selfDestruction: false,
            autoClosing: true,

            height: "60%",
            width: "600",

            buttons: [
                {
                    label: Messages.get("common.ok"),
                    value: true
                }
            ],

            callback: function (value) {
            }
        }, options);


        this.lastFocusedElement = null;
        this.overlayInfo = {};
        this.showTitle = false;
        this.showButtons = false;

        this.dialogContainer = $('<div class="ModalDialog animated" style="display: none;"/>').appendTo("body");

        this.dialogTitleContainer =
            $('<div class="ModalDialog-title lockSelecting" />')
                .text(this.settings.title)
                .appendTo(this.dialogContainer);

        this.contentContainer = $('<div class="ModalDialog-contentContainer"/>')
            .append($(contentSelector))
            .appendTo(this.dialogContainer);

        $(contentSelector).show();

        this.buttonsContainer = $('<div class="ModalDialog-buttonsContainer lockSelecting"/>').appendTo(this.dialogContainer);

        for (var i = 0; i < this.settings.buttons.length; i++) {
            var value = _this.settings.buttons[i].value;

            _this.settings.buttons[i].dom = $('<button>' + this.settings.buttons[i].label + '</button>')
                .appendTo(_this.buttonsContainer)
                .data('value', value)
                .click(function () {
                    _this.settings.callback($(this).data('value'));
                    if (_this.settings.autoClosing) {
                        _this.close();
                    }
                });
        }
    }

    ModalDialog.prototype.destroy = function () {
        $(this.dialogContainer).remove();
    }


    ModalDialog.prototype.open = function () {
        var _this = this;

        showOrHideTitleAndButtons.apply(this);
        resizeContainer.apply(this);
        adjustSizeOfContent.apply(this);

        this.lastFocusedElement = document.activeElement;
        $(document).on('keydown', _this, keydownEscHandler);
        $(document).on('keydown', _this, keydownTabHandler);

        this.overlayInfo = Overlay.show();

        $(this.dialogContainer)
            .css('z-index', this.overlayInfo.zindex + 1)
            .addClass("preInAnimation")
            .css("display", '');

        this.updateZIndex();

        Animator.animateIn(this.dialogContainer, 'fadeFromTop');

        setTimeout(function () {
            focusFirstElement.apply(_this);
        }, 200);
    }


    ModalDialog.prototype.close = function () {
        var _this = this;

        this.overlayInfo = Overlay.hide();
        this.dialogContainer.find($(":focus")).blur();
        Animator.animateOut(this.dialogContainer, 'fadeFromTop');

        $(document).off('keydown', keydownEscHandler);
        $(document).off('keydown', keydownTabHandler);

        if (this.lastFocusedElement != null) {
            this.lastFocusedElement.focus();
        }

        if (this.settings.selfDestruction) {
            setTimeout(function () {
                _this.destroy();
            }, 300);
        }
    }

    ModalDialog.prototype.getZIndex = function () {
        return this.overlayInfo.zindex;
    }

    ModalDialog.prototype.updateZIndex = function () {
        var zIndex = this.overlayInfo.zindex + 1;

        $(this.dialogContainer)
            .find(".select2-drop-mask, .select2-drop, .select2-search")
            .each(function () {
                var elementZIndex = $(this).css("z-index");

                if (elementZIndex < zIndex) {
                    $(this).css("z-index", elementZIndex + zIndex);
                }
            });
    }

    function resizeContainer() {
        var windowHeight = $(window).height();
        var maxHeight = windowHeight - 100;
        var desiredHeight = this.settings.height;
        var desiredHeightPixels = 0;

        if (_.isString(desiredHeight) && desiredHeight.indexOf("%") > -1) {
            desiredHeightPixels = (parseInt(desiredHeight) / 100.0) * windowHeight;
        } else {
            desiredHeightPixels = parseInt(desiredHeight);
        }

        var windowWidth = $(window).width();
        var maxWidth = windowWidth - 100;
        var desiredWidth = this.settings.width;
        var desiredWidthPixels = 0;

        if (_.isString(desiredWidth) && desiredWidth.indexOf("%") > -1) {
            desiredWidthPixels = (parseInt(desiredWidth) / 100.0) * windowWidth;
        } else {
            desiredWidthPixels = parseInt(desiredWidth);
        }

        var heightToSet = Math.min(desiredHeightPixels, maxHeight);
        var widthToSet = Math.min(desiredWidthPixels, maxWidth);

        this.dialogContainer
            .css("height", heightToSet + "px")
            .css("width", widthToSet + "px")
            .css("top", ((windowHeight - heightToSet) / 2.0) + "px")
            .css("left", ((windowWidth - widthToSet) / 2.0) + "px")
    }

    function showOrHideTitleAndButtons() {
        if (this.settings.title != null && this.settings.title != "") {
            this.showTitle = true;
            this.dialogTitleContainer.show();
        } else {
            this.showTitle = false;
            this.dialogTitleContainer.hide();
        }

        if (this.settings.buttons != null && this.settings.buttons.length > 0) {
            this.showButtons = true;
            this.buttonsContainer.show();
        } else {
            this.showButtons = false;
            this.buttonsContainer.hide();
        }
    }

    function adjustSizeOfContent() {
        var desiredTopOfContentContainer = 0;
        var desiredBottomOfContentContainer = 0;

        if (this.showTitle) {
            var titleBarHeight = this.dialogTitleContainer.actual('outerHeight');
            desiredTopOfContentContainer = titleBarHeight;
        }

        if (this.showButtons) {
            var buttonsBarHeight = this.buttonsContainer.actual('outerHeight');
            desiredBottomOfContentContainer = buttonsBarHeight;
        }

        this.contentContainer
            .css("top", desiredTopOfContentContainer + "px")
            .css("bottom", desiredBottomOfContentContainer + "px");
    }

    function keydownEscHandler(e) {
        if (e.which == KeyCodesMap.getKeys('Esc')) {
            e.data.close();
        }
    }

    function keydownTabHandler(e) {
        if (e.which == KeyCodesMap.getKeys('Tab')) {
            var focusableElements = $(e.data.dialogContainer).find(':focusable:visible[tabindex!="-1"]');
            var focusedElement = $(focusableElements).filter(document.activeElement);

            if (focusedElement.length == 0) {
                focusableElements.first().focus();
            } else if (focusedElement[0] === focusableElements.last()[0]) {
                $(focusableElements).first().focus();
            } else {
                var nextElementIndex = $.inArray(focusedElement[0], focusableElements) + 1;

                if (nextElementIndex < focusableElements.length) {
                    focusableElements[nextElementIndex].focus();
                } else {
                    focusableElements[0].focus();
                }
            }

            e.stopImmediatePropagation();
            e.preventDefault();
        }
    }

    function focusFirstElement() {
        $(this.dialogContainer).find(':focusable:visible[tabindex!="-1"]').first().focus();
    }
})(window);

(function (window, undefined) {
    var overlayElement = $('<div class="overlay animated preInAnimation" style="display: none"><div class="overlayInside"></div></div>');
    var overlayShowCounter = 0;

    function init() {
        $(overlayElement).appendTo('body');
    }

    Overlay = function () {
    };

    Overlay.prototype.show = function () {
        if(overlayShowCounter<=0){
            $(overlayElement)
                .addClass("preInAnimation")
                .css("display", "block")
                .css('z-index', 80000);

            Animator.animateIn(overlayElement, "fade");
        }

        overlayShowCounter++;

        return {
            'zindex': 80000,
            'id': 1
        };
    };

    Overlay.prototype.hide = function () {
        overlayShowCounter--;

        if (overlayShowCounter < 0){
            overlayShowCounter = 0;
        }

        if (overlayShowCounter == 0) {
            Animator.animateOut(overlayElement, "fade");
        }
    };

    init();
    window.Overlay = new Overlay();
})(window);

(function (window, undefined) {

    Pagination = function (options) {
        this.settings = $.extend({
            domSelector: null,

            currentPageShowMargin: 4,

            limit: 5,
            totalElements: 0,
            currentPage: 0,

            onPageChange: function (page) {
            },

            init: true
        }, options);

        this.totalPages = 0;
        this.limit = this.settings.limit;
        this.totalElements = this.settings.totalElements;
        this.currentPage = this.settings.currentPage;
        this.currentPageShowMargin = this.settings.currentPageShowMargin;

        this.domElement = null;
        this.firstElement = null;
        this.prevElement = null;
        this.pagesElement = null;
        this.nextElement = null;
        this.lastElement = null;

        if (this.settings.init)
            this.init();
    };

    Pagination.prototype.init = function (options) {
        var _this = this;

        this.settings = $.extend(this.settings, options);

        this.totalPages = 0;
        this.currentPage = this.settings.currentPage;
        this.totalElements = this.settings.totalElements;
        this.limit = this.settings.limit;
        this.currentPageShowMargin = this.settings.currentPageShowMargin;

        this.domElement = $(this.settings.domSelector).addClass("pagination");

        this.firstElement = $('<a href="javascript: void(0);" class="first">&nbsp;<span></span></a>').click(function () {
            changePage(_this, this);
        });
        this.prevElement = $('<a href="javascript: void(0);" class="prev">&nbsp;<span></span></a>').click(function () {
            changePage(_this, this);
        });
        this.pagesElement = $('<span class="pages"></span>');
        this.nextElement = $('<a href="javascript: void(0);" class="next">&nbsp;<span></span></a>').click(function () {
            changePage(_this, this);
        });
        this.lastElement = $('<a href="javascript: void(0);" class="last">&nbsp;<span></span></a>').click(function () {
            changePage(_this, this);
        });

        this.domElement
            .append(this.firstElement)
            .append(this.prevElement)
            .append(this.pagesElement)
            .append(this.nextElement)
            .append(this.lastElement);

        this.redraw();
    };

    Pagination.prototype.redraw = function () {
        this.totalPages = Math.ceil(this.totalElements / this.limit);

        if (this.totalPages <= 1) {
            this.domElement.hide();
        } else {
            this.domElement.show();
        }

        this.redrawPages();

        this.firstElement.attr("rel", 0);
        this.lastElement.attr("rel", this.totalPages - 1);
        recalculateNextAndPrev(this);
    };

    Pagination.prototype.redrawPages = function () {
        var _this = this;

        $(this.pagesElement).empty();

        var startIndex = Math.max(0, this.currentPage - this.currentPageShowMargin);
        var endIndex = Math.min(this.totalPages, this.currentPage + this.currentPageShowMargin + 1);

        for (var i = startIndex; i < endIndex; i++) {
            $('<a href="javascript: void(0);" rel="' + i + '">' + (i + 1) + "</a>").click(function () {
                changePage(_this, this);
            }).appendTo(this.pagesElement);
        }
    };

    Pagination.prototype.changeParams = function (params) {
        var newParams = $.extend({
            limit: this.limit,
            totalElements: this.totalElements,
            currentPage: this.currentPage
        }, params);

        var changed = false;

        if (newParams.limit != this.limit) {
            this.limit = newParams.limit;
            changed = true;
        }

        if (newParams.totalElements != this.totalElements) {
            this.totalElements = newParams.totalElements;
            changed = true;
        }

        if (newParams.currentPage != this.currentPage) {
            this.currentPage = newParams.currentPage;
            changed = true;
        }

        if (changed) {
            this.redraw();
        }
    };


    function changePage(pagination, paginationElement) {
        if (!$(paginationElement).hasClass("disabled")) {
            var changeToPage = parseInt($(paginationElement).attr("rel"));

            if (pagination.currentPage != changeToPage) {
                pagination.currentPage = changeToPage;
                pagination.redrawPages();

                recalculateNextAndPrev(pagination);
                pagination.settings.onPageChange(pagination.currentPage);
            }
        }
    }

    function recalculateNextAndPrev(pagination) {
        var prevPage = parseInt(pagination.currentPage) - 1;
        var nextPage = parseInt(pagination.currentPage) + 1;

        if (prevPage >= 0) {
            pagination.prevElement.attr("rel", prevPage).removeClass("disabled");
            pagination.firstElement.removeClass("disabled");
        } else {
            pagination.prevElement.attr("rel", pagination.currentPage).addClass("disabled");
            pagination.firstElement.addClass("disabled");
        }

        if (nextPage < pagination.totalPages) {
            pagination.nextElement.attr("rel", nextPage).removeClass("disabled");
            pagination.lastElement.removeClass("disabled");
        } else {
            pagination.nextElement.attr("rel", pagination.currentPage).addClass("disabled");
            pagination.lastElement.addClass("disabled");
        }

        $(pagination.pagesElement).children(".active").removeClass("active");
        $(pagination.pagesElement).children("[rel='" + pagination.currentPage + "']").addClass("active");
    }

    window.Pagination = Pagination;
})(window);
(function (window, undefined) {

    TableView = function (options) {
        var _this = this;

        this.settings = $.extend({
            tableDomSelector: "",
            url: "",

            onInited: function () {
            },

            onInitError: function () {
            },

            onDataLoadError: function () {
            },

            onDataLoaded: function (data, totalDataCount) {
            },

            onSelectRow: function (row) {
            },

            onDeselectRow: function (row) {
            },

            onDoubleClick: function (row) {
            },

            onNoRowSelected: function () {
            },

            onRowKeyPress: function (keyCode, row) {
            },

            onSortChanged: function (column, order) {

            },

            postRemoteDataDownload: function (data, totalDataCount) {
                return $.Deferred().resolve().promise();
            },

            currentPage: 0,
            limit: 50,

            selectOnlyOneRow: true,

            columns: [],

            rowCallback: function (row, data) {
            },

            data: function (standardData) {

            },

            noDataMessage: "",
            hideTableWhenNoData: true,

            checkable: false,
            idProperty: null,
            checkedOnInit: [],
            hideHeader: false,
            jsonContent: false,

            nestedPaginationAttr: false,

            sortColumn: null,
            sortOrder: null
        }, options);

        this.initingDeferred = $.Deferred();
        this.requestId = 1;
        this.domTable = $(this.settings.tableDomSelector);
        this.domTableBody = $(this.domTable).find("tbody");
        this.domTableHeader = $(this.domTable).find("thead");

        this.paginationContainerDom = $('<div class="tableViewPaginationContainer" />').insertAfter(this.domTable);
        this.paginationDom = $("<div />").appendTo(this.paginationContainerDom);

        this.noDataMessageContainer = $('<div class="tableViewNoDataMessage" style="display: none;"/>').insertAfter(this.domTable);
        this.noDataMessageContainer.text(_this.settings.noDataMessage);

        this.pagination = new Pagination({
            domSelector: _this.paginationDom,
            limit: _this.settings.limit,
            totalElements: 0,
            currentPage: _this.settings.currentPage,

            onPageChange: function (page) {
                _this.changePage(page);
            },

            init: false
        });

        this.domTable.attr('tabindex', '0');
        this.focused = false;
        this.hiddenColumns = new Array();
        this.latestAjaxRequest = null;
        this.checkboxTemplate = null;
        this.checkedRows = new Array();


        this.sortable = false;
        this.sortableColumns = [];
        this.sortColumn = null;
        this.sortOrder = "ASC";

        this.spinner = new Spinner();
        this.spinner.dark();
        this.spinner.withBackground();
        this.spinner.pinTo(this.domTable);

        Location.bindEventForLocation('keydown', function (event) {
            if (_this.focused) {
                if (event.keyCode == KeyCodesMap.getKeys('Down')) {
                    _this.selectNextRow();
                    event.preventDefault();
                } else if (event.keyCode == KeyCodesMap.getKeys('Up')) {
                    _this.selectPrevRow();
                    event.preventDefault();
                } else {
                    if (_this.isAnyRowSelected()) {
                        _this.settings.onRowKeyPress(event.keyCode, _this.getSelectedRows());
                    }
                }

            }
        });

        $(this.domTable).on('focus', function (event) {
            _this.focused = true;
        });

        $(this.domTable).on('blur', function (event) {
            _this.focused = false;
        });

        if (this.settings.checkable) {
            this.domTableHeader.find("tr").prepend("<th>&nbsp;</th>");

            if (this.settings.idProperty == null) {
                throw "When checkable is true you should set idProperty";
            }

            if (this.settings.checkedOnInit != null) {

                this.checkedRows = this.settings.checkedOnInit;
            }
        }

        if (this.settings.hideHeader) {
            this.domTableHeader.hide();
        }


        //Sorting ----------------------------------
        this.sortableColumns = this.domTableHeader.find("th[sortable]");
        if (this.sortableColumns.length > 0) {
            this.sortable = true;
        }

        if (this.sortable) {
            this.sortableColumns.each(function () {
                var columnId = $(this).attr("column-id");

                if (columnId == undefined || columnId == null) {
                    throw "All sortable columns must have column-id attribute set";
                }
            });

            this.sortableColumns.addClass("sortable");
            findDefaultSortColumn.call(this);
            markSortOnDomColumn.call(this, false);

            this.domTableHeader.find("th[sortable]").click(function () {
                if (_this.sortColumn == $(this).attr("column-id")) {
                    _this.sortOrder = otherOrder.call(_this, _this.sortOrder);
                } else {
                    _this.sortOrder = "ASC";
                }
                _this.sortColumn = $(this).attr("column-id");

                _this.settings.onSortChanged(_this.sortColumn, _this.sortOrder);

                _this.reloadData();
                markSortOnDomColumn.call(_this, true);
            });
        }
        //-----------------------


        Dependency3.loadScript("template/widgets/checkbox.do", "text/x-dot-template").
            done(function (descriptor) {
                _this.checkboxTemplate = doT.templateFromElement(descriptor.id);//TODO przenieść taki template gdzieś indziej, może kontrolka checkbox? tak żeby był renderowany tylko raz przy initcie checkboxa w ogóle......
                _this.init();
            });
    };

    TableView.prototype.destroy = function () {
        ;
    };

    TableView.prototype.init = function () {
        var _this = this;

        if (this.isAnyRowSelected())
            this.settings.onDeselectRow();
        this.latestAjaxRequest = callAjaxForData(
            _this,
            prepareData(_this),

            function (response) {
                _this.pagination.init({
                    totalElements: response.totalDataCount
                });

                _this.settings.onDataLoaded(response.data, response.totalDataCount);
                _this.settings.onInited();
                _this.initingDeferred.resolve();
            },

            function () {
                _this.initingDeferred.reject();
                _this.settings.onInitError();
            }
        );
    };

    TableView.prototype.changePage = function (page) {
        this.settings.currentPage = page;
        this.reloadData();

    };

    TableView.prototype.reloadData = function () {
        var _this = this;

        this.spinner.showDelayed();

        if (this.latestAjaxRequest != null && "pending" == this.latestAjaxRequest.state()) {
            this.latestAjaxRequest.abort();
        }

        if (this.isAnyRowSelected())
            this.settings.onDeselectRow();


        this.latestAjaxRequest = callAjaxForData(
            _this,
            prepareData(_this),

            function (response) {
                _this.pagination.changeParams({
                    totalElements: response.totalDataCount,
                    currentPage: _this.settings.currentPage
                });

                _this.settings.onDataLoaded(response.data, response.totalDataCount);
                _this.spinner.hide();
            },

            function(){
                _this.spinner.hide();
                _this.settings.onDataLoadError();
            }

        );

        return this.latestAjaxRequest;
    };


    TableView.prototype.changeRowSelection = function (row) {
        var rowDom = $(row);

        if (this.settings.selectOnlyOneRow == true) {
            $(this.domTableBody)
                .children("tr").not(row)
                .removeClass("selected");

            this.settings.onDeselectRow(rowDom);
        }

        if (rowDom.hasClass("selected")) {
            rowDom.removeClass("selected");
            changeCheckboxIfNecessary(this, row);
            this.settings.onDeselectRow(rowDom);
        } else {
            rowDom.addClass("selected");
            changeCheckboxIfNecessary(this, row);
            this.settings.onSelectRow(rowDom);
        }

        invokeOnNoSelectedRowsIfNecessary(this);
    };

    TableView.prototype.selectFirstRow = function () {
        var firstRow = $(this.domTable).find('tbody tr:visible').first();

        if (firstRow.length != 0) {
            firstRow.addClass("selected");
            changeCheckboxIfNecessary(this, firstRow);
            this.settings.onSelectRow(firstRow);
        }
    };

    TableView.prototype.selectRow = function (rowNumber) {
        var row = $(this.domTable).find('tbody tr:visible').eq(rowNumber);

        if (row.length != 0) {
            row.addClass("selected");
            changeCheckboxIfNecessary(this, row);
            this.settings.onSelectRow(row);
        }
    };

    TableView.prototype.deselectRow = function (rowNumber) {
        var row = $(this.domTable).find('tbody tr:visible').eq(rowNumber);

        if (row.length != 0) {
            row.removeClass("selected");
            changeCheckboxIfNecessary(this, row);
            this.settings.onDeselectRow(row);
        }
    };
//todo rethink with checkable
    TableView.prototype.selectNextRow = function () {
        var selectedRows = this.getFirstSelectedRow();
        var rowNext = selectedRows.next();

        if (rowNext.length == 0) {
            rowNext = $(this.domTable).find('tbody tr:visible').first();
        }

        if (this.settings.selectOnlyOneRow == true) {
            selectedRows.removeClass("selected");
            this.settings.onDeselectRow(selectedRows);
            rowNext.addClass("selected");
            this.settings.onSelectRow(rowNext);
        }

    };
//todo rethink with checkable
    TableView.prototype.selectPrevRow = function () {
        var selectedRows = this.getFirstSelectedRow();
        var rowPrev = selectedRows.prev();

        if (rowPrev.length == 0) {
            rowPrev = $(this.domTable).find('tbody tr:visible').last();
        }

        if (this.settings.selectOnlyOneRow == true) {
            selectedRows.removeClass("selected");
            this.settings.onDeselectRow(selectedRows);
            rowPrev.addClass("selected");
            this.settings.onSelectRow(rowPrev);
        }

    };


    TableView.prototype.getSelectedRows = function () {
        return $(this.domTableBody).children("tr.selected");
    };

    TableView.prototype.getAllRows = function () {
        return $(this.domTableBody).children("tr");
    };

    TableView.prototype.selectAllRows = function () {
        _this = this;
        $(_this.domTableBody).children("tr").each(function (index, row) {
            $(row).addClass("selected");
            changeCheckboxIfNecessary(_this, row);
            _this.settings.onSelectRow(row);
        });
    };

    TableView.prototype.deselectAllRows = function () {
        _this = this;
        $(_this.domTableBody).children("tr.selected").each(function (index, row) {
            $(row).removeClass("selected");
            changeCheckboxIfNecessary(_this, row);
            _this.settings.onDeselectRow(row);
        });
    };


    TableView.prototype.getFirstSelectedRow = function () {
        return $(this.domTableBody).children("tr.selected").first();
    };

    TableView.prototype.changeUrl = function (url) {
        this.settings.url = url;
        this.changePage(0)
    };

    TableView.prototype.isAnyRowSelected = function () {
        return this.getSelectedRows().length > 0;
    };

    TableView.prototype.hideColumn = function (colNumber) {
        $(this.domTableHeader).find("th:nth-child(" + colNumber + ")").hide();
        $(this.domTableBody).find("td:nth-child(" + colNumber + ")").hide();

        this.hiddenColumns.push(colNumber);
    };

    TableView.prototype.showColumn = function (colNumber) {
        $(this.domTableHeader).find("th:nth-child(" + colNumber + ")").show();
        $(this.domTableBody).find("td:nth-child(" + colNumber + ")").show();


        var hiddenElementIndex = this.hiddenColumns.indexOf(colNumber);
        if (hiddenElementIndex > -1) {
            this.hiddenColumns.splice(hiddenElementIndex, 1);
        }
    };

    TableView.prototype.getCheckedIds = function () {
        return this.checkedRows;
    }

    TableView.prototype.setCheckedIds = function (checkedIds) {
        var _this = this;
        this.checkedRows = checkedIds;

        $(_this.domTableBody).children("tr").each(function (index, row) {
            var d = $(row).data("data");
            var dd = d[_this.settings.idProperty];

            if (_this.checkedRows.indexOf(dd) >= 0) {

                _this.selectRow(index);
            } else {
                _this.deselectRow(index);
            }
        });

    }

    TableView.prototype.howManyChecked = function () {
        return this.checkedRows.length;
    }

    function prepareData(_this) {
        var data = {
            requestId: ++_this.requestId
        };

        if (_this.settings.nestedPaginationAttr) {
            data.pagination = {}

            data.pagination.limit = _this.settings.limit;
            data.pagination.page = _this.settings.currentPage;
        } else {
            data.limit = _this.settings.limit;
            data.page = _this.settings.currentPage;
        }

        if (_this.sortable) {
            data.sortColumn = _this.sortColumn;
            data.sortOrder = _this.sortOrder.toUpperCase();
        }

        _this.settings.data(data);
        return data;
    }

    function invokeOnNoSelectedRowsIfNecessary(_this) {
        if ($(_this.domTableBody).children("tr.selected").length == 0) {
            _this.settings.onNoRowSelected();
        }
    }

    function changeCheckboxIfNecessary(_this, row) {
        if (_this.settings.checkable) {
            var _thisRow = $(row);

            var rowSelected = _thisRow.hasClass("selected");

            var d = _thisRow.data("data");
            var dd = d[_this.settings.idProperty];

            if (rowSelected) {//todo refactor
                if (_this.checkedRows.indexOf(dd) < 0) {
                    _this.checkedRows.push(dd);
                }
            } else {

                if (_this.checkedRows.indexOf(dd) >= 0) {
                    _this.checkedRows.splice(_this.checkedRows.indexOf(dd), 1);
                }

            }

            _thisRow.find('input[type="checkbox"]').prop("checked", rowSelected);

        }
    }

    function callAjaxForData(tableView, requestData, onSuccessHandler, onErrorHandler) {
        var _this = tableView;

        var ajaxSettings = {
            url: _this.settings.url,
            dataType: 'json',
            type: 'post',

            data: requestData,

            error: _this.settings.onDataLoadError,

            success: function (response) {
                _this.settings.postRemoteDataDownload(response.data, response.totalDataCount).done(function () {
                    _this.domTableBody.empty();
                    invokeOnNoSelectedRowsIfNecessary(_this);

                    if (!!response.data) {
                        if (response.data.length == 0) {
                            if (_this.settings.noDataMessage != null && _this.settings.noDataMessage != "") {
                                _this.noDataMessageContainer.show();
                            }

                            if (_this.settings.hideTableWhenNoData) {
                                _this.domTable.hide();
                            }
                        } else {
                            _this.noDataMessageContainer.hide();

                            for (var i = 0; i < response.data.length; i++) {
                                var rowOfData = response.data[i];
                                var tableRow = $('<tr/>')
                                    .click(function (e) {
                                        _this.changeRowSelection(this);
                                        e.stopPropagation();
                                    })
                                    .data("data", rowOfData);

                                tableRow.dblclick(function () {
                                    _this.settings.onDoubleClick($(this));
                                });

                                if (_this.settings.checkable) {
                                    var checkboxCell = $("<td />");
                                    var checkBoxData = {
                                        id: null,
                                        value: null
                                    };

                                    //TODO ckeckbaox callbak
                                    var ckeckbox = $(_this.checkboxTemplate(checkBoxData).trim());
                                    ckeckbox.click(function (e) {
                                        e.preventDefault();//todo rethink: pin handler only once
                                    });
                                    checkboxCell.html(ckeckbox);
                                    tableRow.append(checkboxCell);

                                    var f = tableRow.data("data");
                                    var ff = f[_this.settings.idProperty];
                                    if (_this.checkedRows.indexOf(ff) != -1) {
                                        $(ckeckbox).find('input[type="checkbox"]').prop("checked", true);
                                        tableRow.addClass("selected");
                                    }


                                }

                                for (var y = 0; y < _this.settings.columns.length; y++) {
                                    var tableCell = $("<td />");

                                    if (_this.hiddenColumns.indexOf(y + 1) > -1) {
                                        tableCell.hide();
                                    }

                                    _this.settings.columns[y](rowOfData, tableCell);
                                    tableRow.append(tableCell);
                                }

                                _this.settings.rowCallback(tableRow, rowOfData);

                                _this.domTableBody.append(tableRow);
                            }

                            _this.domTable.show();
                        }

                    }

                    onSuccessHandler(response);
                });
            }
        };

        if (_this.settings.jsonContent) {
            ajaxSettings.data = JSON.stringify(ajaxSettings.data);
            ajaxSettings.contentType = "application/json";
        }

        return $.ajax(ajaxSettings);
    }

    TableView.prototype.focus = function () {
        this.domTable.focus();
        var topOfTable = this.domTable.position().top;

        if (topOfTable < 200)
            $.scrollTo(0);
        else
            $.scrollTo(topOfTable);
    }


    TableView.prototype.initingPromise = function () {
        return this.initingDeferred.promise();
    }

    TableView.prototype.forSelected = function (f) {
        var action = $.Deferred();
        var idsToAction = this.getCheckedIds();
        var counter = idsToAction.length;

        if(counter == 0){
            action.resolve();
        }

        for (var i = 0; i < idsToAction.length; i++) {
            var id = idsToAction[i];

            f(id).done(function () {
                counter--;

                if (counter == 0) {
                    action.resolve();
                }
            });
        }

        return action.promise();
    }


    //private -----------------------------
    function findDefaultSortColumn() {

        if (this.settings.sortColumn != null) {
            this.sortColumn = this.settings.sortColumn;
            this.sortOrder = this.settings.sortOrder;
        } else {
            var defaultColumn = this.domTableHeader.find("th[sortable-default]").first();

            if (defaultColumn.length == 0) {
                defaultColumn = this.domTableHeader.find("th[sortable]").first();
            }

            //get column id
            var columnId = defaultColumn.attr("column-id");
            this.sortColumn = columnId;

            if (columnId == undefined && columnId == null && columnId != "") {
                throw "If column is marked as sortable column-id attribute have to be set.";
            }

            ///get order
            var order = defaultColumn.attr("sortable-default");

            if (order != undefined && order != null) {
                this.sortOrder = order.toUpperCase();
            } else {
                this.sortOrder = "ASC";
            }
        }
    }

    function markSortOnDomColumn(animated) {
        var domColumnToSort = this.sortableColumns.filter('[column-id="' + this.sortColumn + '"]');
        var indicatorInThisColumn = domColumnToSort.find(".sortIndicator");

        if (indicatorInThisColumn.length == 0) {
            $(this.domTableHeader).find(".sortIndicator").remove();

            var indicator = $('<span class="sortIndicator" />');
            indicator.addClass(this.sortOrder.toLowerCase());

            if (animated) {
                indicator.addClass("preInAnimation");
            }

            indicator.appendTo(domColumnToSort);

            if (animated) {
                Animator.animateIn(indicator, "bounceFwd", function () {
                });
            }
        } else {
            indicatorInThisColumn.toggleClass("asc desc");
        }
    }

    function otherOrder(order) {
        if (order.toUpperCase() == "ASC") {
            return "DESC";
        } else {
            return "ASC";
        }
    }

    window.TableView = TableView;
})(window);
(function (window, undefined) {

    Tabs = function (options) {
        var _this = this;

        this.settings = $.extend({
            container: "",
            defaultTab: null,
            destroyOnLocationUnload: true
        }, options);


        this.container = null;
        this.tabsContainer = null;
        this.tabs = [];
        this.tabsContents = [];

        this.relToTab = {};
        this.relToTabContent = {};

        this.shouldDisplay = false;
        this.scrollingDiff = 0;
        this.maxLeftScroll = 0;
        this.containerWidth = 0;

        this.leftScrollControl = null;
        this.rightScrollControl = null;

        this.currLeft = 0;

        this.windowResizeHandler = null;

        this.init();
    };

    Tabs.prototype.init = function () {
        var _this = this;

        _this.container = $(_this.settings.container);
        _this.tabsContainer = _this.container.children("ul");
        _this.tabs = _this.container.children("ul").children("li");
        _this.tabsContents = _this.container.children("div");


        _this.container.addClass("tabs_tabsContainer");

        _this.tabsContents
            .hide()
            .addClass("tabs_tabContent");

        _this.tabsContainer.addClass("tabs_tabsLinks");

        _this.tabs.quickEach(function () {
            var rel = this.attr("rel");

            if (rel != null && rel != "") {
                _this.relToTabContent[rel] = $("#" + rel);
                _this.relToTab[rel] = $(this);
            }
        });

        _this.tabs.click(function () {
            _this.openTab($(this).attr("rel"));
        });

        if (_this.settings.defaultTab == null) {
            var firstTab = _this.tabs.first().attr("rel");
            _this.openTab(firstTab);
        } else {
            _this.openTab(_this.settings.defaultTab);
        }

        //add scrolling controls
        _this.rightScrollControl = $('<span style="display: none;"><span class="tabs_tabScroller_arrow"></span></span>')
            .addClass("tabs_tabScroller_bg right")
            .click(function () {
                scrollRight(_this);
            })
            .appendTo(_this.container);

        _this.leftScrollControl = $('<span style="display: none;"><span class="tabs_tabScroller_arrow"></span></span>')
            .addClass("tabs_tabScroller_bg left")
            .click(function () {
                scrollLeft(_this);
            })
            .appendTo(_this.container);

        initScrolling(_this);

        _this.windowResizeHandler = function () {
            recalculateScrolling(_this);
        };

        $(window).on('resize', _this.windowResizeHandler);

        $(_this.container).mouseenter(function () {
            recalculateScrolling(_this);
        });


        if (_this.settings.destroyOnLocationUnload) {
            $(window.eventBus).one('Location.unloading', function () {
                _this.destroy();
            });
        }
    };

    Tabs.prototype.openTab = function (tab) {
        if (tab != null && tab != "") {
            this.relToTab[tab].addClass("active");
            this.tabs.not(this.relToTab[tab]).removeClass("active");

            this.tabsContents.not(this.relToTabContent[tab]).hide();
            this.relToTabContent[tab].show();
        }
    };

    Tabs.prototype.showTab = function (tab) {
        if (tab != null && tab != "") {
            this.relToTab[tab].show();
        }
    };

    Tabs.prototype.hideTab = function (tab) {
        if (tab != null && tab != "") {
            this.relToTab[tab].hide();
        }
    };

    Tabs.prototype.destroy = function () {
        $(window).off('resize', this.windowResizeHandler);
    };

    function initScrolling(tabsObj) {
        if (tabsObj.container.get(0).scrollWidth == 0) {
            setTimeout(function () {
                initScrolling(tabsObj);
            }, 1000);
        } else {
            tabsObj.currLeft = 0;
            recalculateScrolling(tabsObj);
        }
    }

    function recalculateScrolling(tabsObj) {
        tabsObj.containerWidth = tabsObj.container.get(0).clientWidth;
        tabsObj.scrollingDiff = tabsObj.tabsContainer.get(0).scrollWidth - tabsObj.container.get(0).offsetWidth;
        tabsObj.shouldDisplay = tabsObj.scrollingDiff > 0;
        tabsObj.maxLeftScroll = -(32 + tabsObj.scrollingDiff);

        if (tabsObj.shouldDisplay) {
            tabsObj.leftScrollControl.show();
            tabsObj.rightScrollControl.show();
        } else {
            tabsObj.maxLeftScroll = 0;
            tabsObj.leftScrollControl.hide();
            tabsObj.rightScrollControl.hide();
            scroll(tabsObj, 0);
        }
    }

    function scrollLeft(tabsObj) {
        scrollDiff(tabsObj, 100);
    }

    function scrollRight(tabsObj) {
        scrollDiff(tabsObj, -100);
    }

    function scrollDiff(tabsObj, diff) {
        scroll(tabsObj, tabsObj.currLeft + diff);
    }

    function scroll(tabsObj, newLeft) {
        if (tabsObj.currLeft == newLeft) {
            return;
        }

        if (newLeft > 0) {
            newLeft = 0;
        } else if (newLeft < tabsObj.maxLeftScroll) {
            newLeft = tabsObj.maxLeftScroll;
        }

        tabsObj.currLeft = newLeft;
        tabsObj.tabsContainer.css("left", tabsObj.currLeft + "px");
    }

    window.Tabs = Tabs;
})(window);
(function (window) {
    var Ranges = {
        CUSTOM: {range: "CUSTOM", labelExtended: "-", buttonLabel: "-"},

        YESTERDAY: {
            range: "YESTERDAY",
            labelExtended: Messages.get("dateRanges.day.yesterday"),
            buttonLabel: Messages.get("dateRanges.day.yesterday")
        },
        TODAY: {
            range: "TODAY",
            labelExtended: Messages.get("dateRanges.day.today"),
            buttonLabel: Messages.get("dateRanges.day.today")
        },
        TOMORROW: {
            range: "TOMORROW",
            labelExtended: Messages.get("dateRanges.day.tomorrow"),
            buttonLabel: Messages.get("dateRanges.day.tomorrow")
        },

        WEEK_PREVIOUS: {
            range: "WEEK_PREVIOUS",
            labelExtended: Messages.get("dateRanges.week.previous"),
            buttonLabel: Messages.get("common.period.prev")
        },
        WEEK_CURRENT: {
            range: "WEEK_CURRENT",
            labelExtended: Messages.get("dateRanges.week.current"),
            buttonLabel: Messages.get("common.period.current")
        },
        WEEK_NEXT: {
            range: "WEEK_NEXT",
            labelExtended: Messages.get("dateRanges.week.next"),
            buttonLabel: Messages.get("common.period.next")
        },

        MONTH_PREVIOUS: {
            range: "MONTH_PREVIOUS",
            labelExtended: Messages.get("dateRanges.month.previous"),
            buttonLabel: Messages.get("common.period.prev")
        },
        MONTH_CURRENT: {
            range: "MONTH_CURRENT",
            labelExtended: Messages.get("dateRanges.month.current"),
            buttonLabel: Messages.get("common.period.current")
        },
        MONTH_NEXT: {
            range: "MONTH_NEXT",
            labelExtended: Messages.get("dateRanges.month.next"),
            buttonLabel: Messages.get("common.period.next")
        },

        YEAR_PREVIOUS: {
            range: "YEAR_PREVIOUS",
            labelExtended: Messages.get("dateRanges.year.previous"),
            buttonLabel: Messages.get("common.period.prev")
        },
        YEAR_CURRENT: {
            range: "YEAR_CURRENT",
            labelExtended: Messages.get("dateRanges.year.current"),
            buttonLabel: Messages.get("common.period.current")
        },
        YEAR_NEXT: {
            range: "YEAR_NEXT",
            labelExtended: Messages.get("dateRanges.year.next"),
            buttonLabel: Messages.get("common.period.next")
        }
    }

    var CustomRangeField = {FROM: 1, TO: 2}

    DateRangePicker = function (fieldSelector, options) {
        var _this = this;

        this.rangeDto = null;
        this.rangeShift = 0;
        this.currentRange = null;

        this.settings = $.extend({
            attachPopup: null,
            destroyOnLocationUnload: true,
            onRangeChanged: function (newRange) {

            },

            showYearRange: true
        }, options);

        this.field = $(fieldSelector);
        this.popupAttachTo = this.field;
        this.pickerContent = null;
        this.showing = false;
        this.customRangeSelected = [];

        this.customFromField = null;
        this.customFromPicker = null;
        this.customToField = null;
        this.customToPicker = null;

        if (this.settings.attachPopup != null) {
            this.popupAttachTo = $(this.settings.attachPopup)
        }

        this.preventHideOnClick = false;

        this.popup = new PopupBox({
            attachTo: this.popupAttachTo,
            position: "fixed"
        });
        setupPopupContent.apply(this);
        this.popup.setContent(this.pickerContent);

        this.field
            .addClass("DateRangePicker").addClass("DateRangePickerField")
            .attr("readonly", "readonly")
            .attr("placeholder", Messages.get("common.selectDatesRange"))
            .focus(function () {
                showPicker.apply(_this);
            });

        this.fieldEscapeHandler = function (event) {
            if (event.keyCode == KeyCodesMap.getKeys('Esc')) {
                hidePicker.apply(_this);
            }
        }

        this.field.on("keyup", this.fieldEscapeHandler);

        this.documentOnClickHandler = function (event) {
            if (_this.preventHideOnClick == false) {
                var clickedElement = $(event.target);

                if (_this.showing && !clickedElement.is(_this.field)) {

                    if ($(_this.pickerContent).find(clickedElement).length == 0) {
                        hidePicker.apply(_this);
                    }
                }
            } else {
                _this.preventHideOnClick = false;
            }
        }
        $(document).on("click", this.documentOnClickHandler);
        $(document).on("select2-open", this.documentOnClickHandler);

        this.pickerContent.on("click", function () {
            //this is tricky mechanism developed because jquery picker is recreating all its dom elements each time one of them is clicked, so i cannont check if clicked element belongs to picker because after click this element does not exist
            _this.preventHideOnClick = true;
        });

        if (_this.settings.destroyOnLocationUnload) {
            $(window.eventBus).one('Location.unloading', function () {
                _this.destroy();
            });
        }
    };

    DateRangePicker.prototype.destroy = function () {
        hidePicker.apply(this);
        $(document).off("click", this.documentOnClickHandler);
        $(document).off("select2-open", this.documentOnClickHandler);
        this.field.off("keyup", this.fieldEscapeHandler);
        this.popup.destroy();
    };

    DateRangePicker.prototype.getDateRangeDto = function () {
        return this.rangeDto;
    }

    DateRangePicker.prototype.getRangeType = function () {
        return this.rangeDto.range;
    }

    DateRangePicker.prototype.getRangeShift = function () {
        return this.rangeShift;
    }

    DateRangePicker.prototype.getRangeFrom = function () {
        if (this.rangeDto.range == Ranges.CUSTOM.range) {
            return this.rangeDto.from;
        } else {
            return null;
        }
    }

    DateRangePicker.prototype.isCustomRange = function () {
        return this.rangeDto != null && this.rangeDto.range != null && this.rangeDto.range == Ranges.CUSTOM.range;
    }

    DateRangePicker.prototype.setRangeFull = function (type, shift, from, to, fireOnChangeEvent) {
        if (type != null) {
            var dto = {
                range: type,
                shift: shift == null ? 0 : shift,
                from: from,
                to: to
            }

            setRangeFullDto.call(this, dto, fireOnChangeEvent);
        } else {

        }
    }

    DateRangePicker.prototype.setRangeDto = function (rangeDto, fireOnChangeEvent) {
        setRangeFullDto.call(this, rangeDto, fireOnChangeEvent);
    }


    DateRangePicker.prototype.setRangeType = function (type, fireOnChangeEvent) {
        if (type != Ranges.CUSTOM.range) {
            var rangeToSet = Ranges[type];

            if (rangeToSet != null) {
                setRange.call(this, rangeToSet, fireOnChangeEvent);
            }
        }
    }

    DateRangePicker.prototype.setRangeCurrentWeek = function (fireOnChangeEvent) {
        setRange.call(this, Ranges.WEEK_CURRENT, fireOnChangeEvent);
    }

    DateRangePicker.prototype.setRangeCurrentMonth = function (fireOnChangeEvent) {
        setRange.call(this, Ranges.MONTH_CURRENT, fireOnChangeEvent);
    }
    

    DateRangePicker.prototype.setRangeCurrentYear = function (fireOnChangeEvent) {
        setRange.call(this, Ranges.YEAR_CURRENT, fireOnChangeEvent);
    }


    DateRangePicker.prototype.getRangeDto = function () {
        return this.rangeDto;
    }

    DateRangePicker.prototype.getRangeTo = function () {
        if (this.rangeDto.range == Ranges.CUSTOM.range) {
            return this.rangeDto.to;
        } else {
            return null;
        }
    }

    DateRangePicker.prototype.gotoPrevious = function () {
        this.rangeShift -= 1;
        this.rangeDto.shift = this.rangeShift;
        updateInputFieldText.call(this);

        this.settings.onRangeChanged(this.rangeDto);
    }

    DateRangePicker.prototype.gotoNext = function () {
        this.rangeShift += 1;
        this.rangeDto.shift = this.rangeShift;
        updateInputFieldText.call(this);

        this.settings.onRangeChanged(this.rangeDto);
    }

    function setupPopupContent() {
        var _this = this;
        this.pickerContent = $('<div class="DateRangePickerContent" />');

        var predefinedPeriodsContainer = $('<div class="predefinedPeriodsContainer" />').appendTo(this.pickerContent);
        var customPeriodsContainer = $('<div class="customPeriodContainer" />').appendTo(this.pickerContent);

        preparePredefinedPeriodsContainer.call(_this, predefinedPeriodsContainer);
        prepareCustomPeriodContainer.call(_this, customPeriodsContainer);
    }

    function preparePredefinedPeriodsContainer(container) {
        var _this = this;

        var daysPredefinedGroup = $('<div class="predefinedGroup" />')
            .appendTo(container)
            .append('<h2>' + Messages.get("common.day") + '</h2>');

        var dayButtons = $('<div class="buttons" />')
            .append(predefinedRangeButton(Ranges.YESTERDAY))
            .append(predefinedRangeButton(Ranges.TODAY))
            .append(predefinedRangeButton(Ranges.TOMORROW))
            .appendTo(daysPredefinedGroup);

        var weeksPredefinedGroup = $('<div class="predefinedGroup" />').appendTo(container);
        weeksPredefinedGroup.append('<h2>' + Messages.get("common.week") + '</h2>');
        var weekButtons = $('<div class="buttons" />').appendTo(weeksPredefinedGroup);

        weekButtons
            .append(predefinedRangeButton(Ranges.WEEK_PREVIOUS))
            .append(predefinedRangeButton(Ranges.WEEK_CURRENT))
            .append(predefinedRangeButton(Ranges.WEEK_NEXT));

        var monthsPredefinedGroup = $('<div class="predefinedGroup" />').appendTo(container);
        monthsPredefinedGroup.append('<h2>' + Messages.get("common.month") + '</h2>');
        var monthButtons = $('<div class="buttons" />').appendTo(monthsPredefinedGroup);

        monthButtons
            .append(predefinedRangeButton(Ranges.MONTH_PREVIOUS))
            .append(predefinedRangeButton(Ranges.MONTH_CURRENT))
            .append(predefinedRangeButton(Ranges.MONTH_NEXT));


        if(this.settings.showYearRange){
            var yearPredefinedGroup = $('<div class="predefinedGroup" />').appendTo(container);
            yearPredefinedGroup.append('<h2>' + Messages.get("common.year") + '</h2>');
            var yearButtons = $('<div class="buttons" />').appendTo(yearPredefinedGroup);

            yearButtons
                .append(predefinedRangeButton(Ranges.YEAR_PREVIOUS))
                .append(predefinedRangeButton(Ranges.YEAR_CURRENT))
                .append(predefinedRangeButton(Ranges.YEAR_NEXT));

        }

        function predefinedRangeButton(range) {
            var button = $('<div />')
                .addClass("predefinedRangeButton")
                .text(range.buttonLabel)
                .attr("data-range", range.range)
                .data("range", range)
                .click(function () {
                    var r = $(this).data("range");

                    setRange.call(_this, r);
                    hidePicker.call(_this);
                });

            return button;
        }

    }

    function prepareCustomPeriodContainer(container) {
        var _this = this;

        container.append('<h2>' + Messages.get("DateRangePicker.customRange") + '</h2>');

        var row = $('<div class="onerow" />');
        var col1 = $('<div class="col6" />').appendTo(row);
        var col2 = $('<div class="col6 last" />').appendTo(row);


        var fromTemplate = $('<div class="onerow" >' +
            '<div class="col12">' +
            '<label for="onBehalfOfPersonField">' + Messages.get("common.range.from.uppercase") + '</label>' +
            '<input type="text" style="width: 17em" class="pickerInput"/>' +
            '</div>' +
            '</div><div class="onerow"><div class="col12 last"><div class="pickerField"></div></div></div>')
            .appendTo(col1);

        var pickerFromDiv = fromTemplate.find('.pickerField');
        var fromInput = fromTemplate.find('.pickerInput');
        this.customFromField = fromInput;
        this.customFromPicker = pickerFromDiv;

        var toTemplate = $('<div class="onerow" >' +
            '<div class="col12">' +
            '<label for="a">' + Messages.get("common.range.to.uppercase") + '</label>' +
            '<input type="text" style="width: 17em"  class="pickerInput"/>' +
            '</div>' +
            '</div><div class="onerow"><div class="col12 last"><div class="pickerField"></div></div></div>')
            .appendTo(col2);

        var pickerToDiv = toTemplate.find('.pickerField');
        var toInput = toTemplate.find('.pickerInput');
        this.customToField = toInput;
        this.customToPicker = pickerToDiv;

        pickerFromDiv
            .datepicker()
            .on("change", function () {
                var date = pickerFromDiv.datepicker("getDate");
                pickerToDiv.datepicker("option", "minDate", date);

                if (_this.customRangeSelected.lastIndexOf(CustomRangeField.FROM) == -1) {
                    _this.customRangeSelected.push(CustomRangeField.FROM);
                }

                showDatesInFields();

                if (_this.customRangeSelected.length >= 2) {
                    setCustomRange.call(_this);
                    hidePicker.call(_this);
                }
            });

        this.customToPicker = pickerToDiv
            .datepicker()
            .change(function () {
                var date = pickerToDiv.datepicker("getDate");
                pickerFromDiv.datepicker("option", "maxDate", date);

                if (_this.customRangeSelected.lastIndexOf(CustomRangeField.TO) == -1) {
                    _this.customRangeSelected.push(CustomRangeField.TO);
                }

                showDatesInFields();

                if (_this.customRangeSelected.length >= 2) {
                    setCustomRange.call(_this);
                    hidePicker.call(_this);
                }
            });

        container.append(row);

        function showDatesInFields() {
            var from = pickerFromDiv.datepicker("getDate");

            if (from != null) {
                fromInput.val(DateTimeTool.formatDate(from));
            }

            var to = pickerToDiv.datepicker("getDate");

            if (to != null) {
                toInput.val(DateTimeTool.formatDate(to));
            }
        }
    }

    function showPicker() {
        if (!this.showing) {
            this.preventHideOnClick = false;
            this.pickerContent.find(".predefinedRangeButton").removeClass("selected");

            if (this.rangeDto != null && this.rangeShift == 0 && this.rangeDto.range != "CUSTOM") {
                this.pickerContent.find('.predefinedRangeButton[data-range="' + this.rangeDto.range + '"]').addClass("selected");
            }

            this.customRangeSelected = [];

            this.popup.open();
            this.showing = true;
        }
    }

    function hidePicker() {
        if (this.showing) {
            this.popup.hide();
            this.showing = false;
        }
    }

    function setRange(range, fireOnChangeEvent) {
        this.rangeShift = 0;
        this.currentRange = range;

        this.rangeDto = {
            range: range.range,
            shift: 0
        };

        updateInputFieldText.call(this);

        if (fireOnChangeEvent == null || fireOnChangeEvent == true) {
            this.settings.onRangeChanged(this.rangeDto);
        }
    }

    function setCustomRange() {
        var _this = this;
        this.rangeShift = 0;

        var from = moment(_this.customFromPicker.datepicker("getDate"));
        var to = moment(_this.customToPicker.datepicker("getDate"));

        this.currentRange = Ranges.CUSTOM;

        this.rangeDto = {
            range: Ranges.CUSTOM.range,
            from: DateTimeTool.toStrutsDate(from),
            to: DateTimeTool.toStrutsDate(to),
            shift: 0
        }

        updateInputFieldText.call(this);

        this.settings.onRangeChanged(this.rangeDto);
    }

    function setRangeFullDto(dto, fireOnChangeEvent) {
        var rangeToSet = Ranges[dto.range];

        if (rangeToSet != null) {
            this.rangeDto = dto;
            this.currentRange = rangeToSet;
            this.rangeShift = dto.shift;

            updateInputFieldText.call(this);

            if (fireOnChangeEvent == true) {
                this.settings.onRangeChanged(this.rangeDto);
            }
        }
    }

    function updateInputFieldText() {
        if (this.rangeDto.range == Ranges.CUSTOM.range) {
            var daysCount = (new MomentPeriod(this.rangeDto.from, this.rangeDto.to)).daysCount();
            var effectiveFrom = moment(this.rangeDto.from).add(this.rangeDto.shift * daysCount, 'days');
            var effectiveTo = moment(this.rangeDto.to).add(this.rangeDto.shift * daysCount, 'days');


            if (effectiveFrom.isSame(effectiveTo)) {
                this.field.val(
                    DateTimeTool.formatDate(effectiveFrom)
                );
            } else {
                this.field.val(
                    DateTimeTool.formatDate(effectiveFrom) + " - " + DateTimeTool.formatDate(effectiveTo)
                );
            }
        } else {
            if (this.rangeDto.shift == 0) {
                this.field.val(this.currentRange.labelExtended)
            } else {
                var val = '';

                if (this.rangeDto.range == Ranges.YESTERDAY.range) {
                    val = DateTimeTool.formatDate(DateTimeSettings.getToday().subtract(1, 'days').add(this.rangeDto.shift, 'days'));
                } else if (this.rangeDto.range == Ranges.TODAY.range) {
                    val = DateTimeTool.formatDate(DateTimeSettings.getToday().add(this.rangeDto.shift, 'days'));
                } else if (this.rangeDto.range == Ranges.TOMORROW.range) {
                    val = DateTimeTool.formatDate(DateTimeSettings.getToday().add(this.rangeDto.shift + 1, 'days'));
                } else if (this.rangeDto.range == Ranges.MONTH_CURRENT.range) {
                    var from = DateTimeSettings.getToday().startOf('month').add(this.rangeDto.shift, 'months');
                    var to = moment(from).endOf('month');

                    val = DateTimeTool.formatDate(from) + " - " + DateTimeTool.formatDate(to)
                } else if (this.rangeDto.range == Ranges.MONTH_PREVIOUS.range) {
                    var from = DateTimeSettings.getToday().startOf('month').add(this.rangeDto.shift - 1, 'months');
                    var to = moment(from).endOf('month');

                    val = DateTimeTool.formatDate(from) + " - " + DateTimeTool.formatDate(to)
                } else if (this.rangeDto.range == Ranges.MONTH_NEXT.range) {
                    var from = DateTimeSettings.getToday().startOf('month').add(this.rangeDto.shift + 1, 'months');
                    var to = moment(from).endOf('month');

                    val = DateTimeTool.formatDate(from) + " - " + DateTimeTool.formatDate(to)
                } else if (this.rangeDto.range == Ranges.YEAR_CURRENT.range) {
                    var from = DateTimeSettings.getToday().startOf('year').add(this.rangeDto.shift, 'years');
                    var to = moment(from).endOf('year');

                    val = DateTimeTool.formatDate(from) + " - " + DateTimeTool.formatDate(to)
                } else if (this.rangeDto.range == Ranges.YEAR_PREVIOUS.range) {
                    var from = DateTimeSettings.getToday().startOf('year').add(this.rangeDto.shift - 1, 'years');
                    var to = moment(from).endOf('year');

                    val = DateTimeTool.formatDate(from) + " - " + DateTimeTool.formatDate(to)
                } else if (this.rangeDto.range == Ranges.YEAR_NEXT.range) {
                    var from = DateTimeSettings.getToday().startOf('year').add(this.rangeDto.shift + 1, 'years');
                    var to = moment(from).endOf('year');

                    val = DateTimeTool.formatDate(from) + " - " + DateTimeTool.formatDate(to)
                } else if (this.rangeDto.range == Ranges.WEEK_PREVIOUS.range || this.rangeDto.range == Ranges.WEEK_CURRENT.range || this.rangeDto.range == Ranges.WEEK_NEXT.range) {
                    var predefinedShift = 0;
                    var currentWeekStarts;

                    if (this.rangeDto.range == Ranges.WEEK_PREVIOUS.range) {
                        predefinedShift = -1;
                    } else if (this.rangeDto.range == Ranges.WEEK_NEXT.range) {
                        predefinedShift = 1;
                    }

                    if (DateTimeSettings.mondayIsFirstDayOfWeek()) {
                        currentWeekStarts = DateTimeSettings.getToday().startOf('isoWeek');

                    } else if (DateTimeSettings.sundayIsFirstDayOfWeek()) {


                        if (DateTimeSettings.getToday().isoWeekday() == 7) {//sunday
                            currentWeekStarts = DateTimeSettings.getToday();
                        } else {
                            currentWeekStarts = DateTimeSettings.getToday().startOf('isoWeek').subtract(1, 'days');
                        }

                    } else if (DateTimeSettings.saturdayIsFirstDayOfWeek()) {

                        if (DateTimeSettings.getToday().isoWeekday() == 6) {//saturday
                            currentWeekStarts = DateTimeSettings.getToday();
                        } else if (DateTimeSettings.getToday().isoWeekday() == 7) {//sunday
                            currentWeekStarts = DateTimeSettings.getToday().subtract(1, 'days');;
                        } else {
                            currentWeekStarts = DateTimeSettings.getToday().startOf('isoWeek').subtract(2, 'days');
                        }

                    }

                    var from = moment(currentWeekStarts).add(predefinedShift * 7, 'days').add(this.rangeDto.shift * 7, 'days');
                    var to = moment(from).add(6, 'days');

                    val = DateTimeTool.formatDate(from) + " - " + DateTimeTool.formatDate(to);
                }

                this.field.val(val);

            }
        }

    }

    window.DateRangePicker = DateRangePicker;
})(window);
(function (window, undefined) {

    ContextDialog = function () {
        var _this = this;

        this.visible = false;
        this.width = "50%";
        this.container = $('<div class="ContextDialog animated" style="display: none;"/>').appendTo("body");
        this.content = $('<div class="ContextDialogContent" />').appendTo(this.container);

        this.onCloseButtonClicked = function(){

        }

        this.closeButton = $('<div class="ContextDialogCloseButton" />').appendTo(this.container).click(function(){
            _this.hide();
            _this.onCloseButtonClicked();
        });
    }

    ContextDialog.prototype.destroy = function () {
        $(this.container).remove();
    }

    ContextDialog.prototype.show = function () {
        var _this = this;

        return slideIn.call(_this);
    }

    ContextDialog.prototype.hide = function () {
        var _this = this;

        return slideOut.call(_this);
    }

    ContextDialog.prototype.setWidth = function (width) {
        this.width = width;

        if (this.visible) {
            this.container
                .css("width", deriveDesiredPixelsWidth(this.width) + "px");
        }
    };

    ContextDialog.prototype.showCloseButton = function () {
        this.closeButton.show();
    }

    ContextDialog.prototype.hideCloseButton = function () {
        this.closeButton.hide();
    }

    ContextDialog.prototype.addContentClass = function (cssClass) {
        this.content.addClass(cssClass);
    }

    ContextDialog.prototype.removeContentClass = function (cssClass) {
        if(cssClass!=''){
            this.content.removeClass(cssClass);
        }
    }

    ContextDialog.prototype.appendContent = function (element) {
        this.content.append(element);
    }

    ContextDialog.prototype.setOnCloseClicked = function (f) {
        this.onCloseButtonClicked = f;
    }

    //private----------------------------------------------

    function slideIn() {
        if(!this.visible){
            this.visible = true;

            this.container
                .css("width", deriveDesiredPixelsWidth(this.width) + "px");

            var showing = $.Deferred();

            Animator.animateIn(this.container, "fullSlideRight", function () {
                showing.resolve();
            });

            return showing.promise();
        }else{
            return $.Deferred().resolve().promise();
        }
    }

    function slideOut() {
        if(this.visible) {
            this.visible = false;

            var hiding = $.Deferred();

            Animator.animateOut(this.container, "fullSlideRight", function () {
                hiding.resolve();
            });

            return hiding.promise();
        }else{
            return $.Deferred().resolve().promise();
        }
    }

    function deriveDesiredPixelsWidth(desiredWidth) {
        var windowWidth = $("body").innerWidth();
        var desiredWidthPixels = 0;

        if (_.isString(desiredWidth) && desiredWidth.indexOf("%") > -1) {
            desiredWidthPixels = (parseInt(desiredWidth) / 100.0) * windowWidth;
        } else {
            desiredWidthPixels = parseInt(desiredWidth);
        }

        return desiredWidthPixels;
    }
})(window);

(function (window, undefined) {

    SwitchablePanels = function (options) {

        this.settings = $.extend({
            parentElementSelector: null,
            defaultPanelSelector: ":first"
        }, options);

        this.actualActivePanel = null;
        this.init();
    };


    SwitchablePanels.prototype.init = function () {
        $(this.settings.parentElementSelector).children().addClass("animated");
        $(this.settings.parentElementSelector).children().not(this.settings.defaultPanelSelector).hide();
        this.actualActivePanel = $(this.settings.defaultPanelSelector);

    };


    SwitchablePanels.prototype.switchTo = function (panelSelector) {
        var switching = $.Deferred();

        this.actualActivePanel.hide();
        this.actualActivePanel.find(".select2-container").each(function (index, element) {
            select2ElementId = $(this).attr('id').replace(/.*_/g, "");
            $('#' + select2ElementId).select2('close');
        });

        $(panelSelector)
            .addClass("preInAnimation")
            .css("display", '');

        Animator.animateIn(panelSelector, "fadeFromRight", function () {
            switching.resolve();
        });
        this.actualActivePanel = $(panelSelector);

        return switching.promise();
    };

    SwitchablePanels.prototype.add = function (panel) {
        $(panel)
            .addClass("animated")
            .hide()
            .appendTo(this.settings.parentElementSelector);

        if (this.actualActivePanel.length == 0) {
            this.switchTo(panel);
        }
    };

    SwitchablePanels.prototype.getActivePanel = function () {
        return this.actualActivePanel.selector;
    }


    window.SwitchablePanels = SwitchablePanels;
})(window);
(function ($) {
    $.fn.employeeFullCalendar = function (fcOptions, arg1) {
        if (fcOptions == "setDaysReducing") {
            this.data("daysReducingLeavePool", arg1);
        } else if (fcOptions == "setHolidayManager") {
            this.data("holidayManager", arg1);
        } else {
            this.addClass("EmployeeCalendar");

            var now = moment();
            var _this = this;
            var showCalendarName = false;

            var calculatedFCOptions = $.extend({
                header: {
                    left: 'title',
                    right: 'prev,next today'
                },

                timezone: 'local',
                firstDay: DateTimeSettings.getFirstDayOfWeek(),
                handleWindowResize: true,
                theme: false,

                _dayRender: function (date, cell) {
                }
            }, fcOptions);

            this.data("daysReducingLeavePool", fcOptions.daysReducingLeavePool);
            this.data("holidayManager", fcOptions.holidayManager);

            if (fcOptions != null) {
                if (fcOptions.loaderSelector != null) {
                    calculatedFCOptions.loading = function (isLoading, view) {
                        if (isLoading)
                            $(fcOptions.loaderSelector).show();
                        else
                            $(fcOptions.loaderSelector).hide();
                    }
                }

                if (fcOptions.holidayManager != null) {
                    if (fcOptions.holidayManager.countCalendars(false) > 1) {
                        showCalendarName = true;
                    }
                }

                calculatedFCOptions.dayRender = function (date, cell) {



                    var dateString = date.format('YYYY-MM-DD');


                    if (date.isBefore(now)) {
                        $(cell).addClass("calendarPast");
                    } else {
                        var daysReducing = null;
                        var d = _this.data("daysReducingLeavePool");

                        if ($.isFunction(d)) {
                            daysReducing = d();
                        } else if (d != null) {
                            daysReducing = d;
                        }

                        if (daysReducing != null) {
                            if (!daysReducing.isWorkingDay(date.isoWeekday())) {
                                this.el.find(".fc-day-top").filter("[data-date='" + dateString + "']").addClass("calendarNonWorkingDay");
                            }
                        }
                    }


                    var holidayManager = null;
                    var h = _this.data("holidayManager");

                    if ($.isFunction(h)) {
                        holidayManager = h();
                    } else if (h != null) {
                        holidayManager = h;
                    }

                    if (holidayManager != null) {
                        var holiday = holidayManager.getHolidayForDate(date);

                        if (holiday != null && holiday != undefined) {
                            var holidayContent = "";

                            for (var i = 0; i < holiday.length; i++) {
                                holidayContent += '<div class="calendarHolidayTooltip" >';
                                holidayContent += holiday[i].name;

                                if (showCalendarName) {
                                    holidayContent += '<span class="holidayCalendarName">' + holiday[i].calendarName + '</span>';
                                }

                                holidayContent += "</div>";
                            }

                            this.el.find(".fc-day-top").filter("[data-date='" + dateString + "']")
                                .addClass("calendarHoliday")
                                .attr("data-tooltip", "true")
                                .attr("data-tooltip-content", holidayContent);

                            $(cell)
                                .attr("data-tooltip", "true")
                                .attr("data-tooltip-content", holidayContent);


                        }
                    }
                    calculatedFCOptions._dayRender(date, cell);

                }
            }

            return this.fullCalendar(calculatedFCOptions);
        }
    }
}(jQuery));
(function ($) {
    $.fn.holidayFullCalendar = function (fcOptions) {
        var now = moment();

        var calculatedFCOptions = $.extend({
            header: {
                left: 'title',
                right: 'prev,next today'
            },

            timezone: 'local',
            firstDay: DateTimeSettings.getFirstDayOfWeek(),
            handleWindowResize: true,
            theme: true,

            _dayRender: function (date, cell) {

            }
        }, fcOptions);


        if (fcOptions != null) {

            if (fcOptions.loaderSelector != null) {
                calculatedFCOptions.loading = function (isLoading, view) {
                    if (isLoading)
                        $(fcOptions.loaderSelector).show();
                    else
                        $(fcOptions.loaderSelector).hide();
                }

                calculatedFCOptions.viewDestroy = function () {
                    $(fcOptions.loaderSelector).show();
                }
            }

            calculatedFCOptions.dayRender = function (date, cell) {
                calculatedFCOptions._dayRender(date, cell);
            }
        }

        return this.fullCalendar(calculatedFCOptions);
    }
}(jQuery));
(function (window, undefined) {
    ProgressBar = function (options) {
        var _this = this;

        this.settings = $.extend({
            selector: "",
            initialValue: 0,
            maxValue: 100,
            showPercents: true
        }, options);

        this.domProgressBar = $(this.settings.selector);
        this.domProgressInside = $("<div />");
        this.value = 0;

        this.domProgressBar
            .empty()
            .addClass("ProgressBar")
            .append(this.domProgressInside);

        this.setValue(this.settings.initialValue);
    };

    ProgressBar.prototype.setValue = function (value) {
        this.value = value < this.settings.maxValue ? value : this.settings.maxValue;

        var percentage = Math.round((this.value / this.settings.maxValue) * 100) + "%";

        if (this.settings.showPercents) {
            this.domProgressInside.text(percentage);
        }

        this.domProgressInside.css("width", percentage);
    }

    window.ProgressBar = ProgressBar;
})(window);

(function (window, undefined) {

    PopupMenu = function (options) {
        var _this = this;

        this.settings = $.extend({
            container: null,
            destroyOnLocationUnload: true,

            position: "absolute",// fixed, absolute-center-attach
            attachTo: "",
            displayIcons: true,
            class: "",
            minwidth: null,
            overlay: false
        }, options);

        this.container = null;
        this.attachTo = null;
        this.opened = false;
        this.overlayInfo = null;

        this.init();
    };

    PopupMenu.prototype.init = function () {
        var _this = this;

        if (_this.settings.container != null) {
            _this.container = $(_this.settings.container);
        } else {
            _this.container = $('<div style="display: none;" tabindex="0"/>').appendTo("body");
        }

        if (_this.settings.minwidth != null)
            _this.container.css("min-width", "300px");

        _this.container.addClass("PopupMenu");
        _this.attachTo = $(_this.settings.attachTo);

        if (_this.settings.position == "fixed") {
            _this.container.css("position", "fixed");
        } else if (this.settings.position == "absolute-center-attach") {
            _this.container.css("position", "absolute");
        }

        if (_this.settings.destroyOnLocationUnload) {
            $(window.eventBus).one('Location.unloading', function () {
                _this.destroy();
            });
        }

        if (_this.settings.displayIcons == false) {
            _this.container.addClass("noIcon");
        }


        _this.container.addClass(_this.settings.class);

        _this.container.on("click", "a", function () {
            _this.hide();
        });

        _this.container.on("mouseleave", function () {
            if(_this.settings.overlay==false){
                _this.hide();
            }
        });

        _this.container.focus(function () {
        });

        _this.container.blur(function () {
            if(_this.settings.overlay==false){
                _this.hide();
            }
        });
    }

    PopupMenu.prototype.open = function () {
        if (!this.opened) {
            this.opened = true;

            var menuTop;
            var menuHeight;
            var left;

            if (this.settings.position == "fixed") {
                var attachedToOffset = this.attachTo.offset();
                var attachedToScroll = $(document).scrollTop();
                var attachedToHeight = this.attachTo.outerHeight();

                var menuTop = (5 + attachedToOffset.top + attachedToHeight - attachedToScroll );
                left = attachedToOffset.left;

                this.container.css("top", menuTop + "px");
                this.container.css("left", left + "px");
            } else if (this.settings.position == "absolute-center-attach") {
                var attachedToOffset = this.attachTo.offset();
                var attachedToWidth = this.attachTo.actual('outerWidth');
                var attachedToHeight = this.attachTo.actual('outerHeight');

                var realPopupMenuWidth = this.container.actual('outerWidth');
                var realPopupMenuHeight = this.container.actual('outerHeight');

                left = attachedToOffset.left + (attachedToWidth / 2) - (realPopupMenuWidth / 2);
                menuTop = attachedToOffset.top + (attachedToHeight / 2) - (realPopupMenuHeight / 2);

                this.container.css("top", menuTop);
                this.container.css("left", left + "px");
            }


            if(this.settings.overlay){
                this.overlayInfo = Overlay.show();

                $(this.container)
                    .css('z-index', this.overlayInfo.zindex + 1);
            }

            var topContentBoundary = 85;
            var topOverflow = false;
            var bottomOverflow = false;
            var rightOverflow = false;

            if (menuTop < topContentBoundary) {
                topOverflow = true;
            }

            if(topOverflow){
                if (topContentBoundary + this.container.outerHeight() > window.innerHeight) {
                    bottomOverflow = true;
                }
            }else{
                if (this.container.position().top + this.container.outerHeight() > window.innerHeight) {
                    bottomOverflow = true;
                }
            }

            if(topOverflow){
                menuTop = topContentBoundary;
                this.container.css("top", topContentBoundary+"px");
            }

            if(bottomOverflow){
                this.container.css("height", window.innerHeight - menuTop - 10);
            }


            if (this.attachTo.offset().left + this.container.outerWidth() > document.body.clientWidth) {
                rightOverflow = true;
            }

            if(rightOverflow){
                var tooMuch = (this.attachTo.offset().left + this.container.outerWidth()) - document.body.clientWidth;
                left = left - tooMuch;
                this.container.css("left", left + "px");
            }

            this.container.css("display", "block");

            var _this = this;
            setTimeout(function () {
                _this.container.focus();
            }, 50);
        }
    }

    PopupMenu.prototype.hide = function () {
        if (this.opened) {
            this.opened = false;
            this.container.hide();

            if(this.overlayInfo!=null){
                Overlay.hide();
                this.overlayInfo = null;
            }
        }
    }

    PopupMenu.prototype.toggle = function () {
        if (this.opened)
            this.hide();
        else
            this.open();
    }

    PopupMenu.prototype.destroy = function () {
        this.container.remove();
    }

    PopupMenu.prototype.addItem = function (item) {

        var newItem = $("<a />")
            .text(item.label);

        if (item.id != null)
            newItem.attr('id', item.id);

        if (item.data != null)
            newItem.data("itemData", item.data)

        if (item.class != null)
            newItem.addClass(item.class);

        if(item.decorator!=null){
            newItem.append(item.decorator);
        }

        if(item.icon!=null){
            var icon = $("<span />").addClass("icon").addClass(item.icon);
            newItem.append(icon);
        }


        if ($.isFunction(item.onClick))
            newItem.click(function () {
                item.onClick.apply(this);
            });

        newItem.appendTo(this.container);
    }
    PopupMenu.prototype.setLabelText = function (item) {
        var _this = this;
        if (item.id != null)
            _this.container.find("#" + item.id).text(item.label);
    }

    PopupMenu.prototype.removeItem = function (item) {
        var _this = this;
        if (item.id != null)
            _this.container.find("#" + item.id).remove();
    }

    PopupMenu.prototype.hideItem = function (item) {
        var _this = this;
        if (item.id != null)
            _this.container.find("#" + item.id).css('display', 'none');
    }

    PopupMenu.prototype.showItem = function (item) {
        var _this = this;
        if (item.id != null)
            _this.container.find("#" + item.id).show();
    }

    PopupMenu.prototype.disableItem = function (item) {
        var _this = this;
        if (item.id != null) {
            _this.container.find("#" + item.id).addClass("disabled");
            _this.container.find("#" + item.id).unbind('click');
        }
    }

    PopupMenu.prototype.enableItem = function (item) {
        var _this = this;
        if (item.id != null) {
            _this.container.find("#" + item.id).removeClass("disabled");
            _this.container.find("#" + item.id).bind('click');
        }

    }

    PopupMenu.prototype.addSeparator = function () {
        var _this = this;

        $("<span />")
            .addClass("separator")
            .appendTo(_this.container);
    }

    window.PopupMenu = PopupMenu;
})(window);
(function (window, undefined) {

    PopupBox = function (options) {
        var _this = this;

        this.settings = $.extend({
            destroyOnLocationUnload: true,
            position: "absolute",// fixed, absolute-center-attach
            attachTo: "",
            class: "",
        }, options);

        this.container = null;
        this.contentContainer = null;
        this.attachTo = null;
        this.opened = false;

        this.init();
    };

    PopupBox.prototype.init = function () {
        var _this = this;

        _this.container = $('<div style="display: none;" tabindex="0"/>')
            .appendTo("body")
            .addClass("PopupBox");

        _this.contentContainer = $('<div />').appendTo(_this.container);
        _this.attachTo = $(_this.settings.attachTo);

        if (_this.settings.position == "fixed") {
            _this.container.css("position", "fixed");
        } else if (this.settings.position == "absolute-center-attach") {
            _this.container.css("position", "absolute");
        }

        if (_this.settings.destroyOnLocationUnload) {
            $(window.eventBus).one('Location.unloading', function () {
                _this.destroy();
            });
        }

        _this.container.addClass(_this.settings.class);
    }

    PopupBox.prototype.open = function () {
        if (!this.opened) {
            this.opened = true;

            if (this.settings.position == "fixed") {
                var attachedToOffset = this.attachTo.offset();
                var attachedToScroll = $(document).scrollTop();
                var attachedToHeight = this.attachTo.outerHeight();

                this.container.css("top", (attachedToOffset.top + attachedToHeight - attachedToScroll ) + "px");
                this.container.css("left", attachedToOffset.left + "px");
            } else if (this.settings.position == "absolute-center-attach") {
                var attachedToOffset = this.attachTo.offset();
                var attachedToWidth = this.attachTo.actual('outerWidth');
                var attachedToHeight = this.attachTo.actual('outerHeight');

                var realPopupMenuWidth = this.container.actual('outerWidth');
                var realPopupMenuHeight = this.container.actual('outerHeight');

                var menuLeft = attachedToOffset.left + (attachedToWidth / 2) - (realPopupMenuWidth / 2);
                var menuTop = attachedToOffset.top + (attachedToHeight / 2) - (realPopupMenuHeight / 2);

                this.container.css("top", menuTop);
                this.container.css("left", menuLeft + "px");
            }

            this.container.css("display", "block");
        }
    }

    PopupBox.prototype.hide = function () {
        if (this.opened) {
            this.opened = false;
            this.container.hide();
        }
    }

    PopupBox.prototype.toggle = function () {
        if (this.opened)
            this.hide();
        else
            this.open();
    }

    PopupBox.prototype.destroy = function () {
        this.container.remove();
    }

    PopupBox.prototype.setContent = function (content) {
        this.contentContainer.empty().append(content);
    }

    window.PopupBox = PopupBox;
})(window);
(function (window, undefined) {

    FlowPresentationStep = function () {
        var _this = this;

        this.uuid = Utils.uuid();
        this.stateContainer = $('<div class="FlowPresentation_State" />');
        this.indicatorContainer = $('<div class="FlowPresentation_Indicator" />');
        this.descriptionContainer = $('<div class="FlowPresentation_Desc" />');
        this.nextArrowContainer = $('<div class="FlowPresentation_NextArrow" />');
        this.numberContainer = $('<div class="FlowPresentation_Number" />');

        this.container =
            $('<div class="FlowPresentation_Step"></div>')
                .append(_this.stateContainer)
                .append(_this.indicatorContainer)
                .append(_this.descriptionContainer)
                .append(_this.nextArrowContainer)
                .append(_this.numberContainer)
                .append('<div class="FlowPresentation_Clear">&nbsp;</div>');
    }

    FlowPresentationStep.prototype.getContainer = function () {
        return this.container;
    }

    FlowPresentationStep.prototype.setDescription = function (desc) {
        this.descriptionContainer.html(desc);
        return this;
    }

    FlowPresentationStep.prototype.setAdvContent = function (selector) {
        this.descriptionContainer.empty();
        this.descriptionContainer.append(selector);
        return this;
    }

    FlowPresentationStep.prototype.setStatusText = function (txt) {
        this.descriptionContainer.html(txt);
        return this;
    }

    FlowPresentationStep.prototype.indicatorClass = function (clazz) {
        this.indicatorContainer.attr('class', 'FlowPresentation_Indicator ' + clazz);
        return this;
    }

    FlowPresentationStep.prototype.clearIndicatorClass = function () {
        this.indicatorContainer.attr('class', 'FlowPresentation_Indicator');
        return this;
    }

    FlowPresentationStep.prototype.setInactive = function () {
        this.container.addClass("FlowPresentation_StepInactive");
        return this;
    }

    FlowPresentationStep.prototype.setLastActive = function () {
        this.container.addClass("FlowPresentation_StepLastActive");
        return this;
    }

    FlowPresentationStep.prototype.setCurrent = function () {
        this.container.addClass("current");
        return this;
    }

    FlowPresentationStep.prototype.displayNumber = function (no) {
        this.numberContainer.text(no);
        return this;
    }
    /**/


    FlowPresentation = function () {
        var _this = this;
        this.steps = new Array();
        this.container = $('<div class="FlowPresentation"/>');
        this.appendedTo = null;
        this.displayingStepsNumbers = false;


        this.movingDropTarget = null;
        this.movingDraggingElement = null;

        this.onSortEnd = function(oldPosition, newPosition, step){
        };

        this.onChanged = function(){
        };

        this.stepDraggingGhost = $('<div class="FlowPresentation_StepDraggingGhost" />');
    };

    FlowPresentation.prototype.appendTo = function (selector) {
        var _t = this;

        this.appendedTo = $(selector);
        this.appendedTo.append(_t.container);
    };

    FlowPresentation.prototype.addStep = function (desc) {
        var _this = this;

        var step = new FlowPresentationStep();
        step.setDescription(desc);

        this.steps.push(step);
        redisplayStepsNumbers(this);

        this.container.append(step.getContainer());
        this.onChanged();

        return step;
    };

    FlowPresentation.prototype.addStepAdvContent = function (contentSelector) {
        var _this = this;

        var step = new FlowPresentationStep();
        step.setAdvContent(contentSelector);

        this.steps.push(step);
        redisplayStepsNumbers(this);

        this.container.append(step.getContainer());
        this.onChanged();

        return step;
    };

    FlowPresentation.prototype.removeStep = function (step) {
        var stepToRemove = null;
        var stepToRemoveIndex = null;

        for (var i = 0; i < this.steps.length; i++) {
            var s = this.steps[i];

            if (s.uuid == step.uuid) {
                stepToRemove = s;
                stepToRemoveIndex = i;
            }
        }

        if (stepToRemoveIndex != null) {
            stepToRemove.container.remove();
            this.steps.splice(stepToRemoveIndex, 1);
            redisplayStepsNumbers(this);
            this.onChanged();
        }
    }

    FlowPresentation.prototype.isLastStep = function (step) {
        var lastStep = this.getLastStep();

        if (lastStep != null) {
            return lastStep.uuid == step.uuid;
        } else {
            return false;
        }
    }

    FlowPresentation.prototype.hideStatusColumn = function () {
        this.container.addClass("hiddenStatusColumn");
    };

    FlowPresentation.prototype.displayStepsNumbers = function () {
        this.displayingStepsNumbers = true;
        redisplayStepsNumbers(this);
    };

    FlowPresentation.prototype.hideStepsNumbers = function () {
        this.displayingStepsNumbers = false;
        clearStepsNumbers(this);
    };

    FlowPresentation.prototype.sortable = function (changeTo) {
        if (changeTo) {
            this.enableSorting();
        } else {
            this.disableSorting();
        }
    };

    FlowPresentation.prototype.enableSorting = function () {
        var _this = this;


        interact('.FlowPresentation_Step', {
            context: _this.container[0]
        })
            .dropzone({
                enabled: true
            })
            .on('dragmove', function (event) {
                var draggingStep = _this.movingDraggingElement,
                    dropzone = _this.movingDropTarget;

                if (draggingStep != null && dropzone != null) {
                    setDraggingStepPossitionDurringDragging(_this, draggingStep, event);

                    var dropzoneOffset = dropzone.offset();

                    if (event.pageY > dropzoneOffset.top + dropzone.outerHeight() / 2) {
                        draggingStep.insertAfter(dropzone);
                        _this.stepDraggingGhost.insertAfter(dropzone);
                    } else {
                        draggingStep.insertBefore(dropzone);
                        _this.stepDraggingGhost.insertBefore(dropzone);
                    }
                }
            })
            .on('dragenter', function (event) {
                _this.movingDropTarget = $(event.target);
            })
            .draggable({
                onstart: function (event) {
                    _this.movingDraggingElement = $(event.target);

                    var draggingStep = $(event.target);
                    var draggingStepInitialIndex = _this.container.find(".FlowPresentation_Step").index(draggingStep);

                    draggingStep.data("drag-start-as", draggingStepInitialIndex);

                    _this.stepDraggingGhost.css({
                        width: draggingStep.outerWidth(),
                        height: draggingStep.outerHeight()
                    }).insertBefore(draggingStep);

                    draggingStep.css({
                        "position": "absolute",
                        width: draggingStep.css("width"),
                        height: draggingStep.css("height")
                    });

                    setDraggingStepPossitionDurringDragging(_this, draggingStep, event);
                },
                onend: function (event) {
                    var draggingStep = $(event.target);

                    var newIndex = _this.container.find(".FlowPresentation_Step").index(draggingStep);
                    var oldIndex = draggingStep.data("drag-start-as");

                    draggingStep.attr("style", "");
                    _this.stepDraggingGhost.remove();

                    Utils.arrayMove(_this.steps, oldIndex, newIndex);
                    redisplayStepsNumbers(_this);

                    _this.onSortEnd(oldIndex, newIndex, draggingStep);
                    _this.onChanged();
                }
            })
            .ignoreFrom('input, textarea, .select2-choices, button')
            .restrict({
                drag: "parent",
                endOnly: true
            });
    }

    FlowPresentation.prototype.disableSorting = function () {
        //todo
    }

    FlowPresentation.prototype.stepsCount = function () {
        return this.steps.length;
    }

    FlowPresentation.prototype.getLastStep = function () {
        if (this.steps.length > 0) {
            return this.steps[this.steps.length - 1];
        } else {
            return null;
        }
    }

    FlowPresentation.prototype.getSteps = function () {
        return this.steps.slice();
    }

    FlowPresentation.prototype.setOnSortEnd = function (callback) {
        return this.onSortEnd = callback;
    }

    FlowPresentation.prototype.setOnChanged = function (callback) {
        return this.onChanged = callback;
    }

    function redisplayStepsNumbers(flowPresentation) {
        if (flowPresentation.displayingStepsNumbers) {
            for (var i = 0; i < flowPresentation.steps.length; i++) {
                var step = flowPresentation.steps[i];
                var stepNo = i + 1;

                if (step != null) {
                    step.displayNumber(stepNo);
                } else {
                    //console.error("i=", step);
                    //console.info(flowPresentation.steps);
                }
            }
        }
    }

    function clearStepsNumbers(flowPresentation) {
        for (var i = 0; i < flowPresentation.steps.length; i++) {
            var step = flowPresentation.steps[i];
            var stepNo = i + 1;

            step.displayNumber("");
        }
    }

    function setDraggingStepPossitionDurringDragging(_this, draggingStep, event) {
        var containerOffset = _this.container.offset();
        var containerX = containerOffset.left;
        var containerY = containerOffset.top;

        draggingStep.css({
            top: (event.pageY - containerY) + "px",
            left: (event.pageX - containerX) + "px"
        });
    }

    window.FlowPresentation = FlowPresentation;
})(window);
(function (window, undefined) {

    ProblemsBadge = function () {
        this.appendedTo = null;
        this.container = $('<div class="ProblemsBadge" />').hide();
        this.loading = null;
    };

    ProblemsBadge.prototype.appendTo = function (appendedTo) {
        this.appendedTo = appendedTo;
        this.container.appendTo(appendedTo);
    };

    ProblemsBadge.prototype.setProblems = function (problems) {
        if(_.isArray(problems)){
            if(problems.length > 0){
                this.container.css("display", "");
            }else{
                this.container.hide();
            }
        }else if(_.isObject(problems)){
            var problemsBag = problems;

            if (problemsBag.level != "NO_PROBLEM") {
                this.container.css("display", "");
            } else {
                this.container.hide();
            }
        }else{
            this.container.hide();
        }
    };

    ProblemsBadge.prototype.findProblemsOfContext = function(ctx){
        var _this  = this;

        _this.loading = Ajax.post({
            url: URL2.get("/webapi/problems/find/" + ctx),
            contentType: "application/json"
        }).done(function(problemsBag){
            _this.setProblems(problemsBag);
        });
    }

    ProblemsBadge.prototype.destroy = function(){
        if(this.loading!=null){
            this.loading.abort();
        }
    }

    ProblemsBadge.prototype.makeAutoDistroyable = function(){
        var _this = this;

        $(window.eventBus).one('Location.unloading', function () {
            _this.destroy();
        });
    }

    window.ProblemsBadge = ProblemsBadge;
})(window);
(function (window, undefined) {

    ProblemsModule = function () {
        this.appendedTo = null;
        this.problemsBag = null;

        this.container = $('<div class="ProblemsModule"><span class="badge"></span></div>').hide();
        this.container_legend = $('<div class="title"/>').text(Messages.get("common.problems")).appendTo(this.container);
        this.list = $('<ul/>').appendTo(this.container);
        this.container_legend2 = $('<div class="title"/>').text(Messages.get("common.problem.members")).appendTo(this.container);
        this.details = $('<ul/>').appendTo(this.container);

    };

    ProblemsModule.prototype.appendTo = function (appendedTo) {
        this.appendedTo = appendedTo;
        this.container.appendTo(appendedTo);
    };

    ProblemsModule.prototype.setProblems = function (problemsBag) {
        this.problemsBag = problemsBag;

        if (this.problemsBag.problems.length > 0) {
            this.list.empty();
            this.details.empty();

            for (var i = 0; i < this.problemsBag.problems.length; i++) {
                var problem = this.problemsBag.problems[i];
                var item = $("<li />").text(problem.message).appendTo(this.list);
                var details = $("<li />").text(problem.details).appendTo(this.details);
            }

            this.container.css("display", "");
        } else {
            this.container.hide();
        }
    };


    window.ProblemsModule = ProblemsModule;
})(window);
(function (window) {

    RequestFuturePresentation = function () {
        this.flowPresentation = new FlowPresentation();
        this.flowPresentation.hideStatusColumn();
        this.pending;
    };

    RequestFuturePresentation.prototype.init = function () {
        return $.Deferred().resolve().promise();
    }

    RequestFuturePresentation.prototype.appendTo = function (selector) {
        this.flowPresentation.appendTo(selector);
    };

    RequestFuturePresentation.prototype.loadData = function (data) {
        var _this = this;
        _this.pending = data.pending;

        if (data.pending) {
            for (var i = 0; i < data.steps.length; i++) {
                var step = data.steps[i];
                var stepWidget = _this.flowPresentation.addStep("");
                var stepDescription = "";

                if (step.current) {
                    stepWidget.setCurrent();

                    stepDescription = "<b>"+Messages.get("RequestFuturePresentation.nowWaiting") + "</b> ";
                }

                var alternatives = new Array();
                for (var y = 0; y < step.employees.length; y++) {
                    alternatives.push(_.escape(step.employees[y].fullName));
                }

                for (var y = 0; y < step.teams.length; y++) {
                    alternatives.push(_.escape(step.teams[y].name));
                }

                stepDescription = stepDescription + alternatives.join(" " + Messages.get("common.or") + " ");
                stepWidget.setDescription(stepDescription);
            }
        }


        return $.Deferred().resolve().promise();
    }

    RequestFuturePresentation.prototype.loadAbsenceRequest = function (id) {
        var _this = this;

        return _this.init().then(function(){
            return Ajax.post({
                url: URL2.get("/absence/absence/getAbsenceRequestFuturePresentation.do"),
                data: JSON.stringify({
                    id: id
                }),
                contentType: "application/json"
            }).done(function (response) {
                _this.loadData(response);
            })
        });
    };

    RequestFuturePresentation.prototype.isPending = function () {
        return this.pending;
    };



})(window);

(function (window) {

    OneComment = function (comment) {

(function (window) {

    AbsenceCommentsPresentation = function () {
        this.widget = new CommentsPresentation();
        this.comments = null;
    };

    AbsenceCommentsPresentation.prototype.loadData = function (requestId) {
        var _this = this;

        return Dependency3.loadScript(URL2.get("/js/manager/AbsenceRequestManager.js"))
        .then(function(){
                var manager = new AbsenceRequestManager();

                return manager.getComments(requestId).done(function (comments) {
                    if (comments != null && comments.length > 0) {
                        _this.widget.loadData(comments);
                        _this.comments = comments;
                    }
                });
        });

    };

    AbsenceCommentsPresentation.prototype.appendTo = function (selector) {
        this.widget.appendTo(selector);
    };

    AbsenceCommentsPresentation.prototype.hasComments = function () {
        return (this.comments != null && this.comments.length > 0);
    };


    window.AbsenceCommentsPresentation = AbsenceCommentsPresentation;
})(window);

(function (window, undefined) {

    RightPanel = function (options) {
        this.visible = false;
        this.width = "50%";
        this.onUnloadIsSet = false;

        this.container = $('<div class="rightPanel animated preInAnimation" style="width: 300px; display: none;" />')
            .appendTo("body");

        this.contentContainer = $('<div class="rightPanelContent"/>').appendTo(this.container);
    };

    RightPanel.prototype.showURL = function (url) {
        var _this = this;

        if (_this.visible) {
            return showContentURL.call(_this, url);
        } else {
            return slideIn.call(_this).then(function () {
                return showContentURL.call(_this, url);
            });
        }
    };

    RightPanel.prototype.hide = function () {
        var _this = this;

        if (_this.visible) {
            return slideOut.call(_this);
        } else {
            return $.Deferred().resolve().promise();
        }
    };

    RightPanel.prototype.hideOnLocationChange = function () {
        var _this = this;

        if (!_this.onUnloadIsSet) {
            _this.onUnloadIsSet = true;

            $(window.eventBus).one('Location.unloading', function () {
                RightPanel.hide();
                _this.onUnloadIsSet = false;
            });
        }
    };

    RightPanel.prototype.setWidth = function (width) {
        this.width = width;

        if (this.visible) {
            this.container
                .css("width", deriveDesiredPixelsWidth(this.width) + "px");
        }
    };

    //priv


    function slideIn() {
        this.visible = true;

        this.container
            .css("width", deriveDesiredPixelsWidth(this.width) + "px");

        var showing = $.Deferred();

        Animator.animateIn(this.container, "fullSlideRight", function () {
            showing.resolve();
        });

        return showing.promise();
    }

    function slideOut() {
        this.visible = false;

        var hiding = $.Deferred();

        Animator.animateOut(this.container, "fullSlideRight", function () {
            hiding.resolve();
        });

        return hiding.promise();
    }

    function showContentURL(url) {
        var _this = this;

        return Ajax.post({
            url: url,
            contentType: "text/html",
            dataType: "html"
        }).done(function (loadedContent) {
            _this.contentContainer.html(loadedContent);
        });
    }

    function deriveDesiredPixelsWidth(desiredWidth) {
        var windowWidth = $("body").innerWidth();
        var desiredWidthPixels = 0;

        if (_.isString(desiredWidth) && desiredWidth.indexOf("%") > -1) {
            desiredWidthPixels = (parseInt(desiredWidth) / 100.0) * windowWidth;
        } else {
            desiredWidthPixels = parseInt(desiredWidth);
        }

        return desiredWidthPixels;
    }

    window.RightPanel = new RightPanel();
})(window);
(function (window, undefined) {
    ContextHelpIcon = function () {
    };

    ContextHelpIcon.prototype.toolTip = function (selector, text) {
        var element = $(selector);
        ToolTip.setTip(element, text, true, false);
    }

    ContextHelpIcon.prototype.iconInLocation = function (options) {
        var settings = $.extend({
            selector: null,
            subject: null,
            width: null
        }, options);

        var element = $(settings.selector);

        element.addClass("contextHelpIco");

        if (settings.subject != null) {
            RightPanel.hideOnLocationChange();
            var url = figureOutUrl(settings.subject);

            element.click(function () {
                if (settings.width != null) {
                    RightPanel.setWidth(settings.width);
                }

                RightPanel.showURL(url);
            });
        }

        return settings;
    };

    function figureOutUrl(subject){
        return URL2.get("/contextHelp.do?subject=" + encodeURIComponent(subject));
    }

    window.ContextHelpIcon = new ContextHelpIcon();
})(window);
(function (window, undefined) {

    //--- structure classes
    SmartTableRow = function (id) {
        this.id = id;
        this.headerContent = $('<div />');
        this.summaryContent = $('<div />');
        this.data = null;

        this.cells = [];

        this.height = null;//null means default
        this.index = null;
    }

    SmartTableCell = function () {
        this.cellContainer = null;
        this.content = $('<div />');
        this.cssClass = '';
        this.data = null;
    }

    SmartTablleHeaderCell = function () {
        this.content = $('<div />');
    }

    //--- smart table
    SmartTable = function (options) {
        var _this = this;

        //container
        this.appendedTo = null;

        //data gui
        this.columnsWidthMode = "EQUALS";//SEPARATE
        this.columnsWidthsSeparate = [];
        this.columnWidth = 90;
        this.rowHeight = 80;
        this.columnHeaderWidth = 100;
        this.columnSummaryWidth = 100;
        this.headerHeight = 50;
        this.headerColumnTitle = '';
        this.summaryColumnTitle = '';
        this.highlightingContentHeader = false;
        this.selectable = false;
        this.selectableMode = 'CELLS';//ROWS

        //structure
        this.containerTable = $('<div class="SmartTable lockSelecting lockDragging" />');
        this.containerLeftPart = $('<div class="SmartTable-LeftPart" />').appendTo(this.containerTable);
        this.containerLeftPartHeader = $('<div class="SmartTable-Header" />').appendTo(this.containerLeftPart);
        this.containerLeftPartBody = $('<div class="SmartTable-LeftPart-Body" />').appendTo(this.containerLeftPart);
        this.containerRightPart = $('<div class="SmartTable-RightPart" />').appendTo(this.containerTable);
        this.containerRightPartHeader = $('<div class="SmartTable-Header" />').appendTo(this.containerRightPart);
        this.containerRightPartBody = $('<div class="SmartTable-LeftPart-Body" />').appendTo(this.containerRightPart);
        this.containerContentPart = $('<div class="SmartTable-ContentPart" />').appendTo(this.containerTable);
        this.containerContentTable = $('<div class="SmartTable-ContentTable" />').appendTo(this.containerContentPart);
        this.containerContentTableHeader = $('<div class="SmartTable-Header" />').appendTo(this.containerContentTable);
        this.containerContentTableBody = $('<div class="SmartTable-Body" />').appendTo(this.containerContentTable);
        this.containerContentTableBodyScrolling = $('<div class="SmartTable-ScrollingContainer" />').appendTo(this.containerContentTableBody);
        this.containerContentTableHeaderRow = $('<div class="SmartTable-Header-Row" />').appendTo(this.containerContentTableHeader);
        this.containerContentTableHeaderRowScrolling = $('<div class="SmartTable-ScrollingContainer" />').appendTo(this.containerContentTableHeaderRow);
        this.containerContentTableHeaderScroller = $('<div class="VerticalScroll-ScrollerContainer" />').appendTo(this.containerContentTableHeader);
        this.containerContentTableHeaderScrollBar = $('<div class="VerticalScroll-ScrollBar" />').appendTo(this.containerContentTableHeaderScroller);

        this.spinner = new Spinner();
        this.spinner.dark();
        this.spinner.withBackground();
        this.spinner.pinTo(this.containerTable);

        //data
        this.columnsNumber = 0;
        this.rows = [];
        this.rowsByIds = {};
        this.columnsHeaders = [];
        this.onCellClickHandler = function (x, y, data) {

        }
        this.onCellSelectedHandler = function (x, y, data) {

        }
        this.onCellDeselectedHandler = function (x, y, data) {

        }
        this.onRowSelectedHandler = function (rowNumber, data) {

        }
        this.onRowDeselectedHandler = function (rowNumber, data) {

        }

        this.onColumnHeaderClickHandlner = function (colNumber) {

        }


        this.columnHeaderClickable = true;

        this.selectedCell = null;
        this.selectedRow = null;


        //scrolling
        this.scrollVisible = false;
        this.scrollingRatio = 1;
        this.maxScrollPosition = 0;

        this.setHeaderHeight(50);
        this.setHeaderColumnWidth(200);
        this.setSummaryColumnWidth(200);
        this.setRowHeight(70);
        this.showSummaryColumn();
    };

    SmartTable.prototype.init = function () {
        var _this = this;

        setTimeout(function () {
            _this.updateScrollBar();
        }, 0);

        $(window).on("resize", function () {
            _this.updateScrollBar();
        });

        interact('.VerticalScroll-ScrollBar', {context: _this.containerTable[0]}).draggable({
            onmove: function (event) {
                _this.adjustScrollPosition(event.dx);
            },
            axis: 'x',
            inertia: true
        });

        this.containerContentTableHeader.on('mousewheel', function (event) {
            if (_this.scrollVisible == true) {
                _this.adjustScrollPosition(-event.deltaY * event.deltaFactor);
                event.preventDefault();
            } else {

            }
        });
    };

    SmartTable.prototype.setScrollPosition = function (x) {
        this.containerContentTableHeaderScrollBar.attr('data-x', x);
        $(this.containerContentTableHeaderScrollBar).css("left", x + "px");

        var left = -(x / this.scrollingRatio) + "px";
        $(this.containerContentTableBodyScrolling).css("left", left);
        $(this.containerContentTableHeaderRowScrolling).css("left", left);
    }

    SmartTable.prototype.adjustScrollPosition = function (x) {
        var _this = this;

        var newPos = (parseFloat(this.containerContentTableHeaderScrollBar.attr('data-x')) || 0) + x;

        if (newPos > _this.maxScrollPosition) {
            newPos = _this.maxScrollPosition;
        } else if (newPos < 0) {
            newPos = 0;
        }

        this.setScrollPosition(newPos);
    }

    SmartTable.prototype.updateScrollBar = function () {
        var contentWidth = this.containerContentTableHeaderRow.outerWidth();
        var viewportWidth = this.containerContentPart.innerWidth();
        var scrollingBarContainerWidth = this.containerContentTableHeaderRowScrolling.innerWidth();
        var scrollBarWidth = ( viewportWidth * (viewportWidth / contentWidth) );

        if (scrollBarWidth < 150) {
            scrollBarWidth = 150;
        }

        this.containerContentTableHeaderScroller.css("width", scrollingBarContainerWidth + "px");
        this.containerContentTableHeaderScrollBar.css("width", scrollBarWidth + "px");
        this.maxScrollPosition = viewportWidth - scrollBarWidth;
        this.scrollingRatio = this.maxScrollPosition / (contentWidth - viewportWidth);

        if (contentWidth - 5 > viewportWidth) {
            this.scrollVisible = true;
            this.containerContentTableHeaderScroller.fadeIn();
        } else {
            this.scrollVisible = false;
            this.containerContentTableHeaderScroller.hide();
        }

        this.setScrollPosition(0);
    }

    SmartTable.prototype.updateStick = function () {
        $(this.containerTable).find(".SmartTable-Header").trigger("stick:recaltulate");
    }

    SmartTable.prototype.destroy = function () {
        var _this = this;
    };

    SmartTable.prototype.appendTo = function (selector) {
        var _this = this;

        this.appendedTo = $(selector);
        this.appendedTo.append(_this.containerTable);

        $(this.containerTable).find(".SmartTable-Header").stick({
            offsetTop: 130
        });

        $(this.containerContentTableHeader).on("stick:sticked", function (e) {
            var header = e.target;
            var contentContainer = $(_this.containerContentPart);
            $(header).innerWidth(contentContainer.innerWidth());
        });
    };

    SmartTable.prototype.setClass = function (c) {
        this.containerTable.addClass(c);
    }

    SmartTable.prototype.unsetClass = function (c) {
        this.containerTable.removeClass(c);
    }

    SmartTable.prototype.clearSelection = function (preventEvent) {
        if (this.selectedCell != null) {

            this.selectedCell.cellContainer.removeClass("selected");

            if (preventEvent !== true) {
                this.onCellDeselectedHandler(
                    this.selectedCell.cellContainer.data("cell-row"),
                    this.selectedCell.cellContainer.data("cell-col"),
                    this.selectedCell.data
                );
            }

            this.selectedCell = null;
        }

        if (this.selectedRow != null) {
            this.selectedRow.rowHeaderContainer.removeClass("selected");
            this.selectedRow.rowContentContainer.removeClass("selected");
            this.selectedRow.rowSummaryContainer.removeClass("selected");

            if (preventEvent !== true) {
                this.onRowDeselectedHandler(
                    this.selectedRow.rowNumber,
                    this.selectedRow.data
                );
            }

            this.selectedRow = null;
        }
    }

    SmartTable.prototype.selectCell = function (x, y) {
        var cell = this.rows[y].cells[x];

        var celki = this.rows[y].cells;
        var celka = celki[0];



        if (cell != null) {
            if (this.selectedCell != null && this.selectCell == cell) {

            } else {
                toggleCellSelection(this, cell);
            }


        }
    }

    SmartTable.prototype.selectRow = function (y) {
        if (this.selectableMode == "ROWS") {
            var row = this.rows[y];

            if (row != null) {
                if (this.selectedRow != null && this.selectedRow == row) {

                } else {
                    toggleRowSelection(this, row.id);
                }
            }
        }
    }

    //------------


    SmartTable.prototype.redrawDataRow = function (rowId) {
        var _this = this;
        var row = this.rowsByIds[rowId];

        //drawing row summaries
        var previousRowContainer = this.containerContentTableBodyScrolling.find(".SmartTable-Content-Row").eq(row.index);
        var headerRow = this.containerLeftPartBody.find(".SmartTable-Content-Row").eq(row.index);
        var summaryRow = this.containerRightPartBody.find(".SmartTable-Content-Row").eq(row.index);




        summaryRow
            .empty()
            .append(row.summaryContent);


        //drawing content...
        var rowContainer = previousRowContainer;
        rowContainer.empty();




        var h = "";

        if (row.height == null) {
            var h = _this.rowHeight + "px";
        } else {
            var h = row.height + "px";
        }

        rowContainer[0].style.height = h;
        headerRow[0].style.height = h;
        summaryRow[0].style.height = h;


        for (var y = 0; y < this.columnsNumber; y++) {
            var definedCell = row.cells[y];

            if (definedCell != null) {
                var cellContainer = $('<div/>')
                    .addClass('SmartTable-Cell')
                    .data("cell-row", row.index)
                    .data("cell-col", y)
                    .data("cell-rowid", row.id)
                    .on("click", function (e) {
                        var cell = $(this);
                        var definedCell = _this.rows[cell.data("cell-row")].cells[cell.data("cell-col")];

                        if (_this.selectable) {
                            if (_this.selectableMode == "CELLS") {
                                toggleCellSelection(_this, definedCell);
                            }
                        }

                        if (_this.onCellClickHandler != null) {
                            _this.onCellClickHandler(cell.data("cell-row"), cell.data("cell-col"), definedCell.data);
                        }
                    });


                setCellWidth.call(this, y, cellContainer);
                cellContainer.appendTo(rowContainer);

                definedCell.cellContainer = cellContainer;

                setupHeaderHighlightingEvents(_this, cellContainer, y);

                if (definedCell != null) {
                    cellContainer.append(definedCell.content);

                    if (definedCell.cssClass != null && definedCell.cssClass != '') {
                        cellContainer.addClass(definedCell.cssClass);
                    }
                }
            }
        }

    }

    SmartTable.prototype.redraw = function () {
        var _this = this;

        if (this.columnHeaderClickable) {
            this.containerTable.addClass("columnsHeadersClickable");
        } else {
            this.containerTable.removeClass("columnsHeadersClickable");
        }

        //drawing columns headers
        this.containerContentTableHeaderRowScrolling.empty();

        for (var y = 0; y < this.columnsNumber; y++) {
            var predefinedColumnHeader = this.columnsHeaders[y];

            var cellContainer = $('<div/>')
                .addClass("SmartTable-Cell")
                .data("col", y);

            if (this.columnHeaderClickable) {
                cellContainer.on("click", function (e) {
                    var cellContainer = $(this);
                    _this.onColumnHeaderClickHandlner(cellContainer.data("col"));
                })
            }

            setCellWidth.call(this, y, cellContainer);

            if (predefinedColumnHeader != null) {
                cellContainer.append(predefinedColumnHeader.content);
            }

            cellContainer.appendTo(this.containerContentTableHeaderRowScrolling);
        }

        //drawing row headers
        this.containerLeftPartBody.empty();

        for (var i = 0; i < this.rows.length; i++) {
            var row = this.rows[i];
            var rowContainer = prepareRow(_this, i, row);
            row.rowHeaderContainer = rowContainer;

            rowContainer.append(row.headerContent);
            rowContainer.appendTo(this.containerLeftPartBody);
        }


        //drawing row summaries
        this.containerRightPartBody.empty();

        for (var i = 0; i < this.rows.length; i++) {
            var row = this.rows[i];
            var rowContainer = prepareRow(_this, i, row);
            row.rowSummaryContainer = rowContainer;

            rowContainer
                .append(row.summaryContent)
                .appendTo(this.containerRightPartBody);
        }


        //drawing content...
        this.containerContentTableBodyScrolling.empty();
        this.containerContentTable.css("width", (Math.max(this.containerContentPart.innerWidth(), this.columnsNumber * this.columnWidth)) + "px");

        var allColumnsWidth = calculateAllColumntWidth.apply(this);
        this.containerContentTableHeaderRow.css("width", allColumnsWidth + "px");

        for (var i = 0; i < this.rows.length; i++) {
            var row = this.rows[i];

            var rowContainer = prepareRow(_this, i, row);
            row.rowContentContainer = rowContainer;

            for (var y = 0; y < this.columnsNumber; y++) {
                var definedCell = row.cells[y];

                if (definedCell != null) {
                    var cellContainer = $('<div/>')
                        .addClass("SmartTable-Cell")
                        .data("cell-row", i)
                        .data("cell-col", y)
                        .data("cell-rowid", row.id)
                        .on("click", function (e) {
                            var cell = $(this);
                            var definedCell = _this.rows[cell.data("cell-row")].cells[cell.data("cell-col")];

                            if (_this.selectable) {
                                if (_this.selectableMode == "CELLS") {
                                    toggleCellSelection(_this, definedCell);
                                }
                            }

                            if (_this.onCellClickHandler != null) {
                                _this.onCellClickHandler(cell.data("cell-row"), cell.data("cell-col"), definedCell.data);
                            }
                        });


                    setCellWidth.call(this, y, cellContainer);
                    cellContainer.appendTo(rowContainer);

                    definedCell.cellContainer = cellContainer;

                    setupHeaderHighlightingEvents(_this, cellContainer, y);

                    if (definedCell != null) {
                        cellContainer.append(definedCell.content);

                        if (definedCell.cssClass != null && definedCell.cssClass != '') {
                            cellContainer.addClass(definedCell.cssClass);
                        }
                    }
                } else {

                }
            }

            rowContainer.appendTo(this.containerContentTableBodyScrolling);
        }

        //scrollbars
        this.updateScrollBar();
        this.updateStick();
    };

    //------------

    SmartTable.prototype.setColumnsNumber = function (n) {
        this.columnsNumber = n;
    };

    SmartTable.prototype.turnOnHighlightContentHeader = function () {
        this.highlightingContentHeader = true;
    };

    SmartTable.prototype.setColumnsEqualWidthMode = function () {
        this.columnsWidthMode = "EQUALS";
        this.redraw();
    };

    SmartTable.prototype.setColumnsSeparateWidthMode = function () {
        this.columnsWidthMode = "SEPARATE";
        this.redraw();
    };

    SmartTable.prototype.clearSeparateColumnsWidth = function () {
        this.columnsWidthsSeparate = [];
        this.redraw();
    };

    SmartTable.prototype.setSeparateColumnWidth = function (col, width) {
        this.columnsWidthsSeparate[col] = width;
        this.redraw();
    };

    SmartTable.prototype.setColumnsWidth = function (w) {
        this.columnWidth = w;
        this.redraw();
    };

    SmartTable.prototype.setHeaderColumnWidth = function (w) {
        this.columnHeaderWidth = w;

        this.containerLeftPart.width(this.columnHeaderWidth);
        this.containerContentPart.css("margin-left", this.columnHeaderWidth + "px");

        this.redraw();
    };

    SmartTable.prototype.setSummaryColumnWidth = function (w) {
        this.columnSummaryWidth = w;

        this.containerRightPart.width(this.columnSummaryWidth);
        this.containerContentPart.css("margin-right", this.columnSummaryWidth + "px");

        this.redraw();
    };

    SmartTable.prototype.setHeaderHeight = function (h) {
        this.headerHeight = h;

        this.containerLeftPartHeader.height(this.headerHeight);
        this.containerRightPartHeader.height(this.headerHeight);
        this.containerContentTableHeader.height(this.headerHeight);
    };

    SmartTable.prototype.setRowHeight = function (h) {
        this.rowHeight = h;
        this.redraw();
    };

    SmartTable.prototype.setSelectable = function () {
        this.selectable = true;
    };

    SmartTable.prototype.withCellBorders = function () {
        this.containerTable.removeClass("noCellBorders");
    };

    SmartTable.prototype.withoutCellBorders = function () {
        this.containerTable.addClass("noCellBorders");
    };

    SmartTable.prototype.lightRows = function () {
        this.containerTable.addClass("lightRows");
    };

    SmartTable.prototype.zebraRows = function () {
        this.containerTable.removeClass("lightRows");
    };

    SmartTable.prototype.cellsSelecting = function () {
        this.setSelectable();
        this.selectableMode = "CELLS";
        this.containerTable.addClass("cellsHovering");
        this.containerTable.removeClass("rowsHovering");
    }

    SmartTable.prototype.rowsSelecting = function () {
        this.setSelectable();
        this.selectableMode = "ROWS";
        this.containerTable.addClass("rowsHovering");
        this.containerTable.removeClass("cellsHovering");
    }

    SmartTable.prototype.hideSummaryColumn = function () {
        //this.containerTable.removeClass("withSummary");
        this.containerRightPart.hide();
        this.containerContentPart.css("margin-right", "0px");

        this.redraw();
    }

    SmartTable.prototype.showSummaryColumn = function () {
        //this.containerTable.addClass("withSummary");
        this.containerRightPart.show();
        this.containerContentPart.css("margin-right", this.columnSummaryWidth + "px");

        this.redraw();
    }

    //------------

    SmartTable.prototype.clearData = function () {
        this.clearRows();

        this.columnsHeaders = [];
    }

    //------------

    SmartTable.prototype.addRow = function () {
        var rowId = generateRowId();

        var row = new SmartTableRow(rowId);

        var newLength = this.rows.push(row);
        row.index = newLength - 1;
        this.rowsByIds[rowId] = row;

        return rowId;
    }

    SmartTable.prototype.clearRows = function () {
        this.rows = [];
        this.rowsByIds = {};
    }

    SmartTable.prototype.setParticularRowHeight = function (row, height) {
        var row = this.rowsByIds[row];

        if (row != null) {
            row.height = height;
        }
    };

    SmartTable.prototype.setRowHeaderContent = function (row, content) {
        var row = this.rowsByIds[row];

        if (row != null) {
            row.headerContent = content;
        }
    }

    SmartTable.prototype.setRowSummaryContent = function (row, content) {
        var row = this.rowsByIds[row];

        if (row != null) {
            row.summaryContent = content;
        }
    }

    SmartTable.prototype.setColumnHeaderTitle = function (t) {
        this.headerColumnTitle = t;

        if (this.headerColumnTitle == null || this.headerColumnTitle == '') {
            this.containerLeftPartHeader.empty();
        } else {
            this.containerLeftPartHeader.html(this.headerColumnTitle);
        }
    }

    SmartTable.prototype.setColumnSummaryTitle = function (t) {
        this.summaryColumnTitle = t;

        if (this.summaryColumnTitle == null || this.summaryColumnTitle == '') {
            this.containerRightPartHeader.empty();
        } else {
            this.containerRightPartHeader.html(this.summaryColumnTitle);
        }
    }


    SmartTable.prototype.setCellContent = function (row, column, content) {
        var row = this.rowsByIds[row];

        if (row != null) {
            var cell = row.cells[column];

            if (cell == null) {
                cell = new SmartTableCell();
                row.cells[column] = cell;
            }

            cell.content = $(content);
        }
    }

    SmartTable.prototype.setCellHeader = function (column, content) {
        var header = this.columnsHeaders[column];

        if (header == null) {
            var header = new SmartTablleHeaderCell();
            this.columnsHeaders[column] = header;
        }

        header.content = $(content);
    }

    SmartTable.prototype.setCellClass = function (row, column, cssClass) {
        var row = this.rowsByIds[row];

        if (row != null) {
            var cell = row.cells[column];

            if (cell == null) {
                cell = new SmartTableCell();
                row.cells[column] = cell;
            }

            cell.cssClass = cssClass;
        }
    }

    SmartTable.prototype.setCellData = function (row, column, data) {
        var row = this.rowsByIds[row];

        if (row != null) {
            var cell = row.cells[column];

            if (cell == null) {
                cell = new SmartTableCell();
                row.cells[column] = cell;
            }

            cell.data = data;
        }
    }

    SmartTable.prototype.setRowData = function (row, data) {
        var row = this.rowsByIds[row];

        if (row != null) {
            row.data = data;
        }
    }


    SmartTable.prototype.getRowData = function (row) {
        var row = this.rowsByIds[row];

        if (row != null) {
            return row.data;
        } else {
            return null;
        }
    }


    SmartTable.prototype.onCellClick = function (f) {
        this.onCellClickHandler = f;
    }

    SmartTable.prototype.onCellSelect = function (f) {
        this.onCellSelectedHandler = f;
    }

    SmartTable.prototype.onCellDeselect = function (f) {
        this.onCellDeselectedHandler = f;
    }

    SmartTable.prototype.onRowSelect = function (f) {
        this.onRowSelectedHandler = f;
    }

    SmartTable.prototype.onRowDeselect = function (f) {
        this.onRowDeselectedHandler = f;
    }

    SmartTable.prototype.onColumnHeaderClick = function (f) {
        this.onColumnHeaderClickHandlner = f;
    }

    SmartTable.prototype.setColumnHeaderClickable = function (clickable) {
        this.columnHeaderClickable = clickable;
    }

    SmartTable.prototype.showSpinner = function () {
        this.spinner.showDelayed();
    }

    SmartTable.prototype.hideSpinner = function () {
        this.spinner.hide();
    }

    SmartTable.prototype.getVisibleRows = function () {
        var headerRect = this.containerLeftPartHeader[0].getBoundingClientRect();
        var headerBottom = headerRect.top + headerRect.height;
        var rows = this.containerLeftPartBody.find(".SmartTable-Content-Row");

        var visibleRows = [];

        for (var i = 0; i < rows.length; i++) {
            var row = rows[i];

            var rowRect = row.getBoundingClientRect();
            var rowBottom = rowRect.top + rowRect.height;

            if (rowBottom > headerBottom) {

                if (rowRect.top > window.innerHeight) {
                    break;
                }

                visibleRows.push($(row).data("rowId"));
            }
        }

        return visibleRows;
    };


    //------------------------------------------------------------------------------------------------------------------

    function generateRowId() {
        return 'r' + (Math.random().toString(36).substring(2));
    }

    function highlightRow(_this, row) {
        _this.containerTable.find(".SmartTable-Content-Row-" + row).addClass("hover");
    }

    function unhighlightRow(_this, row) {
        _this.containerTable.find(".SmartTable-Content-Row-" + row).removeClass("hover");
    }

    function prepareRow(_this, i, rowData) {
        var row = $('<div/>')
            .addClass("SmartTable-Content-Row")
            .addClass("SmartTable-Content-Row-" + i)
            .data("rowIndex", i)
            .data("rowId", rowData.id);

        if (rowData.height == null)
            row.height(_this.rowHeight);
        else
            row.height(rowData.height);


        row.mouseenter(function (event) {
            var target = $(event.target);

            if (!target.hasClass("SmartTable-Content-Row")) {
                target = target.closest(".SmartTable-Content-Row");
            }

            var rowIndex = target.data("rowIndex");

            highlightRow(_this, rowIndex);
        }).mouseleave(function (event) {
            var target = $(event.target);

            if (!target.hasClass("SmartTable-Content-Row")) {
                target = target.closest(".SmartTable-Content-Row");
            }

            var rowIndex = target.data("rowIndex");

            unhighlightRow(_this, rowIndex);
        }).click(function (event) {
            if (_this.selectable) {
                if (_this.selectableMode == "ROWS") {
                    var target = $(event.target);

                    if (!target.hasClass("SmartTable-Content-Row")) {
                        target = target.closest(".SmartTable-Content-Row");
                    }

                    toggleRowSelection(_this, target.data("rowId"));
                }
            }
        });

        return row;
    }

    function setupHeaderHighlightingEvents(_this, cellContainer, y) {
        cellContainer.mouseenter(function (event) {
            if (_this.highlightingContentHeader) {
                highlightContentColumn(_this, y);
            }
        }).mouseleave(function (event) {
            if (_this.highlightingContentHeader) {
                unhighlightContentColumn(_this, y);
            }
        });
    }

    function highlightContentColumn(_this, column) {
        _this.containerContentTableHeader.find(".SmartTable-Cell:eq(" + column + ")").addClass("hover");
    }

    function unhighlightContentColumn(_this, column) {
        _this.containerContentTableHeader.find(".SmartTable-Cell:eq(" + column + ")").removeClass("hover");
    }

    function toggleCellSelection(_this, cell) {
        if (cell == _this.selectedCell) {

            _this.selectedCell.cellContainer.removeClass("selected");

            _this.onCellDeselectedHandler(
                _this.selectedCell.cellContainer.data("cell-row"),
                _this.selectedCell.cellContainer.data("cell-col"),
                _this.selectedCell.data
            );

            _this.selectedCell = null;
        } else {
            if (_this.selectedCell != null) {
                _this.selectedCell.cellContainer.removeClass("selected");

                _this.onCellDeselectedHandler(
                    _this.selectedCell.cellContainer.data("cell-row"),
                    _this.selectedCell.cellContainer.data("cell-col"),
                    _this.selectedCell.data
                );

                _this.selectedCell = null;
            }

            _this.selectedCell = cell;
            _this.selectedCell.cellContainer.addClass("selected");

            _this.onCellSelectedHandler(
                _this.selectedCell.cellContainer.data("cell-row"),
                _this.selectedCell.cellContainer.data("cell-col"),
                _this.selectedCell.data
            );

        }
    }

    function toggleRowSelection(_this, rowId) {
        var row = _this.rowsByIds[rowId];

        if (row == _this.selectedRow) {
            _this.selectedRow.rowHeaderContainer.removeClass("selected");
            _this.selectedRow.rowContentContainer.removeClass("selected");
            _this.selectedRow.rowSummaryContainer.removeClass("selected");

            _this.onRowDeselectedHandler(
                _this.selectedRow.rowNumber,
                _this.selectedRow.data
            );

            _this.selectedRow = null;
        } else {
            if (_this.selectedRow != null) {
                _this.selectedRow.rowHeaderContainer.removeClass("selected");
                _this.selectedRow.rowContentContainer.removeClass("selected");
                _this.selectedRow.rowSummaryContainer.removeClass("selected");

                _this.onRowDeselectedHandler(
                    _this.selectedRow.rowNumber,
                    _this.selectedRow.data
                );

                _this.selectedRow = null;
            }

            _this.selectedRow = row;
            _this.selectedRow.rowHeaderContainer.addClass("selected");
            _this.selectedRow.rowContentContainer.addClass("selected");
            _this.selectedRow.rowSummaryContainer.addClass("selected");

            _this.onRowSelectedHandler(
                _this.selectedRow.rowNumber,
                _this.selectedRow.data
            );
        }
    }


    function setCellWidth(col, cellContainer) {
        if (this.columnsWidthMode == "EQUALS") {
            cellContainer.css("width", this.columnWidth + "px");
        } else {
            if (this.columnsWidthsSeparate[col] != null) {
                cellContainer.css("width", this.columnsWidthsSeparate[col] + "px");
            } else {
                cellContainer.css("width", this.columnWidth + "px");
            }
        }
    }


    function calculateAllColumntWidth() {
        if (this.columnsWidthMode == "EQUALS") {
            return this.columnsNumber * this.columnWidth;
        } else {
            var summaryWidth = 0;

            for (var i = 0; i < this.columnsNumber; i++) {
                if (this.columnsWidthsSeparate[i] != null) {
                    summaryWidth += this.columnsWidthsSeparate[i];
                } else {
                    summaryWidth += this.columnWidth;
                }
            }

            return summaryWidth;
        }
    }


    window.SmartTable = SmartTable;
})(window);
(function (window, undefined) {

    TimesheetTableDisplayModes = {
        SHIFTS: "SHIFTS",
        WORKED_TIME: "WTIME",
        PROJECTS: "PROJECTS"
    }

    TimesheetTable = function (manager) {
        var _this = this;

        this.timesheetManager = manager;

        this.filterDateRange = null;
        this.filterEmployees = [];
        this.filterTeams = [];
        this.mode = TimesheetTableDisplayModes.SHIFTS;

        this.reloadingPaused = false;

        this.standardColumnWidth = 110;
        this.standardRowHeight = 100;
        this.headerColumnWidth = 250;
        this.summaryColumnWidth = 140;
        this.appendedTo = null;

        this.smartTable = new SmartTable();
        this.smartTable.setClass("TimesheetTable");
        this.smartTable.setHeaderHeight(60);
        this.smartTable.setColumnsWidth(this.standardColumnWidth);
        this.smartTable.setHeaderColumnWidth(this.headerColumnWidth);
        this.smartTable.setSummaryColumnWidth(this.summaryColumnWidth);
        this.smartTable.setRowHeight(90);
        this.smartTable.turnOnHighlightContentHeader();
        this.smartTable.setSelectable();

        this.smartTable.onColumnHeaderClick(function (colNumber) {
            if (_this.frameData != null && _this.frameData.daysCount > 1) {
                var day = DateTimeTool.momentToStrutsDate(moment(_this.frameData.dateFrom).add(colNumber, 'days'));
                var range = {range: "CUSTOM", from: day, to: day, shift: 0};

                _this.dateRange(range);
                _this.onDayColumnClicked(range);
            }
        });

        this.onDayColumnClicked = function (range) {

        }

        this.data = {};
        this.frameData = {};
        this.employeeIdToChunkIndex = {};
        this.chunks = [];
        this.employeeIdToRow = {};

        this.chunksToLoadQueue = [];
        this.loadingChunk = null;
        this.loadingChunkProcess = $.Deferred().resolve();

        this.onScroll = this.onScroll.bind(this);
        $(window).on("scroll", this.onScroll);
    };

    TimesheetTable.prototype.init = function () {
        this.smartTable.init();
    };

    TimesheetTable.prototype.destroy = function () {
        $(window).off("scroll", this.onScroll);
    };

    TimesheetTable.prototype.appendTo = function (selector) {
        this.appendedTo = selector;
        this.smartTable.appendTo(selector);
    };

    TimesheetTable.prototype.onCellClick = function (f) {
        this.smartTable.onCellClick(f);
    };

    TimesheetTable.prototype.onCellSelect = function (f) {
        this.smartTable.onCellSelect(f);
    };

    TimesheetTable.prototype.clearSelection = function (preventEvent) {
        this.smartTable.clearSelection(preventEvent);
    };

    TimesheetTable.prototype.onCellDeselect = function (f) {
        this.smartTable.onCellDeselect(f);
    };

    TimesheetTable.prototype.onRowSelect = function (f) {
        this.smartTable.onRowSelect(f);
    };

    TimesheetTable.prototype.onRowDeselect = function (f) {
        this.smartTable.onRowDeselect(f);
    };

    TimesheetTable.prototype.setOnDayColumnClicked = function (f) {
        this.onDayColumnClicked = f;
    }

    TimesheetTable.prototype.redraw = function () {
        if (this.reloadingPaused == false) {
            _this.setFrameData(data);
            _this.displayChunk();

            this.redrawFrame();
            this.displayChunk();
        }
    }

    TimesheetTable.prototype.redrawFrame = function () {
        if (this.reloadingPaused == false) {
            this.smartTable.clearData();

            if (this.frameData.daysCount == 1) {
                setupTableForOneDayView.call(this);
                putDataToTableForOneDayMode_FRAME.call(this);
                this.smartTable.redraw();
            } else if (this.frameData.daysCount == 7) {
                setupTableForWeekView.call(this);
                putDataToTableForManyDaysMode_FRAME.call(this);
                this.smartTable.redraw();
            } else {
                setupTableForManyDaysView.call(this);
                putDataToTableForManyDaysMode_FRAME.call(this);
                this.smartTable.redraw();
            }
        }
    }

    TimesheetTable.prototype.setFrameData = function (data) {
        this.frameData = data;
        this.chunks = [];
        this.employeeIdToChunkIndex = {};

        for (var chunkIndex in this.frameData.chunks) {
            this.chunks.push({
                employees: this.frameData.chunks[chunkIndex],
                loaded: false,
                loading: false
            })

            for (var employeeIndex in this.frameData.chunks[chunkIndex]) {
                var employeeId = this.frameData.chunks[chunkIndex][employeeIndex];
                this.employeeIdToChunkIndex[employeeId] = chunkIndex;
            }
        }

        this.redrawFrame();
    };

    TimesheetTable.prototype.pauseReloading = function () {
        this.reloadingPaused = true;
    }

    TimesheetTable.prototype.reloadData = function (force) {
        var _this = this;

        if (this.reloadingPaused == false || force == true) {
            this.reloadingPaused = false;

            this.smartTable.showSpinner();

            return this.timesheetManager.getTimesheetDataFrame({
                dateRange: this.filterDateRange,
                employees: this.filterEmployees,
                teams: this.filterTeams,
            }).then(function (data) {
                _this.setFrameData(data);
                return _this.displayChunk();
            }).done(function () {
                _this.smartTable.hideSpinner();
            });
        } else {
            return $.Deferred().resolve().promise;
        }
    };

    TimesheetTable.prototype.dateRange = function (dr) {
        this.filterDateRange = dr;
        return this.reloadData();
    };

    TimesheetTable.prototype.employees = function (e) {
        this.filterEmployees = e;

        return this.reloadData();
    };

    TimesheetTable.prototype.teams = function (t) {
        this.filterTeams = t;

        return this.reloadData();
    };

    TimesheetTable.prototype.selectByDateAndEmployeeId = function (date, employeeId) {
        if (this.isDisplaingManyDays()) {

            var _this = this;

            var chunkIndex = this.employeeIdToChunkIndex[employeeId];
            var chunk = this.chunks[chunkIndex];

            this.loadDataChunk(chunk).done(function () {
                var x = null, y = null;

                for (var i = 0; i < _this.frameData.employees.length; i++) {
                    var employee = _this.frameData.employees[i];

                    if (employee.id == employeeId) {
                        y = i;
                        break;
                    }
                }

                var firstDay = moment(_this.frameData.dateFrom);
                for (var i = 0; i < _this.frameData.daysCount; i++) {

                    if (date == moment(firstDay).add(i, 'days').format("YYYY-MM-DD")) {
                        x = i;
                        break;
                    }
                }

                if (x != null && y != null) {
                    _this.smartTable.selectCell(x, y);
                }
            });
        }
    }

    TimesheetTable.prototype.selectByEmployee = function (employeeId) {
        if (this.isDisplaingOneDay()) {
            var y = null;

            for (var i = 0; i < this.data.employees.length; i++) {
                var employee = this.data.employees[i].employee;

                if (employee.id == employeeId) {
                    y = i;
                    break;
                }
            }

            if (y != null) {
                this.smartTable.selectRow(y);
            }
        }
    }

    TimesheetTable.prototype.isDisplaingOneDay = function () {
        return this.frameData.daysCount == 1;
    }

    TimesheetTable.prototype.isDisplaingManyDays = function () {
        return this.frameData.daysCount > 1;
    }

    TimesheetTable.prototype.getMode = function () {
        return this.mode;
    }

    TimesheetTable.prototype.setModeShifts = function () {
        this.mode = TimesheetTableDisplayModes.SHIFTS;
        return this.reloadData();
    }

    TimesheetTable.prototype.setModeWorkedTime = function () {
        this.mode = TimesheetTableDisplayModes.WORKED_TIME;
        return this.reloadData();
    }

    TimesheetTable.prototype.setModeProjects = function () {
        var _this = this;
        this.mode = TimesheetTableDisplayModes.PROJECTS;

        return this.reloadData();
    }

    TimesheetTable.prototype.setMode = function (mode) {
        for (m in TimesheetTableDisplayModes) {
            if (mode == TimesheetTableDisplayModes[m]) {
                this.mode = TimesheetTableDisplayModes[m];
                this.redraw();
                return;
            }
        }
    }
    //------------------------------------------------------------------------------------------------------------------
    //private methods
    //------------------------------------------------------------------------------------------------------------------


    //drawing main

    function setupTableForOneDayView() {
        if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
            setupTableForOneDayViewForProjects.call(this);
        } else {
            setupTableForOneDayViewForShifts.call(this);
        }
    }

    function setupTableForOneDayViewForShifts() {
        this.smartTable.setClass("TimesheetTableOneDay");
        this.smartTable.lightRows();
        this.smartTable.withoutCellBorders();
        this.smartTable.setColumnsSeparateWidthMode();
        this.smartTable.rowsSelecting();
        this.smartTable.clearSeparateColumnsWidth();
        this.smartTable.setSeparateColumnWidth(0, 30);
        this.smartTable.setSeparateColumnWidth(2, 100);
        this.smartTable.setSeparateColumnWidth(4, 100);
        this.smartTable.setSeparateColumnWidth(6, 200);
        this.smartTable.setColumnsWidth(this.standardColumnWidth + 22);
        this.smartTable.setColumnsNumber(7);
        this.smartTable.hideSummaryColumn();
        this.smartTable.setColumnHeaderClickable(false);

    }

    function setupTableForOneDayViewForProjects() {
        this.smartTable.setClass("TimesheetTableOneDay");
        this.smartTable.lightRows();
        this.smartTable.withoutCellBorders();
        this.smartTable.setColumnsSeparateWidthMode();
        this.smartTable.rowsSelecting();
        this.smartTable.clearSeparateColumnsWidth();
        this.smartTable.setSeparateColumnWidth(0, 200);
        this.smartTable.setSeparateColumnWidth(1, 120);
        this.smartTable.setColumnsNumber(2);
        this.smartTable.hideSummaryColumn();
        this.smartTable.setColumnHeaderClickable(false);
    }

    function setupTableForWeekView() {
        this.smartTable.unsetClass("TimesheetTableOneDay");
        this.smartTable.zebraRows();
        this.smartTable.withCellBorders();
        this.smartTable.cellsSelecting();
        this.smartTable.showSummaryColumn();
        this.smartTable.setColumnsEqualWidthMode();
        this.smartTable.setColumnHeaderClickable(true);
        recalculateColumnsWithForWeekView.call(this);
    }

    function setupTableForManyDaysView() {
        this.smartTable.unsetClass("TimesheetTableOneDay");
        this.smartTable.zebraRows();
        this.smartTable.withCellBorders();
        this.smartTable.cellsSelecting();
        this.smartTable.showSummaryColumn();
        this.smartTable.setColumnsEqualWidthMode();
        this.smartTable.setColumnsWidth(this.standardColumnWidth);
        this.smartTable.setColumnHeaderClickable(true);


    }

    //drawing auxiliary

    function recalculateColumnsWithForWeekView() {
        if (this.appendedTo != null) {
            var availableWidth = $(this.appendedTo).innerWidth();
            var colWidth = Math.round((availableWidth - this.headerColumnWidth - this.summaryColumnWidth) / 7);

            if (colWidth > this.standardColumnWidth) {
                this.smartTable.setColumnsWidth(colWidth);
            } else {
                this.smartTable.setColumnsWidth(this.standardColumnWidth);
            }
        }
    }

    //drawing many days
    TimesheetTable.prototype.displayChunk = function () {
        if (this.frameData.daysCount > 1) {
            var visibleChunks = this.getVisibleChunks();

            for (var i = 0; i < visibleChunks.length; i++) {
                var chunkIndex = visibleChunks[i];
                var chunk = this.chunks[chunkIndex];

                this.loadDataChunk(chunk);
            }
        }
    }

    TimesheetTable.prototype.getVisibleChunks = function () {
        var visibleRows = this.smartTable.getVisibleRows();
        var emplos = [];

        for (var i = 0; i < visibleRows.length; i++) {
            var row = visibleRows[i];
            var data = this.smartTable.getRowData(row);
            emplos.push(data.employee.id);

        }

        var _this = this;
        var visibleChunks = _.uniq(_.map(emplos, function (employeeId) {
            return _this.employeeIdToChunkIndex[employeeId]
        }));

        if (visibleChunks.length == 0 && this.chunks.length > 0) {
            visibleChunks.push(0);
        }

        return visibleChunks;
    }


    TimesheetTable.prototype.onScroll = function () {
        if (this.frameData != null && this.frameData.daysCount > 1) {
            this.displayChunk();
        }
    }

    TimesheetTable.prototype.loadDataChunk = function (chunk) {
        var _this = this;

        if (chunk.loaded == false && chunk.loading == false && this.reloadingPaused == false) {
            chunk.loading = true;

            _this.chunksToLoadQueue.push(chunk);
            return _this.runLoadChunksProcess();
        } else if (chunk.loaded) {
            return $.Deferred().resolve().promise();
        } else if (chunk.loading) {
            return this.loadingChunkProcess;
        }
    }


    TimesheetTable.prototype.runLoadChunksProcess = function () {
        var _this = this;

        if (this.loadingChunk == null && this.chunksToLoadQueue.length > 0) {

            if (this.loadingChunkProcess == null || this.loadingChunkProcess.state() == "resolved") {
                this.loadingChunkProcess = $.Deferred();
                this.smartTable.showSpinner();

                this.loadingChunkProcess.done(function () {
                    _this.smartTable.hideSpinner();
                });
            }

            this.loadingChunk = this.chunksToLoadQueue[0];
            var chunk = this.loadingChunk;

            var loading;
            if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
                loading = this.timesheetManager.getTimesheetTableWithProjects({
                    dateRange: this.filterDateRange,
                    employees: chunk.employees
                })
            } else {
                loading = this.timesheetManager.getTimesheetTable({
                    dateRange: this.filterDateRange,
                    employees: chunk.employees
                });
            }

            loading.done(function (data) {
                _this.setDataChunk(data);
                chunk.loaded = true;
                chunk.loading = false;
                _this.loadingChunk = null;
                _this.chunksToLoadQueue = _.without(_this.chunksToLoadQueue, chunk);

                if (_this.chunksToLoadQueue.length > 0) {
                    setTimeout(function () {
                        _this.runLoadChunksProcess();
                    }, 0);
                } else {
                    _this.loadingChunkProcess.resolve();
                }
            });


            return this.loadingChunkProcess.promise();
        } else {
            return this.loadingChunkProcess.promise();
        }
    }


    TimesheetTable.prototype.loadDataOneDay = function () {
        var _this = this;

        this.smartTable.showSpinner();

        if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
            return this.timesheetManager.getTimesheetTableWithProjects({
                dateRange: this.filterDateRange,
                employees: this.filterEmployees,
                teams: this.filterTeams,
            }).done(function (data) {
            }).always(function () {
                _this.smartTable.hideSpinner();
            });
        } else {
            return this.timesheetManager.getTimesheetTable({
                dateRange: this.filterDateRange,
                employees: this.filterEmployees,
                teams: this.filterTeams,
            }).done(function (data) {
            }).always(function () {
                _this.smartTable.hideSpinner();
            });
        }

    }

    TimesheetTable.prototype.setDataChunk = function (chunkData) {
        $(this.appendedTo).hide();

        for (var i = 0; i < chunkData.employees.length; i++) {
            var employeeReport = chunkData.employees[i];
            var employee = employeeReport.employee;

            var row = this.employeeIdToRow[employee.id];
            setHeightOfRow.apply(this, [row, employeeReport]);

            this.smartTable.setRowSummaryContent(row, prepareSummaryCellContent.apply(this, [employeeReport.summary]));


            for (var y = 0; y < chunkData.daysCount; y++) {
                var employeeDay = employeeReport.days[y];

                var dayContainer = $('<div/>');
                fillDayContainer.apply(this, [dayContainer, employeeDay]);

                this.smartTable.setCellContent(row, y, dayContainer.contents());
                this.smartTable.setCellData(row, y, {
                    day: employeeDay,
                    employee: employee
                });


                if (employeeDay.mark == "WARN") {
                    this.smartTable.setCellClass(row, y, "TimesheetTable-div-cell-warn");
                } else if (employeeDay.mark == "FAIL") {
                    this.smartTable.setCellClass(row, y, "TimesheetTable-div-cell-fail");
                }
            }


            this.smartTable.redrawDataRow(row);
        }

        $(this.appendedTo).show();
    }

    function putDataToTableForOneDayMode_FRAME() {
        var _this = this;

        this.loadDataOneDay().done(function (timesheet) {
            if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
                putDataToTableForOneDayModeForProjects.call(_this, timesheet);
            } else {
                putDataToTableForOneDayModeForShifts.call(_this, timesheet);
            }

            _this.smartTable.redraw();
        });
    }

    function putDataToTableForManyDaysMode_FRAME() {
        var firstDay = moment(this.frameData.dateFrom);
        this.smartTable.setColumnsNumber(this.frameData.daysCount);

        for (var i = 0; i < this.frameData.daysCount; i++) {
            var date = moment(firstDay).add(i, 'days');
            var content = prepareDateHeaderContent(date);
            this.smartTable.setCellHeader(i, content);
        }

        for (var i = 0; i < this.frameData.employees.length; i++) {
            var employee = this.frameData.employees[i];
            var row = this.smartTable.addRow();

            this.smartTable.setRowData(row, {
                employee: employee
            });

            this.employeeIdToRow[employee.id] = row;

            var avatar = new Avatar(employee);
            this.smartTable.setRowHeaderContent(row, $('<div />').text(employee.fullName).prepend(avatar.getElement()));
        }

    }


    function setHeightOfRow(row, employeeReport) {
        if (this.mode == TimesheetTableDisplayModes.WORKED_TIME) {
            this.smartTable.setParticularRowHeight(row, 60);
        } else if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
            var maxProjects = calculateMaxProjectsForEmployee(employeeReport);

            if (maxProjects * 31 < this.standardRowHeight) {
                this.smartTable.setParticularRowHeight(row, this.standardRowHeight);
            } else {
                this.smartTable.setParticularRowHeight(row, maxProjects * 31);
            }
        } else {//defauls SHIFTS
            var maxShiftsPerDayForEmployee = calculateMaxShiftsForEmployee(employeeReport);
            this.smartTable.setParticularRowHeight(row, maxShiftsPerDayForEmployee * this.standardRowHeight);
        }
    }

    function fillDayContainer(dayContainer, employeeDay) {
        if (this.mode == TimesheetTableDisplayModes.WORKED_TIME) {
            fillDayContainerForModeWorkedTime(dayContainer, employeeDay);
        } else if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
            fillDayContainerForModeProjects(dayContainer, employeeDay);
        } else {//defauls SHIFTS
            fillDayContainerForModeShifts(dayContainer, employeeDay);
        }
    }

    function fillDayContainerForModeWorkedTime(dayContainer, employeeDay) {
        if (employeeDay.summary != null && employeeDay.summary.workedSeconds > 0) {
            var summaryDiv = $('<div />').appendTo(dayContainer);
            var durationDiv = $('<div class="duration" />').text(DateTimeTool.secToDisplayableDuration(employeeDay.summary.workedSeconds)).appendTo(summaryDiv);
        }
    }

    function fillDayContainerForModeShifts(dayContainer, employeeDay) {
        for (var z = 0; z < employeeDay.shifts.length; z++) {
            var shift = employeeDay.shifts[z];
            var shiftDiv = $('<div />');

            if (z == 0) {
                shiftDiv.addClass('TimesheetTable-div-first');
            } else {
                shiftDiv.addClass('TimesheetTable-div');
            }

            var indicatorsDiv = $('<div/>').addClass('TimesheetTable-div-indicators').appendTo(shiftDiv);

            if (shift.entryType == "SHIFT") {
                var inDiv = $('<div/>').addClass('TimesheetTable-div-indicators-in').text(DateTimeTool.fromStrutsTimeToDisplayable(shift.started)).appendTo(shiftDiv);

                var outDiv = $('<div/>').addClass('TimesheetTable-div-indicators-out');

                if (shift.finished != null) {
                    outDiv.text(DateTimeTool.fromStrutsTimeToDisplayable(shift.finished));
                } else {
                    if (shift.opened == true) {
                        outDiv.text("...");
                    } else {
                        outDiv.text("??");
                    }
                }

                outDiv.appendTo(shiftDiv);

                if (shift.workedSeconds != null) {
                    var durationDiv = $('<div/>').addClass('TimesheetTable-div-indicators-duration').text(DateTimeTool.secToDisplayableDuration(shift.workedSeconds)).appendTo(shiftDiv);
                }
            }

            if (shift.changeRequestId != null) {
                $('<span/>').addClass('iconPending').appendTo(indicatorsDiv);
            }

            shiftDiv.appendTo(dayContainer);
        }
    }

    function fillDayContainerForModeProjects(dayContainer, employeeDay) {
        prepareContentForProjects(dayContainer, employeeDay.projects);
    }

    function prepareContentForProjects(container, projects) {


        for (var i = 0; i < projects.length; i++) {
            var projectLogged = projects[i];

            var projDiv = $('<div class="project"/>');
            projDiv.text(DateTimeTool.secToDisplayableDuration(projectLogged.seconds));

            var projectMarker = $('<div class="project-marker" />').appendTo(projDiv);

            if (projectLogged.projectId == null) {
                projectMarker.addClass("anyProject");
                ToolTip.setTip(projectMarker, Messages.get("worklog.anyProject"), true);
            } else {
                projectMarker.css("background-color", projectLogged.projectColor);
                ToolTip.setTip(projectMarker, projectLogged.projectName, true);
            }

            projDiv.appendTo(container);
        }


    }

    function calculateMaxShiftsForEmployee(employeeReport) {
        var max = 1;

        for (var i = 0; i < employeeReport.days.length; i++) {
            var inDayCount = 0;

            for (var y = 0; y < employeeReport.days[i].shifts.length; y++) {
                if (employeeReport.days[i].shifts[y].entryType == "SHIFT")
                    inDayCount++;
            }

            if (inDayCount > max)
                max = inDayCount;
        }

        return max;
    }

    function calculateMaxProjectsForEmployee(employeeReport) {
        var projectsIds = [];

        for (var i = 0; i < employeeReport.days.length; i++) {
            var projLength = employeeReport.days[i].projects.length;

            for (var y = 0; y < projLength; y++) {
                var proj = employeeReport.days[i].projects[y];
                projectsIds.push(proj.projectId);
            }

        }

        return _.uniq(projectsIds).length;
    }

    function prepareDateHeaderContent(date) {
        var div = $('<div />');

        $('<div/>').addClass("dayOfMonth").text(date.date()).appendTo(div);
        $('<div/>').addClass("month").text(DateTimeTool.getMonthShortName(date)).appendTo(div);
        $('<div/>').addClass("dayOfWeek").text(DateTimeTool.getDayShortName(date)).appendTo(div);

        return div;
    }


    function prepareSummaryCellContent(summary) {
        if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
            var div = $('<div />');

            prepareContentForProjects(div, summary.projects);

            return div;
        } else {
            var div = $('<div />');

            $('<div class="worked"/>').text(DateTimeTool.secToDisplayableDuration(summary.workedSeconds)).appendTo(div);
            $('<div class="planned"/>').text(DateTimeTool.secToDisplayableDuration(summary.plannedSeconds)).appendTo(div);

            return div;
        }


    }


    //drawing one day - for projects
    function putDataToTableForOneDayModeForProjects(timesheet) {
        this.smartTable.setCellHeader(0, "<span>" + Messages.get("project") + "</span>");
        this.smartTable.setCellHeader(1, "<span>" + Messages.get("timesheet.worked") + "</span>");


        for (var i = 0; i < timesheet.employees.length; i++) {
            var employeeReport = timesheet.employees[i];
            var employee = employeeReport.employee;
            var employeeDay = employeeReport.days[0];

            var row = this.smartTable.addRow();

            var avatar = '<img src="' + employee.avatar + '" class="avatar regular" /> ';
            this.smartTable.setRowHeaderContent(row, $('<div>' + avatar + employee.fullName + '</div>'));
            this.smartTable.setRowSummaryContent(row, prepareSummaryCellContent(employeeReport.summary));


            setRowContentOneDay.apply(this, [row, employeeDay]);


            this.smartTable.setRowData(row, {
                day: employeeDay,
                employee: employee
            });
        }
    }


    function setRowContentOneDayForModeProjects(row, employeeDay) {
        var emploRowHeight = employeeDay.projects.length * 60;
        if (emploRowHeight < 60) {
            emploRowHeight = 60;
        }

        this.smartTable.setParticularRowHeight(row, emploRowHeight);
        this.smartTable.setCellContent(row, 0, prepareCellForOneDayViewProjectName.call(this, employeeDay));
        this.smartTable.setCellContent(row, 1, prepareCellForOneDayViewProjectWorkedTime.call(this, employeeDay));
    }


    function prepareCellForOneDayViewProjectName(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.projects.length; i++) {
            var project = employeeDay.projects[i];
            var projectContainer = $('<div />');
            var projectMarker = $('<div />').addClass("project-marker");

            if (project.projectId == null) {
                projectContainer.text(Messages.get('worklog.anyProject'));
                projectMarker.addClass("anyProject").appendTo(projectContainer);
            } else {
                projectContainer.text(project.projectName);
                projectMarker.css("background-color", project.projectColor).appendTo(projectContainer);
            }

            projectContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }

    function prepareCellForOneDayViewProjectWorkedTime(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.projects.length; i++) {
            var project = employeeDay.projects[i];
            var projectContainer = $('<div />');

            projectContainer.text(DateTimeTool.secToDisplayableDuration(project.seconds));
            projectContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }

    // drawing one day - for shifts and worked time


    function putDataToTableForOneDayModeForShifts(timesheet) {
        this.smartTable.setCellHeader(0, "<span>&nbsp;</span>");
        this.smartTable.setCellHeader(1, "<span>" + Messages.get("timesheet.worked") + "</span>");
        this.smartTable.setCellHeader(2, "<span>&nbsp;</span>");
        this.smartTable.setCellHeader(3, "<span>" + Messages.get("timesheet.planned") + "</span>");
        this.smartTable.setCellHeader(4, "<span>&nbsp;</span>");
        this.smartTable.setCellHeader(5, "<span>&nbsp;</span>");
        this.smartTable.setCellHeader(6, "<span>&nbsp;</span>");


        for (var i = 0; i < timesheet.employees.length; i++) {
            var employeeReport = timesheet.employees[i];
            var employee = employeeReport.employee;
            var employeeDay = employeeReport.days[0];

            var row = this.smartTable.addRow();

            var avatar = '<img src="' + employee.avatar + '" class="avatar regular" /> ';
            this.smartTable.setRowHeaderContent(row, $('<div>' + avatar + employee.fullName + '</div>'));

            setRowContentOneDay.apply(this, [row, employeeDay]);

            this.smartTable.setRowData(row, {
                day: employeeDay,
                employee: employee
            });
        }
    }

    function setRowContentOneDay(row, empoyeeDay) {
        if (this.mode == TimesheetTableDisplayModes.WORKED_TIME) {
            setRowContentOneDayForModeWorkedTime.apply(this, [row, empoyeeDay]);
        } else if (this.mode == TimesheetTableDisplayModes.PROJECTS) {
            setRowContentOneDayForModeProjects.apply(this, [row, empoyeeDay]);
        } else {//defauls SHIFTS
            setRowContentOneDayForModeShifts.apply(this, [row, empoyeeDay]);
        }
    }

    function setRowContentOneDayForModeWorkedTime(row, employeeDay) {
        this.smartTable.setParticularRowHeight(row, 60);

        this.smartTable.setCellContent(row, 0, prepareStatusCellForOneDayViewForModeWorkedTime.call(this, employeeDay));
        this.smartTable.setCellContent(row, 1, prepareWorkedCellForOneDayViewForModeWorkedTime.call(this, employeeDay));
        this.smartTable.setCellContent(row, 2, $("<div />"));
        this.smartTable.setCellContent(row, 3, preparePlannedCellForOneDayViewForModeWorkedTime.call(this, employeeDay));
        this.smartTable.setCellContent(row, 4, $("<div />"));
        this.smartTable.setCellContent(row, 5, prepareInfoIconsCellForOneDayViewForModeWorkedTime.call(this, employeeDay));
    }

    function prepareStatusCellForOneDayViewForModeWorkedTime(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        if (employeeDay.mark != "NONE") {
            $('<div class="shiftIcon"><span class="iconMark workReport_summaryIcon_' + employeeDay.mark + '"></span></div>').appendTo(cellContainer);
        }

        return cellContainer;
    }

    function prepareWorkedCellForOneDayViewForModeWorkedTime(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');
        var content = $('<div />').appendTo(cellContainer);

        if (employeeDay.summary != null && employeeDay.summary.workedSeconds > 0) {
            content.text(DateTimeTool.secToDisplayableDuration(employeeDay.summary.workedSeconds));
        }

        return cellContainer;
    }

    function preparePlannedCellForOneDayViewForModeWorkedTime(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');
        var content = $('<div />').appendTo(cellContainer);

        if (employeeDay.summary != null) {
            content.text(DateTimeTool.secToDisplayableDuration(employeeDay.summary.plannedSeconds));
        }

        return cellContainer;
    }

    function prepareInfoIconsCellForOneDayViewForModeWorkedTime(employeeDay) {
        var types = [];

        var cellContainer = $('<div class="dayContainer" />');
        var content = $('<div />').appendTo(cellContainer);

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];

            for (var y = 0; y < shift.abnormalities.length; y++) {
                var abnormality = shift.abnormalities[y];

                if (!_.includes(types, abnormality.type)) {
                    types.push(abnormality.type);

                    var icon = $('<span />')
                        .addClass("workReport_abnormality")
                        .addClass("icon_" + abnormality.type)
                        .data("tooltip", "true")
                        .appendTo(content);

                    ToolTip.setTip(icon, Messages.get("abnormality." + abnormality.type), true, false);
                }

            }
        }

        return cellContainer;
    }


    function setRowContentOneDayForModeShifts(row, employeeDay) {
        this.smartTable.setParticularRowHeight(row, employeeDay.shifts.length * 60);

        this.smartTable.setCellContent(row, 0, prepareStatusCellForOneDayView.call(this, employeeDay));
        this.smartTable.setCellContent(row, 1, prepareWorkedCellForOneDayView.call(this, employeeDay));
        this.smartTable.setCellContent(row, 2, prepareWorkedCellSummaryForOneDayView.call(this, employeeDay));
        this.smartTable.setCellContent(row, 3, preparePlannedCellForOneDayView.call(this, employeeDay));
        this.smartTable.setCellContent(row, 4, preparePlannedCellSummaryForOneDayView.call(this, employeeDay));
        this.smartTable.setCellContent(row, 5, prepareInfoIconsCellForOneDayView.call(this, employeeDay));
        this.smartTable.setCellContent(row, 6, prepareInOutPhotosForOneDayView.call(this, employeeDay));

    }

    function prepareStatusCellForOneDayView(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];
            var shiftContainer = $('<div />');

            if (shift.mark != "NONE") {
                $('<div class="shiftIcon"><span class="iconMark workReport_summaryIcon_' + shift.mark + '"></span></div>').appendTo(shiftContainer);
            }

            shiftContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }

    function prepareWorkedCellForOneDayView(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];
            var shiftContainer = $('<div />');

            if (shift.entryType == "SHIFT") {
                var content = '';

                content += DateTimeTool.fromStrutsTimeToDisplayable(shift.started);
                content += ' - ';

                if (shift.finished != null) {
                    content += DateTimeTool.fromStrutsTimeToDisplayable(shift.finished);
                } else {
                    if (shift.opened == true) {
                        content += "...";
                    } else {
                        content += "??";
                    }
                }

                shiftContainer.text(content);
            }

            shiftContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }


    function prepareWorkedCellSummaryForOneDayView(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];
            var shiftContainer = $('<div />');

            if (shift.entryType == "SHIFT") {
                var content = '';

                if (shift.workedSeconds != null) {
                    content = DateTimeTool.secToDisplayableDuration(shift.workedSeconds);
                }

                shiftContainer.text(content);
            }

            shiftContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }


    function preparePlannedCellForOneDayView(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];
            var shiftContainer = $('<div />');
            var content = '';


            if (shift.plannedStart != null) {
                content += DateTimeTool.fromStrutsTimeToDisplayable(shift.plannedStart);
                content += ' - ';
            }

            if (shift.plannedFinish != null) {
                content += DateTimeTool.fromStrutsTimeToDisplayable(shift.plannedFinish);
            }

            shiftContainer.text(content);
            shiftContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }


    function preparePlannedCellSummaryForOneDayView(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];
            var shiftContainer = $('<div />');

            if (shift.plannedSeconds != null) {
                shiftContainer.text(DateTimeTool.secToDisplayableDuration(shift.plannedSeconds));
            }

            shiftContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }


    function prepareInfoIconsCellForOneDayView(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];
            var shiftContainer = $('<div />');

            for (var y = 0; y < shift.abnormalities.length; y++) {
                var abnormality = shift.abnormalities[y];

                var icon = $('<span />')
                    .addClass("workReport_abnormality")
                    .addClass("icon_" + abnormality.type)
                    .data("tooltip", "true")
                    .appendTo(shiftContainer);

                ToolTip.setTip(icon, Messages.get("abnormality." + abnormality.type), true, false);
            }

            shiftContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }


    function prepareInOutPhotosForOneDayView(employeeDay) {
        var cellContainer = $('<div class="dayContainer" />');

        for (var i = 0; i < employeeDay.shifts.length; i++) {
            var shift = employeeDay.shifts[i];
            var shiftContainer = $('<div />');

            var shiftPhoto = new ShiftPhoto(shift);

            var inPhoto = shiftPhoto.getInPhoto();
            if (inPhoto != null) {
                shiftContainer.append(inPhoto);
            }

            var outPhoto = shiftPhoto.getOutPhoto();
            if (outPhoto != null) {
                shiftContainer.append(outPhoto);
            }

            shiftContainer.appendTo(cellContainer);
        }

        return cellContainer;
    }

    window.TimesheetTable = TimesheetTable;
})(window);
(function (window, undefined) {


    AttendanceTable = function (manager) {
        var _this = this;

        this.attendanceManager = manager;

        this.filterDateRange = null;
        this.filterEmployees = [];
        this.filterTeams = [];
        this.filterAbsenceTypes = [];
        this.filterContractTypes = [];

        this.reloadingPaused = false;

        this.standardColumnWidth = 55;
        this.standardRowHeight = 50;
        this.headerColumnWidth = 140;
        this.summaryColumnWidth = 100;
        this.appendedTo = null;

        this.smartTable = new SmartTable();
        this.smartTable.setClass("AttendanceTable");
        this.smartTable.setHeaderHeight(60);
        this.smartTable.setColumnsWidth(this.standardColumnWidth);
        this.smartTable.setHeaderColumnWidth(this.headerColumnWidth);
        this.smartTable.setSummaryColumnWidth(this.summaryColumnWidth);
        this.smartTable.setRowHeight(50);
        this.smartTable.turnOnHighlightContentHeader();
        this.smartTable.setSelectable();


        this.data = {};
    };

    AttendanceTable.prototype.init = function () {
        this.smartTable.init();
    };

    AttendanceTable.prototype.destroy = function () {
        var _this = this;
    };

    AttendanceTable.prototype.appendTo = function (selector) {
        this.appendedTo = selector;
        this.smartTable.appendTo(selector);
    };

    AttendanceTable.prototype.onCellClick = function (f) {
        this.smartTable.onCellClick(f);
    };

    AttendanceTable.prototype.onCellSelect = function (f) {
        this.smartTable.onCellSelect(f);
    };

    AttendanceTable.prototype.clearSelection = function (preventEvent) {
        this.smartTable.clearSelection(preventEvent);
    };

    AttendanceTable.prototype.onCellDeselect = function (f) {
        this.smartTable.onCellDeselect(f);
    };

    AttendanceTable.prototype.onRowSelect = function (f) {
        this.smartTable.onRowSelect(f);
    };

    AttendanceTable.prototype.onRowDeselect = function (f) {
        this.smartTable.onRowDeselect(f);
    };

    AttendanceTable.prototype.setOnDayColumnClicked = function (f) {
        this.onDayColumnClicked = f;
    }

    AttendanceTable.prototype.redraw = function () {
        if (this.reloadingPaused == false) {
            var attendance = this.data;
            this.smartTable.clearData();
            setupTableForManyDaysView.call(this);
            this.smartTable.redraw();
        }
    }


    AttendanceTable.prototype.setData = function (data) {
        this.data = data;
        this.redraw();
    };

    AttendanceTable.prototype.pauseReloading = function () {
        this.reloadingPaused = true;
    }

    AttendanceTable.prototype.reloadData = function (force) {
        var _this = this;

        if (this.reloadingPaused == false || force == true) {
            this.reloadingPaused = false;

            return this.attendanceManager.get({
                dateRange: this.filterDateRange,
                employees: this.filterEmployees,
                teams: this.filterTeams,
                absenceTypes: this.filterAbsenceTypes,
                contractTypes: this.filterContractTypes
            }).done(function (data) {
                _this.setData(data);
            });
        }
    };

    AttendanceTable.prototype.dateRange = function (dr) {
        this.filterDateRange = dr;
    };

    AttendanceTable.prototype.employees = function (e) {
        this.filterEmployees = e;
    };

    AttendanceTable.prototype.absenceTypes = function (e) {
        this.filterAbsenceTypes = e;
    };

    AttendanceTable.prototype.contractTypes = function (e) {
        this.filterContractTypes = e;
    };
    

    AttendanceTable.prototype.teams = function (t) {
        this.filterTeams = t;
    };


    AttendanceTable.prototype.isDisplaingOneDay = function () {
        return this.data.daysCount == 1;
    }

    AttendanceTable.prototype.isDisplaingManyDays = function () {
        return this.data.daysCount > 1;
    }


    //------------------------------------------------------------------------------------------------------------------
    //private methods
    //------------------------------------------------------------------------------------------------------------------


    //drawing main


    function setupTableForManyDaysView() {
        this.smartTable.zebraRows();
        this.smartTable.withCellBorders();
        this.smartTable.cellsSelecting();
        this.smartTable.showSummaryColumn();
        this.smartTable.setColumnsEqualWidthMode();
        this.smartTable.setColumnsWidth(this.standardColumnWidth);
        this.smartTable.setColumnHeaderClickable(false);

        putData.call(this);
    }

    function putData() {
        var attendance = this.data;
        var firstDay = moment(attendance.dateFrom);

        this.smartTable.setColumnsNumber(attendance.daysCount);

        for (var i = 0; i < attendance.daysCount; i++) {
            var date = moment(firstDay).add(i, 'days');
            var content = prepareDateHeaderContent(date);
            this.smartTable.setCellHeader(i, content);
        }

        for (var i = 0; i < attendance.employees.length; i++) {
            var employeeReport = attendance.employees[i];
            var employee = employeeReport.employee;
            var row = this.smartTable.addRow();
            this.smartTable.setRowHeaderContent(row, $('<div />').text(employee.fullName));
            this.smartTable.setRowSummaryContent(row, prepareSummaryCellContent(employeeReport.summary));


            setHeightOfRow.apply(this, [row, employeeReport]);


            for (var y = 0; y < attendance.daysCount; y++) {
                var employeeDay = employeeReport.days[y];

                var dayContainer = $('<div class="dayContainer" />');
                fillDayContainer.apply(this, [dayContainer, employeeDay, row, y]);

                this.smartTable.setCellContent(row, y, dayContainer);
                this.smartTable.setCellData(row, y, {
                    day: employeeDay,
                    employee: employee
                });
            }
        }

    }

    function fillDayContainer(dayContainer, employeeDay, row, column) {

        if (employeeDay.workingTime > 0) {
            var workingDiv = $('<div />').appendTo(dayContainer);
            var durationDiv = $('<div class="workingTime" />').text(DateTimeTool.secToDisplayableDuration(employeeDay.workingTime)).appendTo(workingDiv);
        }
        if (employeeDay.absenceTime > 0) {
            var summaryDiv = $('<div />').appendTo(dayContainer);
            var durationDiv = $('<div class="absenceTime" />').text(DateTimeTool.secToDisplayableDuration(employeeDay.absenceTime)).appendTo(summaryDiv);
            this.smartTable.setCellClass(row, column, "absence");
        }
        if (employeeDay.workOutTime > 0 ) {
            var summaryDiv = $('<div />').appendTo(dayContainer);
            var durationDiv = $('<div class="workOutTime" />').text(DateTimeTool.secToDisplayableDuration(employeeDay.workOutTime)).appendTo(summaryDiv);
            this.smartTable.setCellClass(row, column, "absence");
        }


        if (employeeDay.holiday) {
            this.smartTable.setCellClass(row, column, "holiday");
        }

    }

    function prepareDateHeaderContent(date) {
        var div = $('<div />');
        $('<div class="dayOfMonth"/>').text(date.date()).appendTo(div);
        $('<div class="month"/>').text(DateTimeTool.getMonthShortName(date)).appendTo(div);
        $('<div class="dayOfWeek"/>').text(DateTimeTool.getDayShortName(date)).appendTo(div);
        
        return div;
    }


    function prepareSummaryCellContent(summary) {
        var div = $('<div />');

        $('<div class="sumWorked"/>').text(DateTimeTool.secToDisplayableDuration(summary.workingTime)).appendTo(div);

        if (summary.absenceTime > 0) {
            $('<div class="sumAbsence"/>').text(DateTimeTool.secToDisplayableDuration(summary.absenceTime)).appendTo(div);
        }
        if (summary.workOutTime > 0) {
            $('<div class="sumWorkOut"/>').text(DateTimeTool.secToDisplayableDuration(summary.workOutTime)).appendTo(div);
        }

        return div;
    }


    function setHeightOfRow(row, employeeReport) {
        var maxInformationInDay = calculateMaxInformationInDay(employeeReport);
        this.smartTable.setParticularRowHeight(row, maxInformationInDay * this.standardRowHeight);
    }

    function calculateMaxInformationInDay(employeeReport) {
        var max = 1;

        for (var i = 0; i < employeeReport.days.length; i++) {
            var inDayCount = 0;
            var employeeDay = employeeReport.days[i];

            if (employeeDay.workingTime > 0) {
                inDayCount++;
            }
            if (employeeDay.absenceTime > 0) {
                inDayCount++;
            }
            if (employeeDay.workOutTime > 0) {
                inDayCount++;
            }

            if (inDayCount > max)
                max = inDayCount;
        }

        return max;
    }


    window.AttendanceTable = AttendanceTable;
})(window);
(function (window, undefined) {


    EmployeesTable = function (manager) {
        var _this = this;

        this.employeeReportManager = manager;

        this.filterEmployees = [];
        this.filterTeams = [];
        this.filterContractTypes = [];

        this.reloadingPaused = false;

        this.standardColumnWidth = 180;
        this.standardRowHeight = 60;
        this.headerColumnWidth = 0;
        this.summaryColumnWidth = 0;
        this.appendedTo = null;

        this.smartTable = new SmartTable();
        this.smartTable.setClass("EmployeesTable");
        this.smartTable.setHeaderHeight(70);
        this.smartTable.setColumnsWidth(this.standardColumnWidth);
        this.smartTable.setHeaderColumnWidth(this.headerColumnWidth);
        this.smartTable.setSummaryColumnWidth(this.summaryColumnWidth);
        this.smartTable.setRowHeight(70);


        this.data = {};
    };

    EmployeesTable.prototype.init = function () {
        this.smartTable.init();
    };

    EmployeesTable.prototype.destroy = function () {
        var _this = this;
    };

    EmployeesTable.prototype.appendTo = function (selector) {
        this.appendedTo = selector;
        this.smartTable.appendTo(selector);
    };


    EmployeesTable.prototype.redraw = function () {
        if (this.reloadingPaused == false) {
            var data = this.data;
            this.smartTable.clearData();
            setupTable.call(this);
            this.smartTable.redraw();
        }
    }

    EmployeesTable.prototype.setData = function (data) {
        this.data = data;
        this.redraw();
    };

    EmployeesTable.prototype.pauseReloading = function () {
        this.reloadingPaused = true;
    }

    EmployeesTable.prototype.reloadData = function (force) {
        var _this = this;

        if (this.reloadingPaused == false || force == true) {
            this.reloadingPaused = false;

            return this.employeeReportManager.get({
                employees: this.filterEmployees,
                teams: this.filterTeams,
                contractTypes: this.filterContractTypes
            }).done(function (data) {
                _this.setData(data);
            });
        }
    };


    EmployeesTable.prototype.employees = function (e) {
        this.filterEmployees = e;
    };


    EmployeesTable.prototype.contractTypes = function (e) {
        this.filterContractTypes = e;
    };


    EmployeesTable.prototype.teams = function (t) {
        this.filterTeams = t;
    };


    //------------------------------------------------------------------------------------------------------------------
    //private methods
    //------------------------------------------------------------------------------------------------------------------


    //drawing main


    function setupTable() {
        this.smartTable.setColumnsSeparateWidthMode();
        this.smartTable.setColumnsWidth(this.standardColumnWidth);
        putData.call(this);
    }

    function putData() {
        var data = this.data;

        this.smartTable.setColumnsNumber(data.columns.length);

        for (var i = 0; i < data.columns.length; i++) {
            var content = prepareHeaderContent(data.columns[i]);
            this.smartTable.setCellHeader(i, content);
        }


        for (var i = 0; i < data.employees.length; i++) {
            var employeeReport = data.employees[i];
            var row = this.smartTable.addRow();

            for (var y = 0; y < data.columns.length; y++) {

                var columnName = data.columns[y];
                var employeeRecord = employeeReport[columnName];
                var container = $('<div class="employeesContainer" />');


                if (employeeRecord != null) {
                    fillDayContainer.apply(this, [container, employeeRecord, row, y, columnName]);
                    this.smartTable.setCellContent(row, y, container);
                }
                else {
                    fillDayContainer.apply(this, [container, "", row, y, columnName]);
                    this.smartTable.setCellContent(row, y, container);
                }
            }
        }


    }

    function fillDayContainer(container, employeeRecord, row, column, columnName) {

        if (columnName == "maritalStatus" && employeeRecord !== "") {
            employeeRecord = Messages.get("maritalStatus." + employeeRecord);
        }
        if (columnName == "sex" && employeeRecord !== "") {
            employeeRecord = Messages.get("sex." + employeeRecord);
        }
        if (columnName == "hireDate" && employeeRecord !== "") {
            employeeRecord = DateTimeTool.fromStrutsDateToDateDisplayable(employeeRecord);
        }
        if (columnName == "birthDate" && employeeRecord !== "") {
            employeeRecord = DateTimeTool.fromStrutsDateToDateDisplayable(employeeRecord);
        }
        if (columnName == "probationEndDate" && employeeRecord !== "") {
            employeeRecord = DateTimeTool.fromStrutsDateToDateDisplayable(employeeRecord);
        }
        if (columnName == "youngestChildBirthDate" && employeeRecord !== "") {
            employeeRecord = DateTimeTool.fromStrutsDateToDateDisplayable(employeeRecord);
        }
        var recordDiv = $('<div/>').text(employeeRecord).appendTo(container);

    }


    function prepareHeaderContent(columnName) {
        var div = $('<div />');
        $('<div class="employeesHeader"/>').text(Messages.get("report.employees." + columnName)).appendTo(div);
        return div;
    }


    window.EmployeesTable = EmployeesTable;
})(window);
(function (window) {
    FilesPresentation = function () {
        this.table = $('<table style="width: 100%;" class="presentationTable2 invisibleHeader leftAligned FilesPresentation">');
        this.thead = $('<thead />').appendTo(this.table);
        this.tbody = $('<tbody />').appendTo(this.table);


        this.downloadFileOnClick = false;
        this.files = [];
        this.fileUploader = null;
        this.removeTempFomrUIAfterUpload = false;

        this.allowingDelete = false;
        this.onDelete = function (file) {
            return $.Deferred().resolve(true).promise();
        }

        this.onUploadSuccess = function (uploadIdentifier, uploadedTempId, uploadedTempAccessId) {
            return $.Deferred().resolve(true).promise();
        }
    };

    FilesPresentation.prototype.appendTo = function (selector) {
        var _t = this;
        $(selector).append(_t.table);
    };

    //DATA
    //todo change the name (set persisted file or sth like that)
    FilesPresentation.prototype.setData = function (data) {
        this.files = data;
        this.tbody.empty();

        for (var i = 0; i < this.files.length; i++) {
            var file = this.files[i];
            displayFile.apply(this, [file]);
        }
    };

    FilesPresentation.prototype.addStoredFile = function (fileName, accessId) {
        var file = {
            fileName: fileName,
            accessId: accessId
        };

        this.files.push(file);
        displayFile.apply(this, [file]);
    };

    FilesPresentation.prototype.addUploadingFile = function (uploadIdentifier, fileName) {
        var fileObject = {
            type: "TempFile",
            uploadId: uploadIdentifier,
            fileName: fileName,
            uploaded: false
        };
        this.files.push(fileObject);

        displayFile.apply(this, [fileObject]);
    }

    FilesPresentation.prototype.getTempFiles = function () {
        var preparedFiles = [];

        for (var i = 0; i < this.files.length; i++) {
            var file = this.files[i];
            if (file.type == "TempFile" && file.uploaded == true) {
                preparedFiles.push({
                    type: "TempFile",
                    id: file.id,
                    accessId: file.accessId
                });
            }
        }

        return preparedFiles;
    }

    FilesPresentation.prototype.uploading = function () {
        if (this.fileUploader != null) {
            return this.fileUploader.uploadingProcess();
        } else {
            return $.Deferred().resolve().promise();
        }
    }

    FilesPresentation.prototype.hasFiles = function () {
        return this.files.length > 0;
    }

    FilesPresentation.prototype.changeTempToStoredFile = function (uploadIdentifier, fileName, accessId) {
        _.remove(this.files, function (file) {
            return (file.type == "TempFile" && file.uploadId == uploadIdentifier);
        });

        var newFile = {
            fileName: fileName,
            accessId: accessId
        };

        this.files.push(newFile);

        this.tbody.find("tr").each(function () {
            var row = $(this);
            var file = row.data("file");

            if (file != null && file.type == "TempFile" && file.uploadId == uploadIdentifier) {
                row.data("file", newFile);
            }
        });
    }

    //SETUP METHODS
    FilesPresentation.prototype.enableUploadTempFiles = function (category) {
        var _this = this;

        this.fileUploader = new FileUploader({
            category: category,

            onFileAdded: function (uploadIdentifier, fileName) {
                _this.addUploadingFile(uploadIdentifier, fileName);
                markUploading.apply(_this, [uploadIdentifier]);
            },

            onFileSuccess: function (uploadIdentifier, uploadedTempId, uploadedTempAccessId) {
                setUploadedIdsToTempfile.apply(_this, [uploadIdentifier, uploadedTempId, uploadedTempAccessId]);

                var row = findRowByUploadId.apply(_this, [uploadIdentifier]);

                _this.onUploadSuccess(uploadIdentifier, uploadedTempId, uploadedTempAccessId)
                    .done(function () {
                        if (_this.removeTempFomrUIAfterUpload) {
                            removeTempFileFromArray.apply(_this, [uploadIdentifier]);
                        }

                    })
                    .fail(function () {

                    }).always(function () {
                        row.removeClass("uploading");
                    });
            },

            onFileError: function (uploadIdentifier, errorMessage) {
                unmarkUploading.apply(_this, [uploadIdentifier]);
                removeTempFileFromArray.apply(_this, [uploadIdentifier]);

                var messageContent = extractErrorMessageContent(errorMessage);
                markErrorOnTempFile.apply(_this, [uploadIdentifier, messageContent]);
                displayErrorDialog(messageContent);
            }
        });

        this.fileUploader.init();
    }

    FilesPresentation.prototype.setUpUploadButtonId = function (buttonId) {
        this.fileUploader.assignButtonId(buttonId);
    }

    FilesPresentation.prototype.allowDelete = function () {
        this.allowingDelete = true;
    }

    FilesPresentation.prototype.downloadOnClickRow = function () {
        this.downloadFileOnClick = true;

    }

    FilesPresentation.prototype.setOnDelete = function (f) {
        this.onDelete = f;
    }

    FilesPresentation.prototype.setOnUploadSuccess = function (f) {
        this.onUploadSuccess = f;
    }

    FilesPresentation.prototype.removeTempAfterSuccessfulUpload = function () {
        this.removeTempFomrUIAfterUpload = true;
    }


    //PRIVATE
    function downloadFile(file) {
        if (this.downloadFileOnClick) {
            document.location = URL2.get("/webapi/file/download/" + file.accessId);
        }
    }

    function displayFile(file) {
        var _this = this;
        var tr = $("<tr />");
        tr.data("file", file);

        var nameLower = file.fileName.toLowerCase();
        var fileExt = nameLower.split('.').pop();
        var icon = "iconFile";

        if (fileExt == "pdf") {
            icon = "iconPdf";
        } else if (fileExt == "zip" || fileExt == "rar" || fileExt == "tar" || fileExt == "gz") {
            icon = "iconZip";
        } else if (fileExt == "doc" || fileExt == "docx" || fileExt == "rtf" || fileExt == "txt") {
            icon = "iconDoc";
        }

        var iconTd = $("<td />").addClass("iconCell").appendTo(tr);
        iconTd.append('<span class="' + icon + '" />').appendTo(tr);

        var nameTd = $("<td />").appendTo(tr);
        nameTd.text(file.fileName);

        if (this.allowingDelete) {
            var actionTd = $("<td />").addClass("actionsCell").appendTo(tr);

            var deleteButton = $('<button class="ghostButton small onlyIcon iconDelete" >' + Messages.get("common.delete") + '</button>');
            deleteButton.appendTo(actionTd);
            deleteButton.click(function (event) {
                event.stopPropagation();
                var rowFile = tr.data("file");

                _this.onDelete(rowFile).done(function (canDelete) {
                    if (canDelete) {
                        removeFileFromArray.apply(_this, [rowFile]);
                        tr.remove();
                    }
                });
            });
        }

        if (this.downloadFileOnClick) {
            iconTd.addClass("clickable");
            nameTd.addClass("clickable");


            $(tr).click(function () {
                var file = $(this).data("file");

                if (file != null && file.type != "TempFile") {
                    downloadFile.apply(_this, [file]);
                }
            });
        }

        tr.appendTo(this.tbody);
    }


    function removeFileFromArray(file) {
        this.files = _.reject(this.files, function (element) {
            return (element.type == file.type && element.id == file.id);
        });
    }

    function removeTempFileFromArray(fileIdentifier) {
        this.files = _.reject(this.files, function (element) {
            return (element.type == "TempFile" && element.uploadId == fileIdentifier);
        });
    }

    function markErrorOnTempFile(fileIdentifier, errorContent) {
        var _this = this;

        _this.tbody.find("tr").each(function () {
            var row = $(this);
            var file = row.data("file");

            if (file != null && file.type == "TempFile" && file.uploadId == fileIdentifier) {
                row.addClass("error");

                ToolTip.setTip(row, errorContent, false);
            }
        });
    }

    function extractErrorMessageContent(errorObject) {
        if (errorObject != null) {
            if (errorObject.nonfields != null && errorObject.nonfields.length > 0) {
                for (var i = 0; i < errorObject.nonfields.length; i++) {
                    return errorObject.nonfields[i];
                }

                return null;
            } else if (errorObject.fields != null) {
                for (fieldName in errorObject.fields) {
                    return errorObject.fields[fieldName];
                }

                return null;
            } else if (errorObject.message != null) {
                return errorObject.message;
            } else {
                return null;
            }
        } else {
            return null;
        }
    }

    function displayErrorDialog(errorMessage) {
        var dialog = new MessageDialog({
            title: Messages.get("files.upload.error.title"),
            message: errorMessage,
            type: "error",
            selfDestruction: true,
            buttons: [
                {label: Messages.get("common.ok"), value: true},
            ]
        });

        dialog.open();

    }

    function markUploading(uploadId) {
        var _this = this;

        _this.tbody.find("tr").each(function () {
            var row = $(this);
            var file = row.data("file");

            if (file != null && file.type == "TempFile" && file.uploadId == uploadId) {
                row.addClass("uploading");
            }
        });
    }

    function unmarkUploading(uploadId) {
        var _this = this;

        _this.tbody.find("tr").each(function () {
            var row = $(this);
            var file = row.data("file");

            if (file != null && file.type == "TempFile" && file.uploadId == uploadId) {
                row.removeClass("uploading");
            }
        });
    }

    function findRowByUploadId(uploadId) {
        var _this = this;

        var r = null;

        _this.tbody.find("tr").each(function () {
            var row = $(this);
            var file = row.data("file");

            if (file != null && file.type == "TempFile" && file.uploadId == uploadId) {
                r = row;
            }
        });

        return r;
    }

    function setUploadedIdsToTempfile(uploadId, fileId, accessId) {
        for (var i = 0; i < this.files.length; i++) {
            var file = this.files[i];
            if (file.type == "TempFile" && file.uploadId == uploadId) {
                file.id = fileId;
                file.accessId = accessId;
                file.uploaded = true;
            }
        }
    }

    window.FilesPresentation = FilesPresentation;
})(window);

(function (window, undefined) {

    Avatar = function (employee) {
        this.container = $('<img />').addClass("avatar regular");

        this.container.attr("src", employee.avatar);
        ToolTip.setTip(this.container, _.escape(employee.fullName), true);
    };

    Avatar.prototype.getElement = function () {
        return this.container;
    };

    Avatar.prototype.small = function () {
        return this.container.addClass("small").removeClass("regular");
    };

    Avatar.prototype.regular = function () {
        return this.container.removeClass("small").addClass("regular");
    };


    window.Avatar = Avatar;
})(window);
(function (window) {
    ShiftPhoto = function (shift) {
        this.shift = shift;
    };

    ShiftPhoto.prototype.dark = function () {
        this.loader.addClass("dark");
    };


    ShiftPhoto.prototype.getInPhoto = function () {
        return preparePhoto(this.shift.photoInMini, this.shift.photoIn, "in");
    }

    ShiftPhoto.prototype.getOutPhoto = function () {
        return preparePhoto(this.shift.photoOutMini, this.shift.photoOut, "out");
    }


    function preparePhoto(photoMini, photoBig, m) {
        var mode = "";
        var imgClass = "";

        if (m == "in") {
            mode = "in";
            imgClass = "clockInImage";
        } else {
            mode = "out"
            imgClass = "clockOutImage";
        }

        if (photoMini != null) {
            var imageContainer = $("<div />").addClass(imgClass);

            var miniSpinner = new Spinner();
            miniSpinner.dark();
            miniSpinner.small();
            miniSpinner.pinTo(imageContainer);
            miniSpinner.show();

            var downloadingMiniImage = new Image();

            downloadingMiniImage.onload = function () {
                var img = $('<img />')
                    .attr("src", photoMini)
                    .appendTo(imageContainer);

                miniSpinner.hide();
            }
            downloadingMiniImage.src =  photoMini;

            var toolTipContainer = $('<div class="ShiftPhotoToolTip" />').text(Messages.get("timesheet.photo." + mode + ".long"));
            var bigImgContainer = $('<div class="img" />');
            toolTipContainer.append(bigImgContainer);

            var spinner = new Spinner();
            spinner.pinTo(bigImgContainer);
            spinner.show();

            imageContainer.data("bigImgContainer", bigImgContainer);
            imageContainer.data("spinner", spinner);
            imageContainer.data("bigImg", photoBig);

            ToolTip.setTip(imageContainer, $("<div />").append(toolTipContainer), true, false);
            imageContainer.on("onTooltipShow", function () {
                var _this = this;
                var downloadingImage = new Image();

                downloadingImage.onload = function () {
                    $(_this).data("spinner").hide();
                    $(_this).data("bigImgContainer").attr("style", "background-image: url('" + $(_this).data("bigImg") + "')");
                }
                downloadingImage.src = $(_this).data("bigImg");

            });

            return imageContainer;
        }
    }


    window.ShiftPhoto = ShiftPhoto;
})(window);









(function (window, undefined) {

    var notificationsContainer = $('<div id="NotificationsWidget-Container" />').appendTo("body");

    Notification = function (options) {
        this.settings = $.extend({
            text: null,
            html: null,
            contentElement: null,
            icon: null,
            actions: [],
            timeout: 6000
        }, options);

        var _this = this;
        this.box = $('<div class="Notification preInAnimation" />');
        this.contentContainer = $('<div class="Notification-Content" />').appendTo(this.box);
        this.heightExpander = $('<div style="height: 0px; width: 0px;">');

        if (this.settings.icon != null) {
            this.box.addClass("withIcon");

            var iconContainer = $('<div class="IconContainer" />').appendTo(this.box);
            $('<div class="IconElement" />').addClass(this.settings.icon).appendTo(iconContainer);
        }

        if (this.settings.text != null) {
            this.contentContainer.text(this.settings.text);
        } else if (this.settings.html != null) {
            this.contentContainer.html(this.settings.html);
        } else if (this.settings.contentElement != null) {
            this.contentContainer.append(this.settings.contentElement);
        }

        if (this.settings.actions != null && this.settings.actions.length > 0) {

            if (this.settings.timeout == null || this.settings.timeout < 8000) {
                this.settings.timeout = 8000;
            }

            var actionsContainer = $('<div class="Actions" />');

            for (var i = 0; i < this.settings.actions.length; i++) {
                var action = this.settings.actions[i];
                $('<a href="javascript: void(0);"  />')
                    .text(action.label.toLowerCase())
                    .data("action", action)
                    .data("notification", _this)
                    .appendTo(actionsContainer)
                    .click(function () {
                        $(this).data("action").action();
                        $(this).data("notification").hide();
                    });
            }

            actionsContainer.appendTo(this.box);
        }
    }

    Notification.prototype.show = function (options) {
        var _this = this;

        this.heightExpander.prependTo(notificationsContainer);
        var notificationHeight = calculateNotificationDesiredHeight.call(this);

        $(this.heightExpander).velocity({
            height: notificationHeight + 10
        }, {
            duration: 200,
            easing: "easeInOutSine",
            complete: function () {
                _this.box.prependTo(notificationsContainer);
                Animator.animateIn(_this.box, "fadeFromTop", function () {

                });
            }
        });

        if (this.settings.timeout != null && this.settings.timeout > 0) {
            setTimeout(function () {
                _this.hide();
            }, this.settings.timeout);
        }
    }

    Notification.prototype.hide = function (options) {
        var _this = this;

        Animator.animateOut(_this.box, "fadeFromTop", function () {
        });

        $(this.heightExpander).velocity({
            height: 0
        }, {
            duration: 350,
            easing: "easeInOutSine",
            complete: function () {

            }
        });

        setTimeout(function () {
            _this.heightExpander.remove();
            _this.box.remove();
        }, 5000);
    }

    //Notification private--------------

    function calculateNotificationDesiredHeight() {
        this.box.css("visibility", "hidden");
        this.box.appendTo(notificationsContainer);

        var desiredHeight = $(this.box).outerHeight();

        this.box.detach();
        this.box.css("visibility", "visible");

        return desiredHeight;
    }

    //--------------

    Notifications = function () {

    }

    Notifications.prototype.show = function (options) {
        var notification = new Notification(options);
        notification.show();
    }

    window.Notifications = new Notifications();

})(window);
(function (window, undefined) {

    WithEmployeeNotification = function () {

    }

    WithEmployeeNotification.prototype.show = function (options) {
        var settings = $.extend({
            text: null,
            html: null,
            employee: null
        }, options);

        var avatar = null;

        if (settings.employee != null) {
            avatar = new Avatar(settings.employee);
            avatar.small();
        }

        var content = $("<div />");

        if(settings.text!=null){
            content.text(settings.text);
        }else if(settings.html!=null) {
            content.html(settings.html);
        }

        if (avatar != null) {
            content.prepend(avatar.getElement());
        }

        Notifications.show({
            contentElement: content,
            timeout: 5000,
        });
    }

    window.WithEmployeeNotification = new WithEmployeeNotification();

})(window);
(function (window, undefined) {


    ColorPicker = function (fieldSelector, color) {
        this.fieldSelector = fieldSelector;

        if ("random" === color) {
            $(fieldSelector).val(randomColor());
        } else if (_.isString(color) && _.startsWith(color, "#")) {
            $(fieldSelector).val(color);
        }

        setup(fieldSelector);
    }

    ColorPicker.prototype.destroy = function (url) {
        $(this.fieldSelector).colorPicker.destroy();
    }

    function setup(selector) {
        var settings = {
            doRender: 'div div',
            css: '  ',

            buildCallback: function ($elm) {
                var colorInstance = this.color,
                    colorPicker = this;

                $elm.prepend('<div class="cp-panel">' +
                    '<div class="onerow">R <input type="text" class="cp-r" /></div>' +
                    '<div class="onerow">G <input type="text" class="cp-g" /></div>' +
                    '<div class="onerow">B <input type="text" class="cp-b" /></div>' +
                    '<div class="onerow cp-HEX-row "># <input type="text" class="cp-HEX" /></div>' +
                    '</div>').on('change', 'input', function (e) {
                    var value = this.value,
                        className = this.className,
                        type = className.split('-')[1],
                        color = {};

                    color[type] = value;
                    colorInstance.setColor(type === 'HEX' ? value : color,
                        type === 'HEX' ? 'HEX' : /(?:r|g|b)/.test(type) ? 'rgb' : 'hsv');
                    colorPicker.render();
                    this.blur();
                });
            },


            renderCallback: function ($elm, toggled) {
                var colors = this.color.colors.RND,
                    modes = {
                        r: colors.rgb.r, g: colors.rgb.g, b: colors.rgb.b,
                        h: colors.hsv.h, s: colors.hsv.s, v: colors.hsv.v,
                        HEX: this.color.colors.HEX
                    };

                $('input', '.cp-panel').each(function () {
                    this.value = modes[this.className.substr(3)];
                });
            }
        };


        $(selector).attr("autocomplete", "off");
        $(selector).colorPicker(settings);
    }


    function randomColor() {
        var red = _.random(80, 200, false).toString(16);
        var green = _.random(80, 200, false).toString(16);
        var blue = _.random(80, 200, false).toString(16);

        var color = "#" + red + green + blue;
        return color;
    }


    window.ColorPicker = ColorPicker;

})(window);
(function (window, undefined) {

    ContextShiftDetails = function () {
        var _this = this;

        this.contentContainer = $('<div class="ContextShiftDetails"/>');

        this.employeeContainer = $('<h2 class="employee"/>').appendTo(this.contentContainer);
        this.dateContainer = $('<div class="date"/>').appendTo(this.contentContainer);

        this.dayDescriptionContainer = $('<div class="dayDetails"/>').appendTo(this.contentContainer);
        this.dayButtonsContainer = $('<div class="buttonsContainer" />').appendTo(this.contentContainer);
        this.shiftsContainer = $('<div class="shifts"/>').appendTo(this.contentContainer);

        this.dateTemplate = doT.template('<h2>{{!it.date}}<span class="weekdayName">{{!it.weekdayName}}</span></h2>');

        this.timingTableTemplate = doT.template('<table class="presentationTable2 invisibleHeader topAligned"></table>');
        this.timingRowTemplate = doT.template('<tr><td>{{!it.label}}</td><td>{{!it.val1}}</td><td>{{=it.val2}}</td></tr>');
        this.mapRowTemplate = doT.template('<tr><td>{{!it.label}}</td><td colspan="2">' +
            '<img src="' +
            'https://maps.googleapis.com/maps/api/staticmap?' +
            'center={{=it.latitude}},{{=it.longitude}}&size=400x200&zoom=14&markers=color:0x279c83|size:big|{{=it.latitude}},{{=it.longitude}}&key={{=it.apiKey}}' +
            '" width="400" height="200" />' +
            '</td></tr>');

        this.breaksTableTemplate = doT.template('<table class="presentationTable2 invisibleHeader topAligned"></table>');
        this.breaksRowTemplate = doT.template('<tr><td>{{!it.typeName}}</td><td>{{!it.time}}</td><td>{{!it.duration}}</td></tr>');

        this.projectsTableTemplate = doT.template('<table class="presentationTable2 invisibleHeader topAligned"></table>');
        this.projectsRowTemplate = doT.template('<tr><td><div class="project-marker {{!it.additionalMarkerClass}}" style="background-color: {{!it.color}}"></div>{{!it.name}}</td><td>&nbsp;</td><td>{{!it.loggedDuration}}</td></tr>');

        this.abnormalitiesTableTemplate = doT.template('<table class="presentationTable2 invisibleHeader topAligned abnormalities"></table>');
        this.abnormalitiesRowTemplate = doT.template('<tr><td><span class="workReport_summaryIcon_{{=it.mark}}"></span></td><td>{{!it.name}}</td></tr>');

        //managers
        this.timesheetManager = new TimesheetAdvManager();


        //----buttons
        this.dayButtonCreate = $('<button class="ghostButton withIcon iconAdd" />')
            .text(Messages.get("clockin.work.edit.addShift"))
            .appendTo(this.dayButtonsContainer)
            .click(function () {
                _this.timesheetManager.openCreateEntryWindow($(this).data("dayMoment"), $(this).data("employeeId"));
            });

        this.reloadHandler = function () {
        }
    }

    ContextShiftDetails.prototype.prepareContent = function (employee, day) {
        this.employeeContainer.text(employee.fullName);

        this.dateContainer.html(this.dateTemplate({
            date: DateTimeTool.fromStrutsDateToDateDisplayable(day.date),
            weekdayName: DateTimeTool.getDayShortName(moment(day.date))
        }));

        this.dayDescriptionContainer.empty();

        if (day.hired != null && day.hired == false) {
            $('<div class="notHired" />')
                .text(Messages.get("workPlan.notHired"))
                .appendTo(this.dayDescriptionContainer);
        }

        for (var i = 0; i < day.dayOff.length; i++) {
            var dayOffDescriptor = day.dayOff[i];

            if (dayOffDescriptor.reason == "HOLIDAY") {
                $('<div class="dayoff HOLIDAY" />')
                    .text(dayOffDescriptor.name)
                    .appendTo(this.dayDescriptionContainer);
            } else if (dayOffDescriptor.reason == "LEAVE") {
                $('<div class="dayoff LEAVE" />')
                    .text(dayOffDescriptor.name)
                    .appendTo(this.dayDescriptionContainer);
            } else if (dayOffDescriptor.reason == "REMOTE") {
                $('<div class="dayoff REMOTE" />')
                    .text(dayOffDescriptor.name)
                    .appendTo(this.dayDescriptionContainer);
            }
        }

        if (day.security.create == true) {
            this.dayButtonCreate.show();

            this.dayButtonCreate
                .data("dayMoment", moment(day.date))
                .data("employeeId", employee.id);
        } else {
            this.dayButtonCreate.hide();
        }

        //todo tak samo oznaczyć dzień pracujący
        //todo oznaczyć podsumowanie OK/WARN/ERROR dla całego dnia

        this.shiftsContainer.empty();

        for (var i = 0; i < day.shifts.length; i++) {
            var shift = day.shifts[i];
            this.shiftsContainer.append(prepareShiftDetailsContent(this, shift));
        }


    };


    ContextShiftDetails.prototype.getContent = function () {
        return this.contentContainer;
    }

    ContextShiftDetails.prototype.setReloadHandler = function (f) {
        this.reloadHandler = f;
    }

    //------------------------------------------------------------------------------------------------------------------


    function prepareShiftDetailsContent(_this, shift) {
        var shiftContainer = $('<div class="shift formGroup"/>');

        if (shift.entryType == "PLAN") {
            $("<h2 / >").text(Messages.get("clockin.timesheet.plan")).appendTo(shiftContainer);
        } else if (shift.entryType == "SHIFT") {
            $("<h2 / >").text(Messages.get("clockin.timesheet.shift")).appendTo(shiftContainer);
        } else if (shift.entryType == "PENDING_SHIFT") {
            $("<h2 / >").text(Messages.get("clockin.timesheet.unapprovedShift")).appendTo(shiftContainer);
        }

        if (shift.mark != "NONE" && shift.mark != "OK") {
            $('<div class="shiftIcon"><span class="iconMark workReport_summaryIcon_' + shift.mark + '"></span></div>').appendTo(shiftContainer);
        }

        if (shift.changeRequestId != null) {
            $('<div class="shiftIcon"><span class="iconPending"></span></div>').appendTo(shiftContainer);
        }

        var buttonsContainer = $('<div class="buttons" />').appendTo(shiftContainer);

        if (shift.changeRequestId != null) {
            $('<button class="ghostButton onlyIcon iconInfo small" />')
                .text(Messages.get("common.details"))
                .appendTo(buttonsContainer)
                .click(function () {
                    _this.timesheetManager.openShiftChangeRequestDetails(shift.changeRequestId);
                });
        }

        if (shift.security.edit == true) {
            $('<button class="ghostButton onlyIcon iconEdit small" />')
                .text(Messages.get("common.edit"))
                .appendTo(buttonsContainer)
                .click(function () {
                    _this.timesheetManager.openEditShiftWindow(shift.id);
                });
        }

        if (shift.security.delete == true) {
            $('<button class="ghostButton onlyIcon iconDelete small" />')
                .text(Messages.get("common.delete"))
                .appendTo(buttonsContainer)
                .click(function () {
                    _this.timesheetManager.claimShiftDelete(shift.id).done(function (response) {
                        if (response.changeMode == "CHANGE_REQUEST") {
                            showChangeRequestCreatedDialog().done(function () {
                                reloadReport(_this);
                            });
                        } else {
                            reloadReport(_this);
                        }
                    });
                });
        }


        if (shift.entryType == "SHIFT") {
            var clockInOutContainer = $('<div class="clockInOut" />').appendTo(shiftContainer);

            var inContainer = $('<div class="in" />')
                .appendTo(clockInOutContainer)
                .append('<div class="time">' + DateTimeTool.fromStrutsTimeToDisplayable(shift.started) + '</div>');

            if (shift.terminalStarted != null) {
                var terminalDesc = $('<div class="terminal iconTerminal' + Utils.capitalize(shift.terminalStarted.type) + '" />').text(shift.terminalStarted.name);
                inContainer.append(terminalDesc);
            }

            var shiftPhoto = new ShiftPhoto(shift);

            var inPhoto = shiftPhoto.getInPhoto();
            if (inPhoto != null) {
                inContainer.append(inPhoto);
            }
        }


        var inDetailsTable = $(_this.timingTableTemplate().trim()).appendTo(shiftContainer);


        var workedTimeText = "";

        if (shift.started == null && shift.finished == null) {
            workedTimeText = "-";
        } else {
            if (shift.started != null)
                workedTimeText += DateTimeTool.fromStrutsTimeToDisplayable(shift.started);

            workedTimeText += " - ";

            if (shift.finished != null)
                workedTimeText += DateTimeTool.fromStrutsTimeToDisplayable(shift.finished);
        }

        inDetailsTable.append(_this.timingRowTemplate({
            label: Messages.get("clockin.report.timesheet.workedTime"),
            val1: workedTimeText,
            val2: DateTimeTool.secToDisplayableDuration(shift.workedSeconds)
        }).trim());


        if (shift.breaks.length > 0) {
            var breaksTable = $(_this.breaksTableTemplate().trim()).appendTo(shiftContainer);
            breaksTable.before('<h3>' + Messages.get('clockin.report.breaks') + '</h3>');


            for (var i = 0; i < shift.breaks.length; i++) {
                var breakEntry = shift.breaks[i];

                breaksTable.append(_this.breaksRowTemplate({
                    typeName: breakEntry.typeName,
                    time: DateTimeTool.fromStrutsTimeToDisplayable(breakEntry.started) + " - " + (breakEntry.finished != null ? DateTimeTool.fromStrutsTimeToDisplayable(breakEntry.finished) : "..."),
                    duration: breakEntry.secondsDuration != null ? DateTimeTool.secToDisplayableDuration(breakEntry.secondsDuration) : "-"
                }).trim());
            }
        }

        ////////////////////////////////PROJETS-------------------------------------------------------------------------
        var projectsTable = $(_this.projectsTableTemplate().trim()).appendTo(shiftContainer);
        var projectsHeader = $('<h3>' + Messages.get('projects') + '</h3>').insertBefore(projectsTable);
        var dataLoadingContainer = $('<div class="dataLoadingContainer" />').insertAfter(projectsHeader);
        loadProjectsForShift.apply(_this, [shift, projectsTable, dataLoadingContainer, projectsHeader]);
        ////////////////////////////////PROJETS-------------------------------------------------------------------------

        var plannedTimeText = '-';

        if (shift.plannedStart != null && shift.plannedFinish != null) {
            plannedTimeText = DateTimeTool.fromStrutsTimeToDisplayable(shift.plannedStart) + " - " + DateTimeTool.fromStrutsTimeToDisplayable(shift.plannedFinish);

        }

        inDetailsTable.append(_this.timingRowTemplate({
            label: Messages.get('clockin.report.timesheet.regularTime'),
            val1: plannedTimeText,
            val2: shift.plannedSeconds != null ? DateTimeTool.secToDisplayableDuration(shift.plannedSeconds) : '-'
        }).trim());

        if (shift.locationStarted != null) {
            inDetailsTable.append(_this.mapRowTemplate({
                label: Messages.get("clockin.report.timesheet.locationStarted"),
                apiKey: "AIzaSyCHg88kwbIJLcezTTFJUSyIVgBHuccRy8k",
                longitude: shift.locationStarted.longitude,
                latitude: shift.locationStarted.latitude
            }).trim());
        }

        if (shift.locationFinished != null) {
            inDetailsTable.append(_this.mapRowTemplate({
                label: Messages.get("clockin.report.timesheet.locationFinished"),
                apiKey: "AIzaSyCHg88kwbIJLcezTTFJUSyIVgBHuccRy8k",
                longitude: shift.locationFinished.longitude,
                latitude: shift.locationFinished.latitude
            }).trim());
        }

        if (shift.changeRequestId != null) {
            var pendingRow = $(_this.timingRowTemplate({
                label: Messages.get('clockin.timesheet.pending'),
                val1: Messages.get("ShiftChangeRequestMode.details.waitingFor." + shift.chrqMode),
                val2: '<button class="detailsButton withIcon small iconInfo ghostButton">' + Messages.get("common.details") + '</button>'
            }).trim());

            pendingRow.find(".detailsButton").click(function () {
                _this.timesheetManager.openShiftChangeRequestDetails(shift.changeRequestId);
            });

            inDetailsTable.append(pendingRow);
        }


        if (shift.entryType == "SHIFT") {
            var outText = "";

            if (shift.finished != null) {
                outText = DateTimeTool.fromStrutsTimeToDisplayable(shift.finished);
            } else {
                if (shift.opened == true) {
                    outText = "...";
                } else {
                    outText = "???";
                }
            }

            var outContainer = $('<div class="out" />')
                .appendTo(clockInOutContainer)
                .append('<div class="time">' + outText + '</div>');

            if (shift.terminalFinished != null) {
                var terminalDesc = $('<div class="terminal iconTerminal' + Utils.capitalize(shift.terminalFinished.type) + '" />').text(shift.terminalFinished.name);
                outContainer.append(terminalDesc);
            }


            var shiftPhoto = new ShiftPhoto(shift);

            var outPhoto = shiftPhoto.getOutPhoto();
            if (outPhoto != null) {
                outContainer.append(outPhoto);
            }

        }


        if (shift.abnormalities.length > 0) {
            var abnormalitiesTable = $(_this.abnormalitiesTableTemplate().trim()).appendTo(shiftContainer);
            abnormalitiesTable.before('<h3>' + Messages.get('clockin.work.abnormalities') + '</h3>');


            for (var i = 0; i < shift.abnormalities.length; i++) {
                var abnormality = shift.abnormalities[i];

                abnormalitiesTable.append(_this.abnormalitiesRowTemplate({
                    mark: abnormality.mark,
                    name: Messages.get("abnormality." + abnormality.type)
                }).trim());
            }
        }


        return shiftContainer;
    }


    function showChangeRequestCreatedDialog() {
        var showing = $.Deferred();

        var dialog = new MessageDialog({
            title: Messages.get("clockin.shiftChangeRequest.created.title"),
            type: "info",
            selfDestruction: true,
            buttons: [
                {label: Messages.get("common.ok"), value: true},
            ],
            callback: function (answer) {
                showing.resolve();
            }
        });

        dialog.open();

        return showing;
    }

    function reloadReport(_this) {
        _this.reloadHandler();
    }


    function loadProjectsForShift(shift, projectsTable, dataLoadingContainer, projectsHeader) {
        if(shift.id!=null){
            var _this = this;

            var miniSpinner = new Spinner();
            miniSpinner.dark();
            miniSpinner.small();
            miniSpinner.pinTo(dataLoadingContainer);
            miniSpinner.show();

            this.timesheetManager.getProjectsForShift(shift.id).done(function(response){

                for(var i=0; i<response.length; i++){
                    var project = response[i];

                    projectsTable.append(_this.projectsRowTemplate({
                        color: project.projectName!=null ? project.projectColor : "",
                        additionalMarkerClass: project.projectName==null ? "anyProject" : "",
                        name: project.projectName!=null ? project.projectName : Messages.get("worklog.anyProject"),
                        loggedDuration: DateTimeTool.secToDisplayableDuration(project.seconds)
                    }).trim());

                }

                dataLoadingContainer.hide();
                miniSpinner.hide();
            });
        }else{
            projectsTable.hide();
            dataLoadingContainer.hide();
            projectsHeader.hide();
        }
    }
})(window);

(function (window, undefined) {
    var toolTipElement;
    var showing = false;
    var tooltipX = 0;
    var tooltipY = 0;
    var toolTipMargin = 8;
    var bodyWidth;
    var bodyHeight;

    function setTip(element, content, gluedToElement, immediateShow) {
        if (content != null && content != "") {
            $(element)
                .data("tooltip", true)
                .data("tooltip-content", content);

            if (true === gluedToElement) {
                $(element).data("tooltip-glued");
            }
        }

        if (immediateShow == true) {
            showToolTip(element);
        }

        $(element)
            .on('mouseenter', function (event) {
                showToolTip(element, event);
            })
            .on('mouseleave', function (event) {
                let enteredElementIsTooltip = toolTipElement.find(event.relatedTarget).length > 0;

                if(enteredElementIsTooltip){
                    $(toolTipElement).one("mouseleave", function(){
                        hideToolTip(element, event);
                    });
                }else{
                    hideToolTip(element, event);
                }
            })
            .on('mousemove', function (event) {
                updatePosition(event);
            });
    };

    function showToolTip(e, mouseEvent) {
        var element = $(e);
        var content = $(element).data("tooltip-content");
        var contentLabel = $(element).data("tooltip-content-label");

        if(content==null || content==""){
            if(contentLabel!=null){
                content = Messages.get(contentLabel);
            }
        }

        updatePosition(mouseEvent, true);

        if (content != null && content != "") {
            toolTipElement.empty().append(content);
            toolTipElement.show();
        }

        element.trigger("onTooltipShow");

        showing = true;
    }

    function hideToolTip(element, mouseEvent) {
        showing = false;
        toolTipElement.hide();
    }

    function hideAllTooltips() {
        showing = false;
        toolTipElement.hide();
    }

    function updatePosition(mouseEvent, force) {
        if (mouseEvent != null && (showing || force)) {
            var tooltipElementWidth = toolTipElement.outerWidth();
            var tooltipElementHeight = toolTipElement.outerHeight();
            var mouseX = mouseEvent.clientX;
            var mouseY = mouseEvent.clientY;

            var tooMuchRight = tooltipElementWidth+mouseX+toolTipMargin-bodyWidth;
            var deltaX = tooMuchRight > 0 ? tooMuchRight : 0;

            var tooMuchTop = tooltipElementHeight+mouseY+toolTipMargin-bodyHeight;
            var deltaY = tooMuchTop > 0 ? tooMuchTop : 0;

            tooltipX = mouseEvent.clientX +toolTipMargin - deltaX + 5;
            tooltipY = mouseEvent.clientY +toolTipMargin- deltaY + 5;

            toolTipElement
                .css("left", tooltipX + "px")
                .css("top", tooltipY + "px");
        }
    }

    function init() {
        toolTipElement = $('<div style="display: none;" class="ToolTip"/>').appendTo("body");

        $(document)
            .on('mouseenter', '[data-tooltip="true"]', function (event) {
                showToolTip(this, event);
            })
            .on('mouseleave', '[data-tooltip="true"]', function (event) {
                hideToolTip(this, event);
            })
            .on('mousemove', function (event) {
                updatePosition(event);
            });

        $(window).on("resize", function(){
            bodyWidth = $("body").innerWidth();
            bodyHeight = window.innerHeight;
        });


        bodyWidth = $("body").innerWidth();
        bodyHeight = window.innerHeight;
    }

    ToolTip = function () {
    }

    ToolTip.prototype.setTip = function (element, content, glued, immediateShow) {
        setTip(element, content, true === glued, true===immediateShow);
    }

    ToolTip.prototype.hideAll = function () {
        hideAllTooltips();
    }

    window.ToolTip = new ToolTip();
    init();
})(window);
(function (window, undefined) {
    var defaultRect = {
        left: 0,
        top: 0,
        width: 0,
        height: 0
    }

    var INTRO_FORWARD = 1;
    var INTRO_BACKWARD = -1;

    Intro = function (options) {
        var _this = this;

        this.currentStep = 0;
        this.boxMargin = 20;
        this.timeout = 200;
        this.loading = false

        this.settings = $.extend({
            destroyOnFinish: false,
            showNextTutorial: false,
            onNextTutorial: function () {
            },
            onFinish: function () {
            },
            steps: []
        }, options);

        this.box = $('<div class="intro-box"/>')
            .addClass("hidden")
            .appendTo("body");

        this.messageBox = $('<div class="intro-box-text"/>')
            .appendTo(this.box);

        this.controlsBox = $('<div class="intro-box-controls"><button class="intro-button-prev"/><button class="intro-button-next"/><button class="intro-button-nextTutorial"/><button class="intro-button-finish"/></div>')
            .appendTo(this.box);

        this.progressBox = $('<div class="intro-progress-box" />')
            .appendTo(this.box);

        this.progressProgress = $('<div class="intro-progress-progress" />')
            .appendTo(this.box);

        this.lockingScreen1 = $('<div class="intro-lockingScreen"/>').addClass("hidden")
            .css("top", "0")
            .appendTo("body");

        this.lockingScreen2 = $('<div class="intro-lockingScreen"/>').addClass("hidden")
            .css({
                "top": 0,
                "bottom": 0,
                "right": 0
            })
            .appendTo("body");

        this.lockingScreen3 = $('<div class="intro-lockingScreen"/>').addClass("hidden")
            .css("bottom", "0")
            .appendTo("body");

        this.lockingScreen4 = $('<div class="intro-lockingScreen"/>').addClass("hidden")
            .css({
                "top": 0,
                "left": 0,
                "bottom": 0
            })
            .appendTo("body");

        this.buttonPrev = this.controlsBox.find(".intro-button-prev");
        this.buttonNext = this.controlsBox.find(".intro-button-next");
        this.buttonFinish = this.controlsBox.find(".intro-button-finish");
        this.nextTutorial = this.controlsBox.find(".intro-button-nextTutorial");


        this.buttonPrev.html("&lt; " + Messages.get("common.prev")).click(function () {
            _this.prevStep();
        });

        this.buttonNext.html(Messages.get("common.next") + " &gt;").click(function () {
            _this.nextStep();
        });

        this.buttonFinish.html(Messages.get("common.finish")).click(function () {
            _this.finish();
        });

        this.nextTutorial.html(Messages.get("common.nextTutorial")).click(function () {
            _this.showNextTutorial();
        });

        this.resizingHandler = function (event) {
            if (!_this.stepLoading) {
                var step = _this.settings.steps[_this.currentStep];
                var rect = _this.getLockingHoleRect(step);
                _this.resizeLockingScreen(rect, step);
                _this.moveBoxToPosition(rect, step.messagePosition);
            }
        }
    };


    Intro.prototype.start = function () {
        var _this = this;
        this.currentStep = 0;

        $(this.box).css("display", "block").addClass("hidden");
        $(this.lockingScreen1).css("display", "block").addClass("hidden");
        $(this.lockingScreen2).css("display", "block").addClass("hidden");
        $(this.lockingScreen3).css("display", "block").addClass("hidden");
        $(this.lockingScreen4).css("display", "block").addClass("hidden");

        this.runStep(this.currentStep, INTRO_FORWARD);

        $(window).on("scroll", _this.resizingHandler);
        $(window).on("resize", _this.resizingHandler);

    };

    Intro.prototype.runStep = function (stepNumber, direction) {
        if (this.settings.steps.length > stepNumber) {
            var _this = this;
            var step = this.settings.steps[stepNumber];
            step.number = stepNumber;

            _this.stepLoading = true;

            var stepTransformation = {
                waitForEvent: null,
                beforeChange: function () {
                }
            }

            if (direction == INTRO_FORWARD) {
                if (step.forward != null)
                    stepTransformation = step.forward;
                else
                    stepTransformation = {
                        waitForEvent: step.waitForEvent,
                        beforeChange: step.beforeChange
                    }
            } else if (direction == INTRO_BACKWARD) {
                if (step.backward != null)
                    stepTransformation = step.backward;
                else
                    stepTransformation = {
                        waitForEvent: step.waitForEvent,
                        beforeChange: step.beforeChange
                    }
            }

            if(step.always!=null){
                step.always();
            }

            if (stepTransformation.beforeChange != null) {
                stepTransformation.beforeChange();
            }

            if (step.maxWidth != null) {
                _this.box.css("max-width", step.maxWidth);
            } else {
                _this.box.css("max-width", "");
            }

            if (!!stepTransformation.waitForEvent) {
                $(window.eventBus).one(stepTransformation.waitForEvent, function (e) {
                    _this.displayStep(step);
                    _this.stepLoading = false;
                });
            } else {
                _this.displayStep(step);
                _this.stepLoading = false;
            }

        }
    }

    Intro.prototype.displayStep = function (step) {
        var _this = this;


        setTimeout(function () {
            _this.box.addClass("hidden");
            _this.showHideButtons();

            var lockingHoleRect = _this.getLockingHoleRect(step);
            _this.resizeLockingScreen(lockingHoleRect, step);

            _this.lockingScreen1.removeClass("hidden");
            _this.lockingScreen2.removeClass("hidden");
            _this.lockingScreen3.removeClass("hidden");
            _this.lockingScreen4.removeClass("hidden");

            _this.scrollScreen(step);

            _this.messageBox.html(step.message);
            _this.moveBoxToPosition(lockingHoleRect, step.messagePosition);
            _this.box.removeClass("hidden");

            _this.progressBox.text((step.number+1) + " / " + _this.settings.steps.length);

            _this.progressProgress.css("width", ((step.number+1) / _this.settings.steps.length)*100+"%" );
        }, 400);
    }

    Intro.prototype.nextStep = function () {
        this.currentStep++;
        this.runStep(this.currentStep, INTRO_FORWARD);
    }

    Intro.prototype.prevStep = function () {
        this.currentStep--;
        this.runStep(this.currentStep, INTRO_BACKWARD);
    }

    Intro.prototype.showHideButtons = function () {

        if (this.currentStep == 0) {
            this.buttonPrev.hide();
        } else {
            this.buttonPrev.show();
        }

        if (this.currentStep == this.settings.steps.length - 1) {
            this.buttonNext.hide();
            this.buttonFinish.focus();
        } else {
            this.buttonNext.show().focus();
        }

        if(this.settings.showNextTutorial && this.currentStep == this.settings.steps.length - 1) {
            this.nextTutorial.show().focus();
        } else {
            this.nextTutorial.hide();
        }

    }

    Intro.prototype.getLockingHoleRect = function (step) {
        var rect = defaultRect;
        var _this = this;

        if (step.workspaceRect == true) {
            rect = {
                left: 170,
                top: 85,
                width: $(window).width() - 170,
                height: $(window).height() - 85
            }
        } else if (step.element != null) {
            if (!!$(step.element).get(0)) {
                rect = $(step.element).get(0).getBoundingClientRect();
            }
        } else if (step.rect != null) {
            rect = step.rect;
        }

        return rect;
    }


    Intro.prototype.scrollScreen = function (step) {
        if (step.workspaceRect == true) {
            $.scrollTo(0, 0);
        } else if ((step.element != null)) {
            var e = $(step.element);

            if (e != null && e.length > 0) {
                $.scrollTo(0, e.position().top);
            }
        }

        $(window).trigger("scroll");
    }

    Intro.prototype.moveBoxToPosition = function (holeRect, boxPosition) {
        var boxRect = $(this.box).get(0).getBoundingClientRect();
        var newTop = "0px";
        var newLeft = "0px";

        if (boxPosition == "center") {

            newTop = ($(window).height() / 2) - (boxRect.height / 2);
            newLeft = ($(window).width() / 2) - (boxRect.width / 2);

        } else if (boxPosition == "right-top") {

            newTop = holeRect.top;
            newLeft = (holeRect.left + holeRect.width + this.boxMargin);

        } else if (boxPosition == "right-bottom") {

            newTop = (holeRect.top + holeRect.height - boxRect.height);
            newLeft = (holeRect.left + holeRect.width + this.boxMargin);

        } else if (boxPosition == "bottom-right") {

            newTop = (holeRect.top + holeRect.height + this.boxMargin);
            newLeft = (holeRect.left + holeRect.width - boxRect.width);

        } else if (boxPosition == "bottom-left") {

            newTop = (holeRect.top + holeRect.height + this.boxMargin);
            newLeft = (holeRect.left);

        } else if (boxPosition == "left-bottom") {
            newTop = (holeRect.top + holeRect.height - boxRect.height);
            newLeft = (holeRect.left - boxRect.width - this.boxMargin);

        } else if (boxPosition == "left-top") {
            newTop = holeRect.top;
            newLeft = (holeRect.left - boxRect.width - this.boxMargin);


        } else if (boxPosition == "top-left") {
            newTop = (holeRect.top - boxRect.height - this.boxMargin);
            newLeft = (holeRect.left);

        } else if (boxPosition == "top-right") {
            newTop = (holeRect.top - boxRect.height - this.boxMargin);
            newLeft = (holeRect.left + holeRect.width - boxRect.width);

        } else {
            newTop = (holeRect.top + holeRect.height + this.boxMargin);
            newLeft = (holeRect.left);
        }


        if (newTop < 0) {
            newTop = 0 + this.boxMargin;
        }

        if (newLeft < 0) {
            newLeft = 0 + this.boxMargin;
        }

        if (newTop + (boxRect.height + this.boxMargin) > $(window).height()) {
            newTop = $(window).height() - (boxRect.height + this.boxMargin);
        }

        if (newLeft + (boxRect.width + this.boxMargin) > $(window).width()) {
            newLeft = $(window).width() - (boxRect.width + this.boxMargin);
        }

        this.box.css({
            top: newTop + "px",
            left: newLeft + "px"
        });

    }

    Intro.prototype.resizeLockingScreen = function (rect, step) {
        var right_padding = 0;
        var left_padding = 0;
        var bottom_padding = 0;
        var top_padding = 0;

        //TODO: Parse padding
        if (!!step.customPadding) {

            right_padding = step.customPadding;
            left_padding = step.customPadding;
            bottom_padding = step.customPadding;
            top_padding = step.customPadding;
        }

        this.lockingScreen1
            .css({
                height: rect.top - top_padding + "px",
                left: rect.left - left_padding + "px",
                width: rect.width + left_padding + right_padding + "px"
            });

        this.lockingScreen2
            .css({
                left: (rect.left + rect.width) + right_padding + "px"
            });

        this.lockingScreen3
            .css({
                top: (rect.top + rect.height) + bottom_padding + "px",
                left: rect.left - left_padding + "px",
                width: rect.width + left_padding + right_padding + "px"
            });

        this.lockingScreen4
            .css({
                width: rect.left - left_padding + "px"
            });
    }

    Intro.prototype.finish = function () {
        var _this = this;

        $(this.box).addClass("hidden").hide();
        $(this.lockingScreen1).addClass("hidden").hide();
        $(this.lockingScreen2).addClass("hidden").hide();
        $(this.lockingScreen3).addClass("hidden").hide();
        $(this.lockingScreen4).addClass("hidden").hide();

        $(window).off("scroll", _this.resizingHandler);
        $(window).off("resize", _this.resizingHandler);

        this.settings.onFinish();

        if (this.settings.destroyOnFinish == true) {
            this.destroy();
        }

    }

    Intro.prototype.showNextTutorial = function () {
        this.finish();
        this.settings.onNextTutorial();



    }

    Intro.prototype.destroy = function () {
        $(this.box).remove();
        $(this.lockingScreen1).remove();
        $(this.lockingScreen2).remove();
        $(this.lockingScreen3).remove();
        $(this.lockingScreen4).remove();
    }

    window.Intro = Intro;
})
(window);


/*


(function (window) {
    PersonalStoredPropertyManager = function () {

    }

    PersonalStoredPropertyManager.prototype.getProperties = function (propertyNames) {
        var getting = $.Deferred();

        Ajax.post({
            url: URL2.get("/webapi/personal-property/get"),
            contentType: "application/json",
            data: JSON.stringify({
                properties: propertyNames
            })
        }).done(function(response){
            getting.resolve(response.properties);
        }).fail(function(response){
            getting.reject();
        });

        return getting;
    }

    PersonalStoredPropertyManager.prototype.getProperty = function (name) {
        var getting = $.Deferred();

        Ajax.post({
            url: URL2.get("/webapi/personal-property/get"),
            contentType: "application/json",
            data: JSON.stringify({
                properties: [name]
            })
        }).done(function(response){
            getting.resolve(response.properties[name]);
        }).fail(function(response){
            getting.reject();
        });


        return getting.promise();
    }

    PersonalStoredPropertyManager.prototype.saveProperties = function (propertiesMap) {
        return Ajax.post({
            url: URL2.get("/webapi/personal-property/save"),
            contentType: "application/json",
            data: JSON.stringify({
                properties: propertiesMap
            })
        });
    }

    PersonalStoredPropertyManager.prototype.saveProperty = function (name, value) {
        var propertiesMap = {};
        propertiesMap[name] = value;

        return Ajax.post({
            url: URL2.get("/webapi/personal-property/save"),
            contentType: "application/json",
            data: JSON.stringify({
                properties: propertiesMap
            })
        });
    }

    window.PersonalStoredPropertyManager = new PersonalStoredPropertyManager();
})(window);
(function (window, undefined) {

    TimeExploder = function () {
        this.exploded = {
            hours: 0,
            minutes: 0,
            seconds: 0
        };
    };

    TimeExploder.prototype.bySeconds = function (sec) {
        var secAbs = Math.abs(sec);
        var sign = sec < 0 ? -1 : 1;


        var hours = Math.floor(secAbs / (60 * 60));
        var divisor_for_minutes = secAbs % (60 * 60);
        var minutes = Math.floor(divisor_for_minutes / 60);

        var divisor_for_seconds = divisor_for_minutes % 60;
        var seconds = Math.ceil(divisor_for_seconds);

        this.exploded.hours = sign * hours;
        this.exploded.minutes = sign * minutes;
        this.exploded.seconds = sign * seconds;

        return this;
    };

    TimeExploder.prototype.byMinutes = function (minutes) {
        this.bySeconds(minutes * 60);

        return this;
    };

    TimeExploder.prototype.byHours = function (hours) {
        this.bySeconds(hours * 3600);

        return this;
    };

    TimeExploder.prototype.getHoursDisplayable = function () {
        var displayable = "";

        if (this.exploded.hours != 0) {
            displayable = displayable + this.exploded.hours + Messages.get("common.time.hour.short") + '';
        }

        if (this.exploded.minutes != 0) {
            if (displayable == "")
                displayable = displayable + " " + this.exploded.minutes + Messages.get("common.time.minute.short") + '';
            else
                displayable = displayable + " " + Math.abs(this.exploded.minutes) + Messages.get("common.time.minute.short") + '';
        }

        if (displayable == "") {
            displayable = 0 + Messages.get("common.time.hour.short");
        }

        return displayable;
    };


    TimeExploder.prototype.getHoursDisplayablePureNumber = function () {
        var displayable = this.exploded.hours;

        if (this.exploded.minutes != 0) {
            displayable = displayable + ":" + Math.abs(this.exploded.minutes) + Messages.get("common.time.minute.short") + '';
        } else {
            displayable = displayable + ":00";
        }

        return displayable;
    };


    window.TimeExploder = TimeExploder;
})(window);
(function (window) {
    function AbsenceRequestLength() {

    }

    AbsenceRequestLength.prototype.hasFutureAccrueEntitlementToDisplay = function (entitlement) {
        if (entitlement.unlimitedAllowed == true || entitlement.futureAccrue <= 0)
            return false;
        else
            return true;
    }

    AbsenceRequestLength.prototype.hasFutureReduceEntitlementToDisplay = function (entitlement) {
        if (entitlement.unlimitedAllowed == true || entitlement.futureReduce <= 0)
            return false;
        else
            return true;
    }

    AbsenceRequestLength.prototype.futureAccrueEntitlementToDisplayable = function (entitlement) {
        return allowedEntitlementToDisplayable(entitlement.futureAccrue, entitlement, false);
    }

    AbsenceRequestLength.prototype.futureReduceEntitlementToDisplayable = function (entitlement) {
        return allowedEntitlementToDisplayable(entitlement.futureReduce, entitlement, false);
    }

    AbsenceRequestLength.prototype.allowedEntitlementToDisplayable = function (entitlement) {
        return allowedEntitlementToDisplayable(entitlement.allowed, entitlement, false);
    }

    AbsenceRequestLength.prototype.pendingEntitlementToDisplayable = function (entitlement) {
        return entitlementToDisplayable(entitlement.pending, false, entitlement, false);
    }

    AbsenceRequestLength.prototype.usedEntitlementToDisplayable = function (entitlement) {
        return entitlementToDisplayable(entitlement.used, false, entitlement, false);
    }

    AbsenceRequestLength.prototype.anyAmountOfEntitlementDisplayable = function (amount, unlimited, entitlement) {
        return entitlementToDisplayable(amount, unlimited, entitlement, false);
    }

    AbsenceRequestLength.prototype.balanceToDisplayable = function (entitlement) {

        var balanceInfo = {
            initial: entitlementToDisplayable(0, false, entitlement, true),
            accrual: entitlementToDisplayable(0, false, entitlement, true),
            transFromPrevYear: entitlementToDisplayable(0, false, entitlement, true),
            edited: entitlementToDisplayable(0, false, entitlement, true),
            transToNextYear: entitlementToDisplayable(0, false, entitlement, true),
            requested: entitlementToDisplayable(0, false, entitlement, true),
            carriedExpiry: entitlementToDisplayable(0, false, entitlement, true),
            mainTypeRunOut: entitlementToDisplayable(0, false, entitlement, true),
            allowed: entitlementToDisplayable(0, false, entitlement, true),
            unitDisplayable: null
        };


        for (var y in entitlement.balanceSummary) {
            var balanceItem = entitlement.balanceSummary[y];


            balanceInfo.absenceTypeName = entitlement.absenceTypeName;

            switch (balanceItem.reason) {
                case 'INITIAL':
                    balanceInfo.initial = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;

                case 'ACCRUAL':
                    balanceInfo.accrual = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;

                case 'TRANSFER_FROM_PREVIOUS_YEAR':
                    balanceInfo.transFromPrevYear = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;

                case 'MANUAL_EDIT':
                    balanceInfo.edited = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;

                case 'LEAVE_REQUEST':
                    balanceInfo.requested = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;

                case 'TRANSFER_TO_NEXT_YEAR':
                    balanceInfo.transToNextYear = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;

                case 'CARRIED_OVER_EXPIRY':
                    balanceInfo.carriedExpiry = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;

                case 'MAIN_TYPE_RUNOUT':
                    balanceInfo.mainTypeRunOut = entitlementToDisplayable(balanceItem.amount, balanceItem.unlimited, entitlement, true);
                    break;
            }

            balanceInfo.allowed = entitlementToDisplayable(entitlement.allowed, entitlement.unlimitedAllowed, entitlement, true);
            balanceInfo.unitDisplayable = unitDisplayable(entitlement);

        }
        return balanceInfo;
    }

    AbsenceRequestLength.prototype.anyAmountDisplayable = function (amount, unlimited, unit) {
        if (true === unlimited) {
            return Messages.get("entitlement.unlimited");
        } else if ("HOURS" == unit) {
            return hoursForcedToDisplayable(amount);
        } else if ("DAYS" == unit) {
            return daysToDisplayable(amount);
        }
    }

    AbsenceRequestLength.prototype.allowedEntitlementToPureNumber = function (entitlement) {
        return entitlementToDisplayable(entitlement.allowed, entitlement.unlimitedAllowed, entitlement, true);
    }

    AbsenceRequestLength.prototype.pendingEntitlementToPureNumber = function (entitlement) {
        return entitlementToDisplayable(entitlement.pending, false, entitlement, true);
    }

    AbsenceRequestLength.prototype.usedEntitlementToPureNumber = function (entitlement) {
        return entitlementToDisplayable(entitlement.used, false, entitlement, true);
    }

    AbsenceRequestLength.prototype.anyAmountOfEntitlementPureNumber = function (amount, unlimited, entitlement) {
        return entitlementToDisplayable(amount, unlimited, entitlement, true);
    }

    AbsenceRequestLength.prototype.anyAmountPureNumber = function (amount, unlimited, unit) {
        if (true === unlimited) {
            return '∞';
        } else if ("HOURS" == unit) {
            return hoursForcedToDisplayable(amount);
        } else if ("DAYS" == unit) {
            return daysToDisplayable(amount);
        }
    }

    AbsenceRequestLength.prototype.effectiveUnitDisplayable = function (entitlement) {
        return unitDisplayable(entitlement);
    }


    //private func
    function allowedEntitlementToDisplayable(amount, entitlement, pureNumber) {
        if(entitlement.entitlementType=="UNLIMITED"){
            if (pureNumber) {
                return '∞';
            } else {
                return Messages.get("entitlement.unlimited");
            }
        }else if (entitlement.entitlementType=="UNDEFINED") {
            if (pureNumber) {
                return '?';
            } else {
                return Messages.get("entitlement.undefined");
            }
        } else if ("HOURS" == entitlement.unit) {
            return hoursToDisplayable(amount, entitlement.displayHoursAsDays, entitlement.secondsPerDay, pureNumber);
        } else if ("DAYS" == entitlement.unit) {
            return daysToDisplayable(amount, pureNumber);
        }
    }


    function entitlementToDisplayable(amount, unlimitedAmount, entitlement, pureNumber) {
        if (true === unlimitedAmount) {
            if (pureNumber) {
                return '∞';
            } else {
                return Messages.get("entitlement.unlimited");
            }
        } else if ("HOURS" == entitlement.unit) {
            return hoursToDisplayable(amount, entitlement.displayHoursAsDays, entitlement.secondsPerDay, pureNumber);
        } else if ("DAYS" == entitlement.unit) {
            return daysToDisplayable(amount, pureNumber);
        }
    }

    function unitDisplayable(entitlement) {

        if ("HOURS" == entitlement.unit && entitlement.displayHoursAsDays == true) {
            return Messages.get("absenceType.durationUnit.DAYS");
        } else if ("HOURS" == entitlement.unit && entitlement.displayHoursAsDays == false) {
            return Messages.get("absenceType.durationUnit.HOURS")
        } else if ("DAYS" == entitlement.unit) {
            return Messages.get("absenceType.durationUnit.DAYS")
        }

    }

    function hoursToDisplayable(amount, displayHoursAsDays, secondsPerDay, pureNumber) {
        if (displayHoursAsDays) {
            return daysToDisplayable((amount * 1000) / secondsPerDay, pureNumber);//todo tego tysiąca to pobierać skądś
        } else {
            var t = new TimeExploder();
            t.bySeconds(amount);

            if (pureNumber)
                return t.getHoursDisplayablePureNumber();
            else
                return t.getHoursDisplayable();
        }
    }

    function hoursForcedToDisplayable(amount) {
        var t = new TimeExploder();
        t.bySeconds(amount);
        return t.getHoursDisplayable();
    }

    function daysToDisplayable(amount, pureNumber) {
        var indays = (amount / 1000); //amount is in 1/0100 of day //TODO może to pobierać

        //TODO usunąc zaokrąlenie
        if (pureNumber == true) {
            return parseFloat(indays.toFixed(2));
        } else {
            if (indays == 1) {
                return parseFloat(indays.toFixed(2)) + " " + Messages.get("common.time.day");
            } else {
                return parseFloat(indays.toFixed(2)) + " " + Messages.get("common.time.days");
            }
        }
    }

    window.AbsenceRequestLength = new AbsenceRequestLength();
})(window);
(function (window, undefined) {

    GoogleToolkit = function () {
    };


    GoogleToolkit.prototype.handleInsufficientPermissionError = function (errorPayload, callbackOk, callbackError) {
        var url = "https://admin.google.com/AdminHome?fral=1#AppDetails:service=" + Messages.get("employees.google.import.app.number") + "&flyout=data";

        var googleInsufficientPermissionsDialog = new MessageDialog({
            title: Messages.get("security.error.google.insufficientPermissions.title"),
            message: Messages.get("security.error.google.insufficientPermissions", url),
            type: 'info access_denied',
            selfDestruction: true,
            buttons: [
                {
                    label: Messages.get("common.yes"),
                    value: true
                },
                {
                    label: Messages.get("common.no"),
                    value: false
                }
            ]
        });

        googleInsufficientPermissionsDialog.open({
            callback: function (answer) {
                if (answer) {
                    //var scopes = $.param({
                    //    scopes: errorPayload.scopes
                    //}, true);

                    //var url = Url.getActionUrl('google', 'upgradeGooglePrivileges') + '?' + scopes;
                    //console.info(url);
                    var googlePopup = window.open(url, "googlePopupWidow", "top=0; left=0");

                    if (googlePopup != null) {
                        googlePopup.focus();

                        var checkingInterval = setInterval(function () {
                            if (googlePopup.closed) {
                                clearInterval(checkingInterval);
                                callbackOk();
                            }
                        }, 600);
                    } else {
                        callbackError();
                    }
                } else {
                    callbackError();
                }
            }
        });

    };


    window.GoogleToolkit = new GoogleToolkit();
})(window);
(function (window, undefined) {

    JSErrorManager = function (options) {
        this.lastMessage = null;
        this.sendingEnabled = true;
    }

    JSErrorManager.prototype.logError = function (msg, errorType, file, line, column, errorObj) {
        if (this.sendingEnabled == false || this.lastMessage == msg) {
            return;
        }

        var _this = this;

        var stack = "";
        if (errorObj != null && errorObj.stack != null) {
            stack = errorObj.stack;
        }

        var payload = {
            errorType: errorType == undefined ? "-" : errorType,
            msg: msg == undefined ? "-" : msg,
            line: line == undefined ? "-" : line,
            column: column == undefined ? "-" : column,
            file: file == undefined ? "-" : file,
            stackTop: stack,
            page: Location.getPage(),
            pageParams: Location.getParameters(),
            userAgent: navigator.userAgent
        };

        this.lastMessage = msg;


        $.ajax({
            url: URL2.get("/logError.do"),
            method: "POST",
            data: JSON.stringify({
                "err": JSON.stringify(payload)
            }),
            contentType: "application/json",
            statusCode: {
                301: function () {
                    _this.sendingEnabled = false;
                },

                403: function () {
                    _this.sendingEnabled = false;
                }
            }
        });
    }

    window.JSErrorManager = new JSErrorManager();
})(window);

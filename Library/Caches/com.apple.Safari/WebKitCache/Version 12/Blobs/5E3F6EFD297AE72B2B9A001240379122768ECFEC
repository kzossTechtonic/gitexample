(function (window, undefined) {
    /*
     opts = assotiative array label->descriptor
     descriptor:{
     mode: HIGHLIGHT_FIELD, NOTHING,

     //in case of highlight
     field: fieldSelector,
     content: html content to display

     //default: MANUAL
     trigger: MANUAL, FIELD_FOCUS
     }
     */

    GotIt = function () {
        this.opts = [];

        this.box = $('<div class="gotit-box"/>')
            .addClass("hidden")
            .appendTo("body");

        this.contentBox = $('<div class="gotit-box-text">').appendTo(this.box);
        this.controlsBox = $('<div class="gotit-box-controls">').appendTo(this.box);

        this.lockingScreen1 = $('<div class="gotit-lockingScreen"/>').addClass("hidden")
            .css("top", "0")
            .appendTo("body");

        this.lockingScreen2 = $('<div class="gotit-lockingScreen"/>').addClass("hidden")
            .css({
                "top": 0,
                "bottom": 0,
                "right": 0
            })
            .appendTo("body");

        this.lockingScreen3 = $('<div class="gotit-lockingScreen"/>').addClass("hidden")
            .css("bottom", "0")
            .appendTo("body");

        this.lockingScreen4 = $('<div class="gotit-lockingScreen"/>').addClass("hidden")
            .css({
                "top": 0,
                "left": 0,
                "bottom": 0
            })
            .appendTo("body");
    }

    GotIt.prototype.destroy = function (opts) {
        this.lockingScreen1.remove();
        this.lockingScreen2.remove();
        this.lockingScreen3.remove();
        this.lockingScreen4.remove();
        this.contentBox.remove();
        this.controlsBox.remove();
        this.box.remove();
    }

    GotIt.prototype.init = function (opts) {
        if (opts == null || opts.length == 0) {
            return $.Deferred().resolve().promise();
        }

        var initing = $.Deferred();
        var _this = this;
        var doneCounter = Object.keys(opts).length;
        this.opts = opts;

        for (var label in opts) {
            if (opts.hasOwnProperty(label)) {
                checkGotIt(label).done(function (answer) {
                    if (answer == false) {
                        setupGotItTrigerring(_this, label);
                    }

                    doneCounter--;
                    if (doneCounter == 0) {
                        initing.resolve();
                    }
                });
            }
        }

        return initing.promise();
    }

    GotIt.prototype.show = function (label) {
        var _this = this;

        if (getAnswerFromCache(label) == false) {
            var descriptor = this.opts[label];

            if (descriptor.mode == "HIGHLIGHT_FIELD") {
                _this.lockingScreen1.css("display", "block").addClass("hidden");
                _this.lockingScreen2.css("display", "block").addClass("hidden");
                _this.lockingScreen3.css("display", "block").addClass("hidden");
                _this.lockingScreen4.css("display", "block").addClass("hidden");

                setTimeout(function () {
                    var lockingHoleRect = $(descriptor.field).get(0).getBoundingClientRect();
                    console.warn(lockingHoleRect);
                    _this.resizeLockingScreen(lockingHoleRect);

                    _this.lockingScreen1.removeClass("hidden");
                    _this.lockingScreen2.removeClass("hidden");
                    _this.lockingScreen3.removeClass("hidden");
                    _this.lockingScreen4.removeClass("hidden");

                    _this.box.css({
                        top: (lockingHoleRect.top + lockingHoleRect.height) + "px",
                        left: lockingHoleRect.left + "px"
                    });

                    _this.contentBox.html(descriptor.content);
                    var gotItButton = $('<button />').text(Messages.get("common.gotit")).click(function () {
                        markGotIt(label);

                        _this.lockingScreen1.addClass("hidden").hide();
                        _this.lockingScreen2.addClass("hidden").hide();
                        _this.lockingScreen3.addClass("hidden").hide();
                        _this.lockingScreen4.addClass("hidden").hide();
                        _this.box.addClass("hidden").hide();

                        $(descriptor.field).focus();
                    });
                    _this.controlsBox.empty().append(gotItButton);


                    _this.box.removeClass("hidden");


                }, 300);

            } else if (descriptor.mode == null || descriptor.mode == "NOTHING") {
                markGotIt(label);
            }
        }
    }

    GotIt.prototype.resizeLockingScreen = function (rect) {
        var right_padding = 0;
        var left_padding = 0;
        var bottom_padding = 0;
        var top_padding = 0;

        this.lockingScreen1
            .css({
                height: rect.top - top_padding + "px",
                left: rect.left - left_padding + "px",
                width: rect.width + left_padding + right_padding + "px"
            });

        this.lockingScreen2
            .css({
                left: (rect.left + rect.width) + right_padding + "px"
            });

        this.lockingScreen3
            .css({
                top: (rect.top + rect.height) + bottom_padding + "px",
                left: rect.left - left_padding + "px",
                width: rect.width + left_padding + right_padding + "px"
            });

        this.lockingScreen4
            .css({
                width: rect.left - left_padding + "px"
            });
    }


    //------------------------------------------------------------------------------------------------
    // private
    //------------------------------------------------------------------------------------------------

    var cacheMode, cacheArray = {};

    if (typeof(Storage) !== "undefined") {
        cacheMode = "STORAGE";
    } else {
        cacheMode = "ARRAY";
    }

    function checkGotIt(label) {
        var checking = $.Deferred();
        var storedAnswer = getAnswerFromCache(label);

        if (storedAnswer != null) {
            checking.resolve(storedAnswer);
        } else {
            Ajax.post({
                url: URL2.get("/webapi/gotit/" + label)
            })
                .done(function (response) {
                    putAnswerToCache(label, response);
                    checking.resolve(response);
                })
                .fail(function () {
                    checking.resolve(true);//when we cannot get info from server its better not to show infos for users
                });
        }

        return checking.promise();
    }

    function markGotIt(label) {
        console.info("Got it!", label);

        putAnswerToCache(label, true);

        Ajax.post({
            url: URL2.get("/webapi/gotit/" + label)
        });
    }

    function setupGotItTrigerring(_this, label) {
        var descriptor = _this.opts[label];

        if (descriptor != null) {
            if (descriptor.trigger == "FIELD_FOCUS") {
                $(descriptor.field).one("focus", function () {
                    _this.show(label);
                });
            }
        }
    }

    function putAnswerToCache(label, answer) {
        if (cacheMode == "STORAGE") {
            if (answer)
                localStorage.setItem("calamari.gotit.answer." + label, "true");
            else
                localStorage.setItem("calamari.gotit.answer." + label, "false");
        } else {
            cacheArray[label] = answer;
        }
    }

    function getAnswerFromCache(label) {
        if (cacheMode == "STORAGE") {
            var a = localStorage.getItem("calamari.gotit.answer." + label);

            if (a == "true") {
                return true;
            } else if (a == "false") {
                return false;
            } else {
                return null;
            }
        } else {
            return cacheArray[label];
        }
    }
})(window);
(function ($) {
    $.fn.fieldCleaner = function (options) {
        var defaults = {
            click: function () {
            }
        };
        var settings = $.extend({}, defaults, options);
        var _this = this;


        var control =
            $('<span class="clear-field" style="display: none;"/>')
                .click(function () {
                    _this.val("");
                    control.hide();
                    _this.focus();
                    settings.click();
                });

        if (this.val() != "") {
            control.show();
        }

        this
            .keyup(function () {
                if (_this.val() != "")
                    control.show();
                else
                    control.hide();
            })
            .change(function () {
                if (_this.val() != "")
                    control.show();
                else
                    control.hide();
            });

        this.after(control);

        return this;
    };
}(jQuery));
(function (window, undefined) {
    ContextMenu = function () {
    }

    ContextMenu.prototype.addItem = function (caption, icon, onClick) {
        var item = $("<a />")
            .text(caption)
            .append($('<span/>').attr('class', icon));

        if ($.isFunction(onClick))
            item.click(onClick);

        $("#contextMenu").append(item);
        return item;
    }

    ContextMenu.prototype.removeItems = function () {
        $("#contextMenu").children("a").remove();
    }

    window.ContextMenu = new ContextMenu();
})(window);
(function (window) {
    Pin = function () {
        this.pinnedElements = {};
    };

    Pin.prototype.pin = function (options) {
        var _this = this;

        var settings = $.extend({
            elementId: null,
            pinTo: null
        }, options);


        if (this.pinnedElements[settings.elementId] == undefined) {
            var el = $("#" + settings.elementId);
            var pinTo = $(settings.pinTo);
            var pos = el.offset();

            var oldCssPosition = el.css("position");
            var oldCssLeft = el.css("left");
            var oldCssTop = el.css("top");
            var oldCssWidth = el.css("width");
            var oldParent = el.parent();

            el
                .appendTo(pinTo)
                .css({
                    position: "fixed",
                    top: pos.top + "px",
                    left: pos.left + "px",
                    width: oldCssWidth
                });

            this.pinnedElements[settings.elementId] = {
                element: el,
                oldParent: oldParent,
                oldCssPosition: oldCssPosition,
                oldCssLeft: oldCssLeft,
                oldCssTop: oldCssTop,
                oldCssWidth: oldCssWidth
            };
        }
    };

    Pin.prototype.unpin = function (elementId) {
        if (this.pinnedElements[elementId] != undefined) {
            var el = $("#" + elementId);
            var opts = this.pinnedElements[elementId];

            el
                .appendTo(opts.oldParent)
                .css({
                    position: opts.oldCssPosition,
                    top: opts.oldCssTop,
                    left: opts.oldCssLeft,
                    width: opts.oldCssWidth
                });


            delete this.pinnedElements[elementId];
        }
    }

    Pin.prototype.destroy = function () {
        for (var i = 0; i < this.pinnedElements.length; i++) {
            var pinned = this.pinnedElements[i];

            pinned.el
                .appendTo(pinned.oldParent)
                .css({
                    position: pinned.oldCssPosition,
                    top: pinned.oldCssTop,
                    left: pinned.oldCssLeft,
                    width: pinned.oldCssWidth
                })
        }
    }

    window.Pin = Pin;
})(window);

(function (window, undefined) {

    function needOfRemoteLoad(holidayManager, date) {
        var info = {
            needLoad: false,
            from: null,
            to: null
        };

        if (holidayManager.holidaysLoadedFrom == null || holidayManager.holidaysLoadedFrom == null) {
            info.needLoad = true;
            info.from = moment(date).subtract("months", 6);
            info.to = moment(date).add("months", 6);
        } else if (date.isAfter(holidayManager.holidaysLoadedTo)) {
            info.needLoad = true;
            info.from = holidayManager.holidaysLoadedTo;
            info.to = moment(date).add("months", 6);
        } else if (date.isBefore(holidayManager.holidaysLoadedFrom)) {
            info.needLoad = true;
            info.from = moment(date).subtract("months", 6);
            info.to = holidayManager.holidaysLoadedFrom;
        }

        return info;
    }


    function downloadFromRemote(holidayManager, from, to) {

        var data = {
            from: DateTimeTool.momentToStrutsDate(from),
            to: DateTimeTool.momentToStrutsDate(to)
        };
        holidayManager.ajaxDataDecorator(data);


        Ajax.post({
            url: holidayManager.url,
            async: false,
            data: data,

            success: function (holidaysFromServer, testStatus, xhr) {
                if (!!holidaysFromServer) {
                    for (var i = 0; i < holidaysFromServer.length; i++) {
                        var h = holidaysFromServer[i];

                        var holidayStart = DateTimeTool.fromStrutsDateToMoment(h.holidayStart);
                        var holidayEnd = DateTimeTool.fromStrutsDateToMoment(h.holidayEnd);

                        var current = moment(holidayStart);

                        while (!current.isAfter(holidayEnd)) {
                            var key = DateTimeTool.momentToStrutsDate(current);

                            if (key in holidayManager.holidaysInDaysMap)
                                holidayManager.holidaysInDaysMap[key].push(h);
                            else
                                holidayManager.holidaysInDaysMap[key] = [h];
                            current.add('days', 1);
                        }
                    }

                    if (holidayManager.holidaysLoadedFrom == null || from.isBefore(holidayManager.holidaysLoadedFrom)) {
                        holidayManager.holidaysLoadedFrom = from;
                    }

                    if (holidayManager.holidaysLoadedTo == null || to.isAfter(holidayManager.holidaysLoadedTo)) {
                        holidayManager.holidaysLoadedTo = to;
                    }
                }
            }
        });
    }

    HolidayManager = function () {
        this.holidaysLoadedFrom = null;
        this.holidaysLoadedTo = null;
        this.holidaysInDaysMap = {};
        this.url = Url.getActionUrl("absence/configurations", "listHolidaysBetweenDates");

        this.ajaxDataDecorator = function (data) {

        }
    };

    HolidayManager.prototype.getHolidayForDate = function (date) {
        var needLoadInfo = needOfRemoteLoad(this, date);

        if (needLoadInfo.needLoad) {
            downloadFromRemote(this, needLoadInfo.from, needLoadInfo.to);
        }

        var key = DateTimeTool.momentToStrutsDate(date);
        return this.holidaysInDaysMap[key];
    };


    HolidayManager.prototype.countCalendars = function (async) {
        if(async == null){
            async = true;
        }

        if(async==true){
            return Ajax.post({
                url: URL2.get("/absence/configurations/countHolidayCalendars.do"),
                contentType: "application/json",
                async: true
            });
        }else{
            return Ajax.post({
                url: URL2.get("/absence/configurations/countHolidayCalendars.do"),
                contentType: "application/json",
                async: false
            }).responseText;
        }
    }


    window.HolidayManager = HolidayManager;//todo should be removed, factory only

    window.HolidayManagerFactory = {
        forCurrentUser: function () {
            var hm = new HolidayManager();
            hm.url = URL2.get("/absence/configurations/listMyHolidaysBetweenDates.do");
            return hm;
        },

        byEmployeeId: function (id) {
            var hm = new HolidayManager();
            hm.url = URL2.get("/absence/configurations/listHolidaysBetweenDates.do");

            hm.ajaxDataDecorator = function (data) {
                data.employeeId = id;
            };

            return hm;
        },

        forWholeCompany: function () {
            var hm = new HolidayManager();
            hm.url = URL2.get("/absence/configurations/listHolidaysBetweenDatesInAnyCalendar.do");
            return hm;
        }
    }
})(window);
(function (window, undefined) {
    ChaberManager = function () {

    CommentsPresentation = function () {
        this.container = $('<div class="comments" />');
        this.comments = [];
    };

    CommentsPresentation.prototype.loadData = function (comments) {
        var _this = this;
        this.comments = comments;

        this.container.empty();

        for (var i = 0; i < comments.length; i++) {
            var c = comments[i];
            var commentDisplay = new OneComment(c);
